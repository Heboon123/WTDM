img {
	position:t = "absolute"
	size:t = "3sw - 3sw*@isInVr, sh/3+1"
	pos:t = "-sw,sh-h-1@bh"
	background-image:t = "#ui/gameuiskin#optionlist_background_down"
}
root {
	type:t = "big"
	menutitle {
		id:t = "scene-title"
		text:t = ""
		pos:t = "50%pw-50%w, @bh"
		position:t = "absolute"
		style:t = "valign:center"
	}
	optionlist_container {
		flow:t = "vertical"
		overflow-y:t = "auto"
		scrollbarShortcuts:t = "yes"
		dyncampresult {
			id:t = "campaign-status"
			text:t = ""
			width:t = "70%pw"
			pos:t = "50%pw-50%w, 0"
			position:t = "relative"
		}
		textAreaCentered {
			id:t = "campaign-result"
			text:t = ""
			width:t = "70%pw"
			pos:t = "50%pw-50%w, 5%ph"
			position:t = "relative"
		}
		tdiv {
			pos:t = "50%pw-50%w, 0"
			position:t = "relative"
			activeText {
				id:t = "info-dc-text"
				text:t = ""
				tdalign:t = "right"
			}
			activeText {
				id:t = "info-dc-wp"
				text:t = ""
				tdalign:t = "left"
			}
		}
		textAreaCentered {
			text:t = "#multiplayer/tableLosses"
			width:t = "30%pw"
			pos:t = "50%pw-50%w, 5%ph"
			position:t = "relative"
		}
