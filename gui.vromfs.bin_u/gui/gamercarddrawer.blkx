gamercardDrawer {
	id:t = "gamercard_drawer"
	position:t = "root"
	pos:t = "0.5sw-0.5w, 0"
	width:t = "sw"
	overflow:t = "hidden"
	class:t = "gamercardDrawer"
	height:t = "0"
	_size-timer:t = "0"
	moveOut:t = "no"
	on_deactivate:t = "onDrawerDeactivate"
	input-transparent:t = "yes"
	tdiv {
		position:t = "absolute"
		height:t = "sh"
		width:t = "sw"
		pos:t = "0.5pw-0.5w, ph-sh"
		flow:t = "vertical"
		tdiv {
			id:t = "gamercard_drawer_content"
			width:t = "pw"
			height:t = "sh - 1@gamercardDrawerPosY"
			pos:t = "0.5pw-0.5w, 1@gamercardDrawerPosY"
			position:t = "absolute"
			flow:t = "vertical"
			tdiv {
				id:t = "queue_table_container"
				height:t = "ph"
				position:t = "relative"
				pos:t = "0.5pw-0.5w, 0"
				display:t = "hide"
			}
		}
	}
}