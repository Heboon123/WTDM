root {
	blur {

	}
	blur_foreground {

	}
	frame {
		width:t = "0.4@scrn_tgt"
		pos:t = "50%pw-50%w, 50%ph-50%h"
		position:t = "absolute"
		class:t = "wndNav"
		frame_header {
			activeText {
				id:t = "title"
				text:t = ""
				caption:t = "yes"
			}
			Button_close {

			}
		}
		text {
			text:t = "#urlMissions/name"
		}
		EditBox {
			id:t = "name_editbox"
			width:t = "pw"
			pos:t = "0, 1@blockInterval"
			position:t = "relative"
			max-len:t = "24"
			_on_cancel_edit:t = "goBack"
			on_change_value:t = "onChangeName"
			on_activate:t = "onKbdWrapDown"
		}
		text {
			pos:t = "0, 1@blockInterval"
			position:t = "relative"
			text:t = "#urlMissions/url"
		}
		EditBox {
			id:t = "url_editbox"
			width:t = "pw"
			pos:t = "0, 1@blockInterval"
			position:t = "relative"
			_on_cancel_edit:t = "goBack"
			on_change_value:t = "onChangeUrl"
			_on_activate:t = "onApply"
		}
		navBar {
			navMiddle {
				Button_text {
					id:t = "btn_apply"
					text:t = "#mainmenu/btnApply"
					enable:t = "no"
					btnName:t = "A"
					_on_click:t = "onApply"
					ButtonImg {

					}
				}
			}
		}
		dummy {
			on_click:t = "onKbdWrapDown"
			behaviour:t = "accesskey"
			accessKey:t = "Tab"
		}
	}
}