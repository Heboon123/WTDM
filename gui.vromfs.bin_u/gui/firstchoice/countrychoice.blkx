root {
	blur {

	}
	blur_foreground {

	}
	div {
		id:t = "country_choice_block"
		max-width:t = "1@rw"
		max-height:t = "1@rh"
		pos:t = "50%pw-50%w, 50%ph-50%h"
		position:t = "absolute"
		flow:t = "vertical"
		tdiv {
			id:t = "usernick_place"
		}
		activeText {
			id:t = "choice_header"
			left:t = "1@countryChoiceInterval"
			position:t = "relative"
			caption:t = "yes"
			text:t = ""
			height:t = "1@firstChoiceHeaderHeight"
		}
		text {
			id:t = "choice_info"
			position:t = "relative"
			left:t = "1@countryChoiceInterval"
			text:t = "#mainmenu/firstChoiseBonusNotice"
		}
		div {
			id:t = "first_choices_block"
			width:t = "pw"
		}
		navBar {
			class:t = "relative"
			isTransparent:t = "yes"
			navLeft {
				margin-left:t = "1@countryChoiceInterval"
				Button_text {
					id:t = "back_button"
					text:t = "#mainmenu/btnBack"
					on_click:t = "onBack"
					display:t = "hide"
					enable:t = "no"
					btnName:t = "B"
					ButtonImg {

					}
				}
			}
		}
	}
}