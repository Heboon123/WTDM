DummyButton {
	size:t = "sw, sh"
	position:t = "root"
	btnName:t = "B"
	on_click:t = "goBack"
	blur {

	}
	blur_foreground {
		isRoot:t = "yes"
	}
}
tdiv {
	id:t = "tablePlace"
	top:t = "sh - 1@slotbarOffset - h - 1@slotbar_pad_top - 1@slotbar_pad_bottom + 1@slotbarTop + 1@slotTopLineHeight"
	position:t = "root"
	flow:t = "vertical"
	tdiv {
		size:t = "pw, 1@slotbarHeight - @slotbar_pad_top - @slotbar_pad_bottom + @swapCrewsHeaderHeight + 3@blockInterval"
		background-color:t = "@frameDarkBackgroundColor"
		position:t = "absolute"
		border:t = "yes"
		border-color:t = "@buttonBorderColorDisabled"
	}
	textareaNoTab {
		position:t = "relative"
		text:t = "#slotbar/swapCrew"
		margin:t = "1@blockInterval"
		height:t = "@swapCrewsHeaderHeight"
	}
	slotsScrollDiv {
		id:t = "swap_crews_scroll"
		width:t = "fw"
		max-width:t = "1@slotbarWidth"
		padding:t = "@slot_interval + 1@slotScrollButtonWidth, 0"
		overflow-x:t = "auto"
		swapCrew:t = "yes"
		css-hier-invalidate:t = "yes"
		input-transparent:t = "yes"
		slotbarTable {
			id:t = "airs_table"
			behavior:t = "ActivateSelect"
			css-hier-invalidate:t = "yes"
			position:t = "relative"
			input-transparent:t = "yes"
			height:t = "2@slotbarHeight - 2@slotbar_pad_top - 2@slotbar_pad_bottom + 1@slotbarTop"
			on_select:t = "onSlotSelect"
			on_pushed:t = "::gcb.delayedTooltipListPush"
			on_hold_start:t = "::gcb.delayedTooltipListHoldStart"
			on_hold_stop:t = "::gcb.delayedTooltipListHoldStop"
			navigatorShortcuts:t = "yes"
		}
	}
}