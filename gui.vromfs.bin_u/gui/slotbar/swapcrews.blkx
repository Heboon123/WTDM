root {
	behaviour:t = "button"
	behaviour:t = "accesskey"
	accessKey:t = "Esc"
	on_click:t = "onCloseSwapCrews"
	tdiv {
		id:t = "itemsHolder"
		position:t = "absolute"
		overflow:t = "hidden"
		tdiv {
			id:t = "itemsNest"
			position:t = "root"
			pos:t = "0, 0"
			size:t = "sw, sh"
		}
	}
	tdiv {
		position:t = "absolute"
		pos:t = "0, 0"
		size:t = "sw, sh"
		cursor:t = "drag-n-drop"
	}
}