root {
	blur {

	}
	blur_foreground {

	}
	frame {
		width:t = "50*@scrn_tgt/100.0"
		pos:t = "50%pw-50%w, 40%ph-40%h"
		position:t = "absolute"
		flow:t = "vertical"
		class:t = "wnd"
		frame_header {
			activeText {
				id:t = "edit_box_window_header"
				caption:t = "yes"
				text:t = ""
			}
			Button_close {
				id:t = "btn_back"
			}
		}
		textAreaCentered {
			id:t = "editbox_label"
			width:t = "pw"
			text:t = ""
			position:t = "relative"
			pos:t = "50%pw-50%w, 1@frameHeaderHeight"
			padding:t = "0.025@scrn_tgt, 0"
			display:t = "hide"
		}
		textareaNoTab {
			id:t = "editbox_label_left_aligned"
			width:t = "pw"
			text:t = ""
			position:t = "relative"
			pos:t = "50%pw-50%w, 1@frameHeaderHeight"
			padding:t = "0.025@scrn_tgt, 0"
			display:t = "hide"
		}
		tdiv {
			width:t = "45*@scrn_tgt/100.0"
			pos:t = "50%pw-50%w, 3*@scrn_tgt/100.0"
			position:t = "relative"
			EditBox {
				id:t = "edit_box_window_text"
				width:t = "pw"
				display:t = "hide"
				enable:t = "no"
				on_activate:t = "onOk"
				_on_cancel_edit:t = "goBack"
				on_change_value:t = "onChangeValue"
			}
			EditBox {
				id:t = "edit_box_window_text_multiline"
				size:t = "pw, 4@buttonHeight"
				multiline:t = "yes"
				display:t = "hide"
				enable:t = "no"
				_on_cancel_edit:t = "goBack"
				on_change_value:t = "onChangeValue"
			}
		}
		div {
			pos:t = "50%pw-50%w, 0.03@scrn_tgt"
			position:t = "relative"
			Button_text {
				id:t = "btn_ok"
				text:t = "#msgbox/btn_ok"
				on_click:t = "onOk"
				btnName:t = "X"
				ButtonImg {

				}
				textarea {
					id:t = "btn_ok_text"
					class:t = "buttonText"
					text:t = "#msgbox/btn_ok"
				}
			}
		}
	}
}