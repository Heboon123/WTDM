animWaitMsg {
	behavior:t = "bhvUpdater"
	pos:t = "50%pw-50%w, 0"
	position:t = "relative"
	value:t = '{"viewId": "DL_DATA_WAIT_MSG"}'
	css-hier-invalidate:t = "yes"
	fade:t = "out"
	tdiv {
		flow:t = "vertical"
		pos:t = "0,50%ph-50%h"
		position:t = "relative"
		css-hier-invalidate:t = "yes"
		animText {
			id:t = "msgText"
			pos:t = "50%pw-50%w, 0"
			position:t = "relative"
			font:t = "@fontNormal"
			textShade:t = "yes"
			overlayTextColor:t = "active"
			text:t = "#loading/skin"
			color-factor:t = "0"
			fade:t = "out"
		}
		tdiv {
			pos:t = "50%pw-50%w, 0"
			position:t = "relative"
			dlDataStat {
				behavior:t = "bhvUpdater"
				pos:t = "50%pw-50%w,0"
				position:t = "relative"
				value:t = '{"viewId": "DL_DATA_STATE_TEXT"}'
				css-hier-invalidate:t = "yes"
				animText {
					text:t = ""
					font:t = "@fontTiny"
					color-factor:t = "0"
					fade:t = "out"
				}
			}
		}
	}
	animated_wait_icon {
		pos:t = "0,50%ph-50%h"
		position:t = "relative"
		background-rotation:t = "0"
		color-factor:t = "0"
		fade:t = "out"
		_transp-timer:t = "0"
	}
}