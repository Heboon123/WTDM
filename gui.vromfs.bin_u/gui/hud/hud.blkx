tdiv {
	id:t = "hud_obj"
	position:t = "absolute"
	pos:t = "@bwHud, @bhHud"
	size:t = "@rwHud, @rhHud"
}
systemMsg {
	pos:t = "1@systemMsgIndent, 0"
	position:t = "root"
	flow:t = "vertical"
	dlDataStat {
		behavior:t = "bhvUpdater"
		pos:t = "50%pw-50%w,0"
		position:t = "relative"
		value:t = '{"viewId": "DL_DATA_STATE_TEXT"}'
		css-hier-invalidate:t = "yes"
		animText {
			text:t = ""
			font:t = "@fontTiny"
			color-factor:t = "0"
			fade:t = "out"
		}
	}
}
textareaNoTab {
	id:t = "unmapped_shortcuts_warning"
	position:t = "root"
	width:t = "45%@rwHud"
	pos:t = "50%sw-50%w, 60%sh-50%h"
	display:t = "hide"
	wink:t = "no"
	style:t = "transp-time:750; color:@warningTextColor; font:@fontHudMedium; text-align:center;"
	text:t = ""
	shadeStyle:t = "HudHeader3D"
}
textareaNoTab {
	id:t = "server_message"
	position:t = "root"
	width:t = "45%@rwHud"
	pos:t = "50%sw-50%w, 60%sh-50%h"
	wink:t = "no"
	style:t = "transp-time:750; color:@warningTextColor; font:@fontHudMedium; text-align:center;"
	text:t = ""
	timer {
		id:t = "server_message_timer"
		timer_handler_func:t = "onServerMessageTimerUpdate"
		timer_interval_msec:t = "200"
	}
}
textAreaCentered {
	id:t = "time_to_kick_alert_text"
	text:t = ""
	_blink:t = "no"
	titleFont:t = "yes"
	style:t = "color:@warningTextColor;"
	behaviour:t = "basicFontSizeTextArea"
	pos:t = "50%sw-50%w, 40%sh-50%h"
	position:t = "root"
	font-ht:t = "28"
	font-ht-base:t = "28"
	font-ht-end:t = "36"
	size-time:t = "200"
	size-func:t = "sin"
	_size-timer:t = "1"
}
timer {
	id:t = "hud_update"
	timer_handler_func:t = "onHudUpdate"
	timer_interval_msec:t = "1000"
}
hudFade {
	id:t = "hud_fade"
	position:t = "root"
	size:t = "sw, sh"
	behavior:t = "screenFade"
	input-transparent:t = "yes"
}