hitCameraRender {
	id:t = "hit_camera_render"
	tdiv {
		position:t = "absolute"
		id:t = "indicators_nest"
	}
	hitCameraTitle {
		id:t = "title"
	}
	hitCameraCrew {
		id:t = "crew_nest"
		pos:t = "1@blockInterval, 0.2ph"
		position:t = "absolute"
		img {
			size:t = "1@cIco, 1@cIco"
			pos:t = "0, ph-h"
			position:t = "relative"
			margin-left:t = "1@blockInterval"
			background-image:t = "#ui/gameuiskin#ship_crew.svg"
			background-svg-size:t = "1@cIco, 1@cIco"
		}
		tdiv {
			pos:t = "0, ph-h"
			position:t = "relative"
			hitCameraLostCrew {
				id:t = "lost_crew_count"
				pos:t = "pw-w, -h"
				position:t = "absolute"
				flow:t = "vertical"
			}
			textareaNoTab {
				id:t = "crew_count"
			}
		}
		textareaNoTab {
			id:t = "max_crew_count"
			pos:t = "0, ph-h"
			position:t = "relative"
			overlayTextColor:t = "active"
		}
	}
	enemyDamageStatusPanel {
		flow:t = "vertical"
		tdiv {
			id:t = "damageStatus"
			pos:t = "0.5pw-0.5w, 0"
			position:t = "relative"
		}
	}
	hitCameraInfo {
		id:t = "info"
	}
	tdiv {
		size:t = "pw, ph"
		position:t = "absolute"
		overflow:t = "hidden"
		hitCameraCritAnim {
			id:t = "critAnim"
			height:t = "1@hitCameraHeight"
			needAnim:t = "no"
			_size-timer:t = "0"
			_transp-timer:t = "0"
			color-factor:t = "255"
		}
	}
}