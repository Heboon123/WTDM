hudActionBar {
	id:t = "hud_action_bar"
	position:t = "root"
	pos:t = "sw/2-w/2, @actionBarBottomPos-h"
	height:t = "@hudActionBarItemSize"
	actionBarNest {
		id:t = "actions_nest"
		_transp-timer:t = "1"
		_pos-timer:t = "0"
	}
}
tdiv {
	position:t = "absolute"
	pos:t = "pw/2-w/2, 0"
	width:t = "@shHud"
	flow:t = "vertical"
	multiplayerScore {
		id:t = "hud_multiplayer_score"
		behaviour:t = "bhvUpdateByWatched"
		left:t = "50%pw-50%w"
		top:t = "1@multiplayerScoreTopPosUnderCompass"
		position:t = "relative"
		height:t = "1@multiplayerScoreHeight"
		tdiv {
			id:t = "hud_multiplayer_score_progress_bar"
			size:t = "11ph, ph"
			position:t = "absolute"
			pos:t = "50%pw-50%w, 0"
		}
	}
	hudMessages {
		id:t = "hud_messages_race_messages"
		message_style:t = "race_notification"
		position:t = "absolute"
		pos:t = "pw/2 - w/2, 0.05@shHud"
		width:t = "pw"
		flow:t = "vertical"
		text {
			id:t = "race_status"
			pos:t = "50%pw - 50%w, 0"
			position:t = "relative"
			overlayTextColor:t = "active"
			text:t = ""
			hideEmptyText:t = "yes"
			text-align:t = "center"
			hudFont:t = "small"
		}
		tdiv {
			id:t = "leader"
			pos:t = "50%pw - 50%w, 0"
			position:t = "relative"
			isPlayer:t = "no"
			isLeader:t = "yes"
			tdiv {
				pos:t = "50%pw-75%w, 0"
				position:t = "relative"
				width:t = "0.1@shHud + 0.05@shHud"
				text {
					id:t = "time"
					width:t = "0.1@shHud"
					text:t = ""
					hideEmptyText:t = "yes"
					text-align:t = "right"
					hudFont:t = "small"
				}
				text {
					id:t = "place"
					width:t = "0.05@shHud"
					text:t = ""
					hideEmptyText:t = "yes"
					text-align:t = "center"
					hudFont:t = "small"
				}
			}
			text {
				id:t = "name"
				text:t = ""
				hideEmptyText:t = "yes"
				position:t = "relative"
				hudFont:t = "small"
			}
		}
		tdiv {
			id:t = "beforePlayer"
			pos:t = "50%pw - 50%w, 0"
			position:t = "relative"
			isPlayer:t = "no"
			tdiv {
				pos:t = "50%pw-75%w, 0"
				position:t = "relative"
				width:t = "0.1@shHud + 0.05@shHud"
				text {
					id:t = "time"
					width:t = "0.1@shHud"
					text:t = ""
					hideEmptyText:t = "yes"
					text-align:t = "right"
					hudFont:t = "small"
				}
				text {
					id:t = "place"
					width:t = "0.05@shHud"
					text:t = ""
					hideEmptyText:t = "yes"
					text-align:t = "center"
					hudFont:t = "small"
				}
			}
			text {
				id:t = "name"
				text:t = ""
				hideEmptyText:t = "yes"
				position:t = "relative"
				hudFont:t = "small"
			}
		}
		tdiv {
			id:t = "player"
			pos:t = "50%pw - 50%w, 0"
			position:t = "relative"
			isPlayer:t = "no"
			isLeader:t = "yes"
			tdiv {
				pos:t = "50%pw-75%w, 0"
				position:t = "relative"
				width:t = "0.1@shHud + 0.05@shHud"
				text {
					id:t = "time"
					width:t = "0.1@shHud"
					text:t = ""
					hideEmptyText:t = "yes"
					text-align:t = "right"
					hudFont:t = "small"
				}
				text {
					id:t = "place"
					width:t = "0.05@shHud"
					text:t = ""
					hideEmptyText:t = "yes"
					text-align:t = "center"
					hudFont:t = "small"
				}
			}
			text {
				id:t = "name"
				text:t = ""
				hideEmptyText:t = "yes"
				position:t = "relative"
				hudFont:t = "small"
			}
			hudMessages {
				id:t = "hud_messages_race_bonus_time"
				message_style:t = "player_damage_notification"
				margin-left:t = "0.02@shHud"
				flow:t = "vertical"
			}
		}
		tdiv {
			id:t = "afterPlayer"
			pos:t = "50%pw - 50%w, 0"
			position:t = "relative"
			isPlayer:t = "no"
			tdiv {
				pos:t = "50%pw-75%w, 0"
				position:t = "relative"
				width:t = "0.1@shHud + 0.05@shHud"
				text {
					id:t = "time"
					width:t = "0.1@shHud"
					text:t = ""
					hideEmptyText:t = "yes"
					text-align:t = "right"
					hudFont:t = "small"
				}
				text {
					id:t = "place"
					width:t = "0.05@shHud"
					text:t = ""
					hideEmptyText:t = "yes"
					text-align:t = "center"
					hudFont:t = "small"
				}
			}
			text {
				id:t = "name"
				text:t = ""
				hideEmptyText:t = "yes"
				position:t = "relative"
				hudFont:t = "small"
			}
		}
	}
	tdiv {
		pos:t = "pw/2 - w/2, 1@topCenterMessagesTopPos"
		position:t = "absolute"
		flow:t = "vertical"
		tdiv {
			id:t = "hud_message_killer_card"
			position:t = "relative"
			pos:t = "pw/2 - w/2, -0.04@shHud"
			display:t = "hide"
		}
		tdiv {
			id:t = "hud_messages_top_center_container"
			behaviour:t = "bhvUpdateByWatched"
			position:t = "relative"
			pos:t = "pw/2 - w/2, 0"
			flow:t = "vertical"
			hudMessages {
				id:t = "hud_message_center_main_notification"
				pos:t = "pw/2 - w/2, 0"
				position:t = "relative"
				message_style:t = "center_main_notification"
				display:t = "hide"
			}
			animSizeObj {
				id:t = "hud_streaks"
				position:t = "relative"
				pos:t = "pw/2 - w/2, 0"
				width:t = "45%@rwHud"
				animation:t = "hide"
				height-base:t = "1"
				height-end:t = "100"
				height:t = "1"
				_size-timer:t = "0"
				tdiv {
					id:t = "streak_content"
					width:t = "pw"
					flow:t = "vertical"
					display:t = "hide"
					enable:t = "no"
					behaviour:t = "massTransparency"
					transp-base:t = "255"
					transp-end:t = "0"
					transp-func:t = "cube"
					transp-time:t = "2000"
					_transp-timer:t = "1"
					_blink:t = "no"
					img {
						id:t = "streak_icon"
						size:t = "0.06@shHud, 0.06@shHud"
						pos:t = "pw/2-w/2, 0"
						position:t = "relative"
					}
					textareaNoTab {
						id:t = "streak_header"
						width:t = "pw"
						style:t = "color:@streakTextColor; font:@fontHudMedium; text-align:center;"
						text:t = ""
						shadeStyle:t = "HudHeader3D"
						pos:t = "0, -0.004*@shHud"
						position:t = "relative"
					}
					textareaNoTab {
						id:t = "streak_score"
						width:t = "pw"
						style:t = "color:@white; font:@fontHudSmall; text-align:center;"
						shadeStyle:t = "HudTinyLight"
						text:t = ""
					}
				}
			}
		}
		tdiv {
			id:t = "warning_hints"
			width:t = "@shHud"
			pos:t = "pw/2 - w/2, 0"
			position:t = "relative"
			flow:t = "vertical"
		}
		hudMessages {
			id:t = "hud_messages_death_reason_notification"
			width:t = "@shHud"
			pos:t = "pw/2 - w/2, 0"
			position:t = "relative"
			flow:t = "vertical"
			message_style:t = "death_reason_notification"
		}
		hudMessages {
			id:t = "hud_messages_reward_messages"
			width:t = "@shHud"
			position:t = "relative"
			pos:t = "pw/2 - w/2, 0"
			flow:t = "vertical"
			message_style:t = "reward_notification"
			message {
				id:t = "reward_message"
				css-hier-invalidate:t = "yes"
				position:t = "relative"
				pos:t = "pw/2 - w/2"
				padding:t = "0.2@shHud, 0"
				display:t = "hide"
				enable:t = "no"
				behaviour:t = "massTransparency"
				transp-base:t = "255"
				transp-end:t = "0"
				transp-func:t = "square"
				transp-time:t = "2000"
				blend-time:t = "0"
				rewardMessageText {
					id:t = "reward_message_text"
					position:t = "relative"
					text:t = ""
					view_class:t = ""
				}
				rewardMessageValueContainer {
					id:t = "reward_value_container"
					position:t = "absolute"
					pos:t = "pw/2 - w/2, ph - h/2 + 0.01@shHud"
					css-hier-invalidate:t = "yes"
					_blink:t = "no"
					rewardMessageValue {
						id:t = "reward_total"
						position:t = "absolute"
						pos:t = "pw/2-w/2, ph/2-h/2"
						text:t = ""
					}
				}
			}
		}
	}
	hudMessages {
		id:t = "hud_message_center_mission_result"
		pos:t = "50%pw-50%w, 0.4@shHud"
		position:t = "absolute"
		message_style:t = "mission_result_notification"
	}
	tdiv {
		id:t = "mission_result_box_start"
		height:t = "0.3@sf"
		width:t = "10h"
		position:t = "absolute"
		pos:t = "50%pw-50%w, 0.3@shHud - 50%h"
	}
}
tdiv {
	id:t = "tutorial_elements_nest"
	size:t = "@rwHud, @rhHud"
	pos:t = "pw/2-w/2, ph/2-h/2"
	position:t = "absolute"
	display:t = "hide"
	total-input-transparent:t = "yes"
}
tdiv {
	id:t = "hintBlock"
	pos:t = "0.5pw - 0.5w, ph - h"
	position:t = "absolute"
	tdiv {
		id:t = "hud_hints_nest"
		width:t = "2/3@rwHud"
		flow:t = "vertical"
		css-hier-invalidate:t = "yes"
		behavior:t = "Timer"
		tdiv {
			id:t = "ship_obstacle_rf"
			size:t = "pw, 0.04@shHud"
		}
		tdiv {
			width:t = "pw"
			position:t = "relative"
			pos:t = "pw/2 - w/2, 0"
			flow:t = "vertical"
			tdiv {
				id:t = "common_priority_hints_holder"
				position:t = "relative"
				tdiv {
					id:t = "common_priority_hints"
					width:t = "pw"
					position:t = "relative"
					pos:t = "0, ph-h"
					flow:t = "vertical"
				}
			}
			tdiv {
				width:t = "pw"
				tdiv {
					id:t = "mission_hints"
					width:t = "pw"
					position:t = "relative"
					pos:t = "0, ph-h"
					flow:t = "vertical"
				}
			}
			tdiv {
				id:t = "mission_action_hints_holder"
				position:t = "relative"
				tdiv {
					id:t = "mission_action_hints"
					width:t = "pw"
					position:t = "relative"
					pos:t = "0, ph-h"
					flow:t = "vertical"
				}
			}
		}
		hudMessages {
			id:t = "hud_message_zone_capture_notification"
			width:t = "pw"
			message_style:t = "zone_capture_notification"
			input-transparent:t = "yes"
		}
		tdiv {
			position:t = "relative"
			left:t = "pw/2 - w/2"
			div {
				id:t = "iconed_hints"
			}
			div {
				id:t = "display_timers"
			}
		}
		tdiv {
			id:t = "actionbar_hints_nest"
			width:t = "1@shHud"
			left:t = "0.5pw-0.5w"
			position:t = "relative"
			tdiv {
				width:t = "pw"
				position:t = "relative"
				pos:t = "0, ph-h"
				flow:t = "vertical"
				tdiv {
					id:t = "actionbar_hints"
					width:t = "pw"
					flow:t = "vertical"
				}
			}
		}
		hudMessages {
			id:t = "hud_message_player_damage_notification"
			message_style:t = "player_damage_notification"
			left:t = "0.2@shHud"
			position:t = "relative"
			flow:t = "vertical"
			width:t = "pw - 0.2@shHud"
		}
		hudMessages {
			id:t = "hud_message_tank_notification"
			message_style:t = "player_damage_notification"
			left:t = "0.2@shHud"
			position:t = "relative"
			flow:t = "vertical"
			width:t = "pw - 0.2@shHud"
		}
		hudMessages {
			id:t = "hud_message_ship_notification"
			message_style:t = "player_damage_notification"
			left:t = "0.2@shHud"
			position:t = "relative"
			flow:t = "vertical"
			width:t = "pw - 0.2@shHud"
		}
		textAreaCentered {
			id:t = "low-quality-model-warning"
			width:t = "pw"
			style:t = "color:@warningTextColor; font:@fontHudNormal;"
			text:t = "#low_quality_model_in_hangar"
			display:t = "hide"
		}
		tdiv {
			id:t = "tutorial_hints"
			width:t = "pw"
			position:t = "root"
			pos:t = "50%sw-50%w, 1@bhHud + 0.67@shHud"
			flow:t = "vertical"
		}
		tdiv {
			id:t = "minor_priority_hints"
			width:t = "pw"
			position:t = "root"
			pos:t = "50%sw-50%w, @shHud - h"
			flow:t = "vertical"
		}
	}
}
tdiv {
	id:t = "hud_live_stats_nest"
	width:t = "@shHud"
	pos:t = "pw/2-w/2, ph-h-0.032@shHud"
	position:t = "absolute"
	display:t = "hide"
}
tdiv {
	id:t = "hud_message_timers"
}
timer {
	id:t = "hud_message_timer"
}