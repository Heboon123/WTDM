hitCamDebuff {
	id:t = "ship_crew"
	state:t = "healthy"
	position:t = "relative"
	pos:t = "0, 0"
	hitCamDebuffIcon {
		size:t = "1@cIco, 1@cIco"
		position:t = "relative"
		pos:t = "0.15@cIco, ph/2-h/2"
		background-image:t = "#ui/gameuiskin#ship_crew.svg"
		background-svg-size:t = "1@cIco, 1@cIco"
	}
	hitCamDebuffLabel {
		id:t = "label"
		position:t = "relative"
		pos:t = "0.15@cIco, 0"
		text:t = "100%"
	}
	hitCameraCrew {
		id:t = "crew_relative_nest"
		position:t = "relative"
		pos:t = "0.25@cIco, 0"
		hitCamLostCrewRelative {
			id:t = "crew_relative_lost"
			flow:t = "horizontal"
		}
	}
}
tdiv {
	position:t = "relative"
	right:t = "0"
	hitCamDebuff {
		id:t = "ship_buoyancy"
		state:t = "healthy"
		marging-right:t = "0.15@cIco"
		hitCamDebuffIcon {
			size:t = "1.7@cIco, 0.37@cIco"
			position:t = "relative"
			top:t = "ph/2-h/2"
			background-image:t = "#ui/gameuiskin#buoyancy_icon.svg"
			background-svg-size:t = "1.7@cIco, 0.37@cIco"
		}
		hitCamDebuffLabel {
			id:t = "label"
			text:t = "100%"
		}
	}
	tdiv {
		id:t = "ship_compartments"
		display:t = "hide"
		hitCamDebuffIcon {
			size:t = "1@cIco, 1@cIco"
			marging-right:t = "0.15@cIco"
			background-image:t = "#ui/gameuiskin#ship_hull.svg"
			background-svg-size:t = "1@cIco, 1@cIco"
		}
		tdiv {
			id:t = "ship_compartments_blocks"
			height:t = "1@cIco"
		}
	}
}