root {
	blur {

	}
	blur_foreground {

	}
	frame {
		id:t = "create_room"
		width:t = "0.4@scrn_tgt"
		pos:t = "50%pw-50%w, 50%ph-50%h"
		position:t = "absolute"
		class:t = "wndNav"
		frame_header {
			activeText {
				id:t = "internet_radio_title"
				text:t = "#options/internet_radio_add"
				caption:t = "yes"
			}
			Button_close {

			}
		}
		text {
			pos:t = "0, 0"
			position:t = "relative"
			text:t = "#options/internet_radio_name"
		}
		EditBox {
			withTab:t = "yes"
			_on_activate:t = "onFocusUrl"
			id:t = "newradio_name"
