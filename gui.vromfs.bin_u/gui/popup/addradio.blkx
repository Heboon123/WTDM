			tooltip:t = "#guiHints/internet_radio_name"
			text:t = ""
			_on_cancel_edit:t = "goBack"
			_on_change_value:t = "onChanged"
		}
		text {
			pos:t = "0, 7"
			position:t = "relative"
			text:t = "#options/internet_radio_url"
		}
		EditBox {
			_on_activate:t = "onAddRadio"
			id:t = "newradio_url"
			pos:t = "0, 5"
			position:t = "relative"
			width:t = "pw"
			selected:t = "yes"
			tooltip:t = "#guiHints/internet_radio_url"
			text:t = ""
			_on_cancel_edit:t = "goBack"
			_on_change_value:t = "onChanged"
		}
		navBar {
			navMiddle {
				Button_text {
					id:t = "btn_add_radio"
					text:t = "#options/internet_radio_btn_add"
					tooltip:t = "#options/internet_radio_btn_add"
					enable:t = "no"
					btnName:t = "A"
					_on_click:t = "onAddRadio"
					ButtonImg {

					}
				}
			}
		}
	}
}