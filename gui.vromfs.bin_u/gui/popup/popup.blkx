width:t = "pw"
overflow:t = "hidden"
_size-timer:t = "0"
on_click:t = "onClickPopup"
on_r_click:t = "onRClickPopup"
timer_handler_func:t = "onTimerUpdate"
timer_interval_msec:t = "10000"
skip-navigation:t = "yes"
popupWrap {
	id:t = "popup_wrap"
	padding:t = "1@framePadding"
	class:t = "smallFont"
	background-color:t = "@frameBackgroundColor"
	border:t = "yes"
	border-color:t = "@frameBorderColor"
	border-offset:t = "@dp"
	position:t = "absolute"
	size:t = "p.p.w, p.p.h"
	bottom:t = "0"
	left:t = "0"
	flow:t = "vertical"
	input-transparent:t = "yes"
	Button_close {
		id:t = "close_button"
		smallIcon:t = "yes"
		on_click:t = "onClosePopup"
	}
	activeText {
		id:t = "title"
		smallFont:t = "yes"
		position:t = "relative"
		max-width:t = "pw - 1@smallButtonCloseHeight"
		pare-text:t = "yes"
		text:t = ""
		input-transparent:t = "yes"
	}
	textAreaNoScroll {
		id:t = "msg"
		text:t = ""
		input-transparent:t = "yes"
		pare-text:t = "yes"
	}
	tdiv {
		id:t = "popup_buttons_place"
		max-width:t = "pw"
		pos:t = "pw-w, 3@dp"
		padding:t = "-1@buttonMargin"
		position:t = "relative"
	}
}