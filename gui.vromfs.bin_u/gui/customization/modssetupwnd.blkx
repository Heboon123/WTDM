{
  "tdiv": {
    "flow": "vertical",
    "frame": [
      {
        "text": {
          "smallFont": "yes",
          "text": "#mainmenu/btnModifications"
        },
        "tdiv": {
          "margin-top": "0.5@blockInterval",
          "id": "modifications_groups",
          "behaviour": "posNavigator"
        }
      },
      {
        "margin-top": "1@blockInterval",
        "text": {
          "id": "mod_group_txt",
          "smallFont": "yes",
          "text": ""
        },
        "tdiv": {
          "margin-top": "0.5@blockInterval",
          "id": "modifications_slots"
        }
      }
    ],
    "div": {
      "id": "mod_wait_screen",
      "position": "absolute",
      "size": "pw,ph",
      "background-color": "@fullScreenBgrColor",
