flow:t = "horizontal"
profileContentBigIcon {
	position:t = "relative"
	size:t = "1@itemIconBlockHeight, 1@itemIconBlockHeight"
	img {
		id:t = "image"
	}
}
tdiv {
	position:t = "relative"
	id:t = "text_container"
	flow:t = "vertical"
	width:t = "fw"
	margin-left:t = "2@blockInterval"
	profilePageTitle {
		id:t = "header"
		max-width:t = "pw"
		hideEmptyText:t = "yes"
		overlayTextColor:t = "unlockHeader"
		padding-bottom:t = "10@sf/@pf"
	}
	tdiv {
		id:t = "conditions"
		flow:t = "vertical"
		challengeDescriptionProgress {
			id:t = "progress"
			isProfileUnlockProgress:t = "yes"
			margin-bottom:t = "8@sf/@pf"
			value:t = ""
			display:t = "hide"
		}
		profilePageText {
			id:t = "mainCond"
			max-width:t = "6@decalIconHeight"
			overflow:t = "hidden"
			smallFont:t = "yes"
			margin-top:t = "@blockInterval"
			hideEmptyText:t = "yes"
			margin-bottom:t = "8@sf/@pf"
			color:t = "@profilePageTextColor"
		}
		profilePageText {
			id:t = "multDesc"
			max-width:t = "6@decalIconHeight"
			overflow:t = "hidden"
			smallFont:t = "yes"
			color:t = "@profilePageTextColor"
		}
		profilePageText {
			id:t = "conds"
			max-width:t = "6@decalIconHeight"
			overflow:t = "hidden"
			smallFont:t = "yes"
			hideEmptyText:t = "yes"
			color:t = "@profilePageTextColor"
		}
		profilePageText {
			id:t = "obtain_info"
			hideEmptyText:t = "yes"
			max-width:t = "6@decalIconHeight"
			smallFont:t = "yes"
			color:t = "@profilePageTextColor"
		}
	}
	profilePageText {
		id:t = "description"
		margin-top:t = "@blockInterval"
		hideEmptyText:t = "yes"
		max-width:t = "6@decalIconHeight"
		smallFont:t = "yes"
		color:t = "@profilePageTextColor"
	}
	profilePageText {
		id:t = "price"
		display:t = "hide"
		max-width:t = "6@decalIconHeight"
		hideEmptyText:t = "yes"
		margin-top:t = "@blockInterval"
		color:t = "@profilePageTextColor"
	}
	tdiv {
		id:t = "additional_description"
		behavior:t = "posNavigator"
		margin-top:t = "8@dp"
		flow:t = "h-flow"
		width:t = "pw"
		display:t = "hide"
		hideEmptyText:t = "yes"
		canSelectNone:t = "yes"
	}
	tdiv {
		position:t = "relative"
		left:t = "-1@buttonMargin"
		margin-top:t = "8@sf/@pf"
		Button_text {
			id:t = "btn_preview"
			display:t = "hide"
			text:t = "#mainmenu/btnPreview"
			btnName:t = "L3"
			_on_click:t = "onDecoratorPreview"
			showButtonImageOnConsole:t = "no"
			class:t = "image"
			visualStyle:t = "secondary"
			img {
				background-image:t = "#ui/gameuiskin#btn_preview.svg"
			}
			buttonWink {

			}
			ButtonImg {

			}
		}
		Button_text {
			id:t = "btn_marketplace_find_coupon"
			text:t = "#msgbox/btn_find_on_marketplace"
			btnName:t = "X"
			on_click:t = "onBtnMarketplaceFindCoupon"
			display:t = "hide"
			showButtonImageOnConsole:t = "no"
			visualStyle:t = "secondary"
			class:t = "image"
			buttonWink {

			}
			img {
				background-image:t = "#ui/gameuiskin#gc.svg"
			}
			ButtonImg {

			}
		}
		Button_text {
			id:t = "btn_store"
			text:t = "#items/shop/emptyTab/toShopButton"
			btnName:t = "X"
			on_click:t = "onItemsShop"
			display:t = "hide"
			showButtonImageOnConsole:t = "no"
			visualStyle:t = "secondary"
			class:t = "image"
			buttonWink {

			}
			img {
				background-image:t = "#ui/gameuiskin#store_icon.svg"
			}
			ButtonImg {

			}
		}
		Button_text {
			id:t = "btn_marketplace_consume_coupon"
			text:t = "#item/consume/coupon"
			btnName:t = "X"
			on_click:t = "onBtnMarketplaceConsumeCoupon"
			display:t = "hide"
			showButtonImageOnConsole:t = "no"
			visualStyle:t = "secondary"
			class:t = "image"
			buttonWink {

			}
			img {
				background-image:t = "#ui/gameuiskin#gc.svg"
			}
			ButtonImg {

			}
		}
		Button_text {
			id:t = "btn_buy_decorator"
			text:t = "#mainmenu/btnOrder"
			btnName:t = "X"
			_on_click:t = "onBuyDecorator"
			hideText:t = "yes"
			display:t = "hide"
			showButtonImageOnConsole:t = "no"
			visualStyle:t = "purchase"
			buttonWink {

			}
			buttonGlance {

			}
			ButtonImg {

			}
			textarea {
				id:t = "btn_buy_decorator_text"
				class:t = "buttonText"
			}
		}
	}
}