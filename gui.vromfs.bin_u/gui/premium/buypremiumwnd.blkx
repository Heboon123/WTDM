root {
	type:t = "big"
	blur {

	}
	blur_foreground {
		filledDark:t = "yes"
	}
	tdiv {
		pos:t = "0, 0.5ph-0.5h"
		position:t = "absolute"
		size:t = "1@swOrRwInVr, 1@shOrRhInVr"
		background-color:t = "#CC111821"
		flow:t = "vertical"
		img {
			width:t = "pw"
			height:t = "0.211w"
			max-height:t = "0.40ph"
			background-image:t = "!ui/images/premium/premium_account_image"
			background-repeat:t = "aspect-ratio"
			tdiv {
				id:t = "premiumInfoHeader"
				pos:t = "0.5pw - 0.5w, 0.55ph - 0.5h"
				position:t = "absolute"
				img {
					id:t = "headerImage"
					height:t = "220@sf/@pf"
				}
				textareaNoTab {
					id:t = "premiumText"
					margin-left:t = "444@sf/@pf - w"
					margin-top:t = "105@sf/@pf - h"
					position:t = "absolute"
					text:t = "#charServer/entitlement/PremiumAccount"
					bigBoldFont:t = "yes"
					overlayTextColor:t = "active"
				}
				textareaNoTab {
					id:t = "premiumExpiredText"
					margin-left:t = "468@sf/@pf - w"
					margin-top:t = "105@sf/@pf"
					position:t = "absolute"
					text:t = ""
					overlayTextColor:t = "active"
				}
				textareaNoTab {
					id:t = "lastPremiumDaysText"
					margin-left:t = "pw - 147@sf/@pf - w/2"
					margin-top:t = "0.44ph - h/2"
					position:t = "absolute"
					bigBoldFont:t = "yes"
					text:t = ""
					overlayTextColor:t = "gray"
					css-hier-invalidate:t = "yes"
				}
			}
		}
		tdiv {
			max-width:t = "8@premiumItemWidth"
			size:t = "0.833pw, fh-@bh"
			pos:t = "pw/2 - w/2, 0"
			margin-top:t = "7@blockInterval"
			position:t = "relative"
			flow:t = "horizontal"
			css-hier-invalidate:t = "yes"
			textarea {
				id:t = "premiumInfoText"
				width:t = "fw"
				max-height:t = "ph"
				wrapRight:t = "yes"
				font-bold:t = "@fontNormal"
				overflow-y:t = "auto"
			}
			tdiv {
				id:t = "separator"
				position:t = "relative"
				width:t = "1@dp"
				background-color:t = "@separatorColor"
				margin:t = "5@blockInterval, 0, 5@blockInterval, 0"
			}
			tdiv {
				id:t = "premiumList"
				behavior:t = "posNavigator"
				width:t = "3@premiumItemWidth + 10@blockInterval"
				flow:t = "h-flow"
				navigatorShortcuts:t = "yes"
			}
		}
		tdiv {
			width:t = "1@rw"
			pos:t = "0.5pw-0.5w, 1@bh"
			position:t = "absolute"
			Breadcrumb {
				normalFont:t = "yes"
				Button_text {
					on_click:t = "goBack"
					visualStyle:t = "noBgr"
					img {

					}
					btnText {
						text:t = "#mainmenu/hangar"
					}
				}
			}
			tdiv {
				position:t = "absolute"
				pos:t = "pw-w-1@buttonCloseHeight, 0"
				Button_text {
					id:t = "gc_warpoints"
					visualStyle:t = "noFrame"
					tooltip:t = "#mainmenu/warpoints"
					showBonusPersonal:t = ""
					showBonusCommon:t = ""
					_on_click:t = "onOnlineShopLions"
					img {
						isFirstLeft:t = "yes"
						size:t = "@cIco, @cIco"
						background-image:t = "#ui/gameuiskin#shop_warpoints.svg"
						background-svg-size:t = "@cIco, @cIco"
					}
					btnText {
						id:t = "gc_balance"
						min-width:t = "0.05@sf"
						pos:t = "1@blockInterval, 50%ph-50%h"
						position:t = "relative"
						text-align:t = "left"
					}
					BonusCorner {
						bonusType:t = "personal"
					}
					BonusCorner {
						bonusType:t = "common"
					}
					chapterSeparator {
						position:t = "absolute"
						pos:t = "pw, 0"
					}
				}
				Button_text {
					id:t = "gc_eagles"
					visualStyle:t = "noFrame"
					tooltip:t = "#mainmenu/gold"
					_on_click:t = "onOnlineShopEagles"
					img {
						isFirstLeft:t = "yes"
						size:t = "@cIco, @cIco"
						background-image:t = "#ui/gameuiskin#shop_warpoints_premium.svg"
						background-svg-size:t = "@cIco, @cIco"
					}
					btnText {
						id:t = "gc_gold"
						min-width:t = "0.05@sf"
						pos:t = "1@blockInterval, 50%ph-50%h"
						position:t = "relative"
						text-align:t = "left"
					}
					chapterSeparator {
						position:t = "absolute"
						pos:t = "pw, 0"
					}
				}
			}
			Button_close {
				id:t = "btn_close"
				on_click:t = "goBack"
			}
		}
	}
	div {
		id:t = "premiumTimer"
		behavior:t = "Timer"
		timer_handler_func:t = "onTimer"
		timer_interval_msec:t = "1000"
	}
	gamercard_div {

	}
}