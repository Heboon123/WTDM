root {
	blur {

	}
	blur_foreground {

	}
	frame {
		pos:t = "0.5sw-0.5w, 0.5sh-0.5h"
		position:t = "absolute"
		width:t = "0.85sw"
		height:t = "0.8sh"
		frame_header {
			activeText {
				id:t = "eula_title"
				caption:t = "yes"
				text:t = "#eula/eulaTitle"
			}
			tdiv {
				id:t = "usernick_place"
				position:t = "absolute"
				pos:t = "pw-w, 0.5ph-0.5h"
			}
		}
		textarea {
			id:t = "eulaText"
			width:t = "pw"
			height:t = "fh"
			overflow-y:t = "auto"
			padding:t = "0.01@sf, 1@frameHeaderHeight, 0, 1@frameTopPadding"
			text-align:t = "left"
		}
		navBar {
			navMiddle {
				Button_text {
					id:t = "acceptNewEulaVersion"
					text:t = "#msgbox/btn_ok"
					on_click:t = "onAcceptEula"
					btnName:t = "AB"
					ButtonImg {

					}
				}
				Button_text {
					id:t = "accept"
					text:t = "#msgbox/btn_haveReadAndAgree"
					on_click:t = "onAcceptEula"
					btnName:t = "A"
					ButtonImg {

					}
				}
				Button_text {
					id:t = "decline"
					text:t = "#msgbox/btn_cancel"
					_on_click:t = "onExit"
					btnName:t = "B"
					ButtonImg {

					}
				}
			}
		}
	}
}