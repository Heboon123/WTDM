root {
	type:t = "big"
	blur {

	}
	blur_foreground {
		filledDark:t = "yes"
	}
	tdiv {
		pos:t = "0, 0.5ph-0.5h"
		position:t = "absolute"
		size:t = "1@swOrRwInVr, 1@shOrRhInVr"
		background-color:t = "#CC111821"
		flow:t = "vertical"
		tdiv {
			width:t = "pw"
			flow:t = "vertical"
			img {
				id:t = "header_banner"
				size:t = "pw, 0.212w"
				halign:t = "center"
				background-image:t = ""
				background-repeat:t = "aspect-ratio"
			}
			tdiv {
				width:t = "3@ingameCurrencyShopItemWidth + 4@ingameCurrencyShopItemMargin"
				halign:t = "center"
				flow:t = "vertical"
				textarea {
					id:t = "currency_caption"
					width:t = "0.95pw"
					margin-top:t = "40@sf/@pf"
					halign:t = "center"
					text:t = ""
					text-align:t = "center"
					font-bold:t = "@fontMedium"
				}
				tdiv {
					id:t = "items_list"
					width:t = "pw"
					position:t = "relative"
					pos:t = "0.5pw-0.5w, 0"
					margin-top:t = "16@sf/@pf"
					behavior:t = "HoverNavigator"
					flow:t = "h-flow"
				}
			}
		}
		tdiv {
			width:t = "1@rw"
			position:t = "absolute"
			pos:t = "0.5pw-0.5w, 1@bh"
			tdiv {
				position:t = "absolute"
				pos:t = "pw-w-1@buttonCloseHeight, 0"
				Button_text {
					id:t = "gc_warpoints"
					visualStyle:t = "noFrame"
					tooltip:t = "#mainmenu/warpoints"
					showBonusPersonal:t = ""
					showBonusCommon:t = ""
					_on_click:t = "onOnlineShopLions"
					img {
						isFirstLeft:t = "yes"
						size:t = "@cIco, @cIco"
						background-image:t = "#ui/gameuiskin#shop_warpoints.svg"
						background-svg-size:t = "@cIco, @cIco"
					}
					btnText {
						id:t = "gc_balance"
						min-width:t = "0.05@sf"
						pos:t = "1@blockInterval, 50%ph-50%h"
						position:t = "relative"
						text-align:t = "left"
					}
					BonusCorner {
						bonusType:t = "personal"
					}
					BonusCorner {
						bonusType:t = "common"
					}
					chapterSeparator {
						position:t = "absolute"
						pos:t = "pw, 0"
					}
				}
				Button_text {
					id:t = "gc_eagles"
					visualStyle:t = "noFrame"
					tooltip:t = "#mainmenu/gold"
					_on_click:t = "onOnlineShopEagles"
					img {
						isFirstLeft:t = "yes"
						size:t = "@cIco, @cIco"
						background-image:t = "#ui/gameuiskin#shop_warpoints_premium.svg"
						background-svg-size:t = "@cIco, @cIco"
					}
					btnText {
						id:t = "gc_gold"
						min-width:t = "0.05@sf"
						pos:t = "1@blockInterval, 50%ph-50%h"
						position:t = "relative"
						text-align:t = "left"
					}
					chapterSeparator {
						position:t = "absolute"
						pos:t = "pw, 0"
					}
				}
			}
			Button_close {
				id:t = "btn_close"
				on_click:t = "goBack"
			}
		}
	}
	timer {
		id:t = "wnd_update"
		timer_handler_func:t = "onUpdate"
	}
	gamercard_div {

	}
}