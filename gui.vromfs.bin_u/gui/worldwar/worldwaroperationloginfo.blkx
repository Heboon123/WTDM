tdiv {
	id:t = "ww_operation_log_info"
	width:t = "pw"
	height:t = "fh"
	position:t = "relative"
	flow:t = "vertical"
	tdiv {
		id:t = "ww_operation_empty_log"
		width:t = "pw"
		height:t = "0.04@scrn_tgt"
		background-color:t = "@objectiveHeaderBackground"
		display:t = "hide"
		textareaNoTab {
			pos:t = "50%pw-50%w, 50%ph-50%h"
			position:t = "relative"
			text:t = "#worldwar/log/is_empty"
		}
	}
	tdiv {
		id:t = "ww_operation_log_frame"
		width:t = "pw"
		height:t = "fh"
		flow:t = "vertical"
		overflow-y:t = "auto"
		tdiv {
			id:t = "show_prev_logs"
			width:t = "pw"
			height:t = "1@logBlockHeaderHeight"
			background-color:t = "@objectiveHeaderBackground"
			margin-bottom:t = "0.01@scrn_tgt"
			textareaNoTab {
				id:t = "show_prev_logs_text"
				behavior:t = "button"
				width:t = "fw"
				padding-left:t = "1@framePadding"
				position:t = "relative"
				top:t = "(ph-h)/2"
				text:t = "#userlog/showMore"
				on_click:t = "onClickShowPrevLogs"
			}
			frameBlock_dark {
				id:t = "show_prev_logs_btn_nest"
				height:t = "ph"
				hasBorder:t = "no"
				padding:t = "1@dp, 0"
				margin-right:t = "2@blockInterval"
				Button_text {
					id:t = "show_prev_logs_btn"
					position:t = "relative"
					top:t = "(ph-h)/2"
					reduceWidthToHeight:t = "yes"
					showConsoleImage:t = "no"
					useParentHeight:t = "yes"
					noMargin:t = "yes"
					text:t = "#keysPlus"
					_on_click:t = "onClickShowPrevLogs"
					animated_wait_icon {
						id:t = "show_prev_logs_btn_wait_anim"
						pos:t = "50%pw-50%w,50%ph-50%h"
						position:t = "absolute"
						background-rotation:t = "0"
					}
				}
			}
		}
		tdiv {
			id:t = "ww_operation_log"
			width:t = "pw"
			flow:t = "vertical"
			total-input-transparent:t = "yes"
		}
		tdiv {
			id:t = "show_next_logs"
			width:t = "pw"
			height:t = "1@logBlockHeaderHeight"
			background-color:t = "@objectiveHeaderBackground"
			textareaNoTab {
				id:t = "show_next_logs_text"
				behavior:t = "button"
				width:t = "fw"
				padding-left:t = "1@framePadding"
				position:t = "relative"
				top:t = "(ph-h)/2"
				text:t = "#userlog/showMore"
				on_click:t = "onClickShowNextLogs"
			}
			frameBlock_dark {
				height:t = "ph"
				hasBorder:t = "no"
				padding:t = "1@dp, 0"
				margin-right:t = "2@blockInterval"
				Button_text {
					id:t = "show_next_logs_btn"
					reduceWidthToHeight:t = "yes"
					position:t = "relative"
					top:t = "(ph-h)/2"
					text:t = "#keysPlus"
					_on_click:t = "onClickShowNextLogs"
					showConsoleImage:t = "no"
					noMargin:t = "yes"
					useParentHeight:t = "yes"
				}
			}
			frameBlock_dark {
				height:t = "ph"
				hasBorder:t = "no"
				padding:t = "1@dp, 0"
				margin-right:t = "2@blockInterval"
				Button_text {
					id:t = "show_first_logs_btn"
					reduceWidthToHeight:t = "yes"
					position:t = "relative"
					top:t = "(ph-h)/2"
					text:t = "#keysScrollToBottom"
					_on_click:t = "onClickShowFirstLogs"
					showConsoleImage:t = "no"
					useParentHeight:t = "yes"
					noMargin:t = "yes"
				}
			}
		}
	}
	tdiv {
		width:t = "pw"
		flow-align:t = "left"
		background-color:t = "@operationLogFilterBar"
		Button_text {
			id:t = "ww_log_filters"
			text:t = "#worldwar/logFilters"
			_on_click:t = "onShowOperationLogFilters"
		}
		textareaNoTab {
			id:t = "hidden_logs_text"
			padding-left:t = "1@framePadding"
			position:t = "relative"
			top:t = "(ph-h)/2"
			smallFont:t = "yes"
			text:t = "#userlog/showMore"
		}
	}
}