        {
          "id": "log_zone",
          "position": "relative",
          "width": "@wwLogZoneColumnWidth",
          "text-align": "center",
          "isYourZone": "yes",
          "on_hover": "onHoverZoneName",
          "on_unhover": "onHoverLostZoneName"
        }
      ],
      "tdiv": {
        "id": "log_body",
