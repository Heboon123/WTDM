img {
	position:t = "absolute"
	size:t = "3sw - 3sw*@isInVr, sh/3+1"
	pos:t = "-sw,sh-h-1@bh"
	background-image:t = "#ui/gameuiskin#optionlist_background_down"
}
_newline {

}
root {
	flow:t = "vertical"
	titletext {
		id:t = "campaign_name"
		text:t = "#mainmenu/btnEncyclopedia"
	}
	table {
		id:t = "pedia_box"
		width:t = "80%sw"
		height:t = "sh"
		position:t = "absolute"
		top:t = "20*@scrn_tgt/100.0"
		left:t = "10%sw"
		tr {
			td {
				width:t = "10%pw"
				pedia_navigate_bar {
					id:t = "articles_list"
					position:t = "relative"
					flow:t = "vertical"
					left:t = "3%pw"
					size:t = "200, 200"
					style:t = "overflow-x:none;overflow-y:auto;"
					_on_select:t = "onArticleSelect"
				}
			}
			td {
				tdiv {
					id:t = "article_page"
					width:t = "600"
					height:t = "600"
				}
			}
		}
	}
}
