tdiv {
	id:t = "bg_picture_container"
	size:t = "sw, sh"
	position:t = "root"
	background-color:t = "@black"
	tdiv {
		pos:t = "50%sw-50%w,50%sh-50%h"
		position:t = "absolute"
		size:t = "2.333sh, sh"
		overflow:t = "hidden"
		tdiv {
			id:t = "animated_bg_picture"
			pos:t = "pw/2-w/2, ph/2-h/2"
			position:t = "absolute"
			size:t = "2.5sh, sh"
			behaviour:t = "multiLayerImage"
			blend-time:t = "500"
		}
		tdiv {
			size:t = "0.333sh, sh"
			pos:t = "-w/2, 0"
			position:t = "absolute"
			background-image:t = "!ui/gameuiskin#debriefing_bg_grad@@ss"
			background-color:t = "@black"
		}
		tdiv {
			size:t = "0.333sh, sh"
			pos:t = "pw -w/2, 0"
			position:t = "absolute"
			background-image:t = "!ui/gameuiskin#debriefing_bg_grad@@ss"
			background-color:t = "@black"
		}
	}
}