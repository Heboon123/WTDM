root {
	type:t = "big"
	blur {

	}
	blur_foreground {

	}
	frame {
		width:t = "0.7@sf"
		class:t = "wndNav"
		isCenteredUnderLogo:t = "yes"
		frame_header {
			activeText {
				id:t = "item_name_header"
				text:t = "#shop/multiplePurchaseHeader"
				caption:t = "yes"
				pare-text:t = "yes"
				width:t = "pw-1@cIco-16"
			}
			Button_close {

			}
		}
		tdiv {
			size:t = "pw, 1@modCellHeight"
			tdiv {
				id:t = "icon"
				size:t = "1@modCellWidth, 1@modCellHeight"
			}
			tdiv {
				width:t = "fw"
				pos:t = "0, ph -h -1@modPadSize"
				position:t = "relative"
				Button_text {
					id:t = "buttonDec"
					text:t = "-"
					square:t = "yes"
					on_click:t = "onButtonDec"
					tooltip:t = "#weaponry/descreaseValue"
				}
				invisSlider {
					id:t = "skillSlider"
					size:t = "fw, 2*@scrn_tgt/100.0"
					pos:t = "0, 50%ph-50%h"
					position:t = "relative"
					margin:t = "0.5@sliderThumbWidth + 1@blockInterval, 0"
					value:t = "0"
					min:t = "0"
					max:t = "100"
					on_change_value:t = "onProgressChanged"
					total-input-transparent:t = "yes"
					expProgress {
						id:t = "newSkillProgress"
						width:t = "pw"
						pos:t = "50%pw-50%w, 50%ph-50%h"
						position:t = "absolute"
						type:t = "new"
						value:t = "70"
					}
					expProgress {
						id:t = "oldSkillProgress"
						width:t = "pw"
						pos:t = "50%pw-50%w, 50%ph-50%h"
						position:t = "absolute"
						type:t = "old"
						value:t = "30"
					}
					focus_border {

					}
					sliderButton {
						id:t = "weaponry_slider_button"
						type:t = "various"
						img {

						}
						activeText {
							id:t = "text_buyingValue"
							pos:t = "50%pw-50%w, -h"
							position:t = "absolute"
							talign:t = "left"
							valign:t = "center"
							overlayTextColor:t = "userlog"
							input-transparent:t = "yes"
						}
					}
				}
				Button_text {
					id:t = "buttonInc"
					text:t = "+"
					square:t = "yes"
					on_click:t = "onButtonInc"
					tooltip:t = "#weaponry/increaseValue"
				}
			}
		}
		navBar {
			navLeft {
				Button_text {
					id:t = "buttonMax"
					text:t = "#profile/maximumExp"
					on_click:t = "onButtonMax"
					tooltip:t = "#shop/setMaxValue"
					btnName:t = "Y"
					ButtonImg {

					}
				}
			}
			navRight {
				Button_text {
					id:t = "item_price"
					on_click:t = "onBuy"
					hideText:t = "yes"
					btnName:t = "X"
					visualStyle:t = "purchase"
					buttonWink {

					}
					buttonGlance {

					}
					textarea {
						id:t = "item_price_text"
						class:t = "buttonText"
					}
					discount {
						id:t = "multPurch_discount"
						text:t = "-300%"
						pos:t = "pw-30%w, -50%h"
						position:t = "absolute"
						rotation:t = "-30"
					}
					ButtonImg {

					}
				}
			}
		}
	}
}