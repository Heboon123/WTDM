frame_header {
	HorizontalListBox {
		id:t = "shop_pages_list"
		height:t = "ph"
		class:t = "header"
		withImages:t = "yes"
		activeAccesskeys:t = "RS"
		move-only-hover:t = "yes"
		on_select:t = "onAircraftsPage"
	}
	textareaNoTab {
		id:t = "shop_header"
		caption:t = "yes"
		text:t = ""
	}
	top_right_holder {
		hasRightIndent:t = "yes"
		tdiv {
			id:t = "shop_search_box"
			width:t = "0.33@scrn_tgt"
			pos:t = "0, ph/2-h/2"
			position:t = "relative"
			flow:t = "horizontal"
		}
	}
	Button_close {
		id:t = "btn_back"
	}
}
tdiv {
	id:t = "tree_header_div"
	size:t = "pw, 1@buttonHeight"
}
tdiv {
	size:t = "pw, 2@dp"
	background-color:t = "@frameBackgroundColor"
}
tdiv {
	size:t = "pw, fh"
	id:t = "shop_items_visible_div"
	overflow-y:t = "auto"
	scrollbarShortcuts:t = "yes"
	total-input-transparent:t = "yes"
	tdiv {
		id:t = "shop_items_scroll_div"
		width:t = "fw"
		tdiv {
			id:t = "vertSeparators"
			size:t = "pw,ph"
			position:t = "absolute"
		}
		tdiv {
			id:t = "shop_useful_width"
			pos:t = "0, 0"
			position:t = "absolute"
			size:t = "pw-@scrollBarSize, 0"
		}
		tdiv {
			id:t = "shopTable_air_plates"
			pos:t = "0, 0"
			position:t = "absolute"
			height:t = "ph"
			flow:t = "vertical"
		}
		tdiv {
			id:t = "shop_items_pos_div"
			padding:t = "0, 0, 1@scrollBarSize, 0"
			position:t = "relative"
			width:t = "pw"
			tdiv {
				id:t = "shopTable_air_rows"
				pos:t = "1@modBlockTierNumHeight, 0"
				position:t = "absolute"
				width:t = "pw"
				height:t = "ph"
				flow:t = "vertical"
			}
			shopTable {
				id:t = "shop_items_list"
				width:t = "pw"
				behavior:t = "inContainersNavigator"
				isContainerBhv:t = "yes"
				flow:t = "vertical"
				deep:t = "2"
				navigatorShortcuts:t = "SpaceA"
				alwaysShowBorder:t = "yes"
				moveY:t = "linear"
				on_click:t = "onUnitClick"
				on_r_click:t = "onUnitRightClick"
				on_activate:t = "onUnitActivate"
				on_select:t = "onUnitSelect"
				on_dbl_click:t = "onUnitDblClick"
				on_pushed:t = "::gcb.delayedTooltipListPush"
				on_hold_start:t = "::gcb.delayedTooltipListHoldStart"
				on_hold_stop:t = "::gcb.delayedTooltipListHoldStop"
			}
		}
		tdiv {
			id:t = "shop_dark_screen"
			pos:t = "0, 0"
			position:t = "absolute"
			size:t = "pw, ph"
			total-input-transparent:t = "yes"
			display:t = "hide"
		}
	}
}
navBar {

}
timer {
	id:t = "shop_timer"
	timer_handler_func:t = "onUpdate"
}