group:t = ""
DummyButton {
	id:t = "slotb-aircraft-choose-root"
	size:t = "sw, sh"
	position:t = "root"
	btnName:t = "B"
	on_click:t = "onCancelSlotChoose"
	tooltip:t = ""
	blur {

	}
	blur_foreground {
		isRoot:t = "yes"
	}
}
div {
	id:t = "tablePlace"
	position:t = "root"
	behaviour:t = "basicSize"
	size-time:t = "300"
	size-func:t = "squareInv"
	blend-time:t = "0"
	css-hier-invalidate:t = "yes"
	max-height:t = "@rh"
	slotsScrollDiv {
		id:t = "slots_scroll_div"
		height:t = "ph"
		pos:t = "0, -h-1*@scrn_tgt/100.0"
		position:t = "relative"
		class:t = "shopDiv"
		flow:t = "vertical"
		overflow:t = "hidden"
		tooltip:t = ""
		background-image:t = "#ui/gameuiskin#item_table_block_bg_grad"
		background-color:t = "@commonTextColor"
		background-position:t = "4"
		background-repeat:t = "expand"
		padding:t = "4,4"
		tdiv {
			id:t = "arrows_nest"
			position:t = "absolute"
			pos:t = "0.5*pw - 0.5*w, 2*@sf/@pf_outdated"
			size:t = "@shop_width, ph"
		}
		shopTable {
			id:t = "airs_table"
			left:t = "50%pw-50%w"
			position:t = "relative"
			margin:t = "0,1"
			flow:t = "vertical"
			slotDiscountsPos:t = "rightInside"
			navigatorShortcuts:t = "SpaceA"
			behavior:t = "posNavigator"
			on_click:t = "onUnitClick"
			on_r_click:t = "onUnitRightClick"
			on_select:t = "onUnitSelect"
			on_activate:t = "onUnitActivate"
			on_dbl_click:t = "onUnitDblClick"
			on_pushed:t = "::gcb.delayedTooltipListPush"
			on_hold_start:t = "::gcb.delayedTooltipListHoldStart"
			on_hold_stop:t = "::gcb.delayedTooltipListHoldStop"
		}
	}
}
