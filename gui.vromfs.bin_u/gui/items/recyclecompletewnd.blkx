root {
	blur {

	}
	blur_foreground {

	}
	gamercardShadePlace {
		gamercardShade {

		}
	}
	frame {
		id:t = "recycle_frame"
		min-width:t = "1@trophyWndWidth"
		max-width:t = "1@rh"
		class:t = "wndNav"
		type:t = "big"
		isCenteredUnderLogo:t = "yes"
		css-hier-invalidate:t = "yes"
		flow:t = "h-flow"
		frame_header {
			textareaNoTab {
				id:t = "reward_title"
				caption:t = "yes"
				overlayTextColor:t = "active"
				text:t = "#items/recycledComplete"
			}
			Button_close {
				id:t = "btn_back"
			}
		}
		tdiv {
			position:t = "relative"
			flow:t = "vertical"
			flow-align:t = "center"
			left:t = "(pw-w)/2"
			width:t = "pw"
			css-hier-invalidate:t = "yes"
			div {
				id:t = "scroll_container"
				width:t = "pw"
				max-height:t = "0.7@rh"
				overflow-y:t = "auto"
				div {
					id:t = "reward_image_place"
					width:t = "pw"
					css-hier-invalidate:t = "yes"
					position:t = "relative"
					flow:t = "h-flow"
					flow-align:t = "center"
					display:t = "show"
					total-input-transparent:t = "yes"
				}
			}
			tdiv {
				size:t = "pw, 1@sf/@pf"
				background-color:t = "#2d343c"
			}
			textareaNoTab {
				id:t = "extended_desc"
				position:t = "relative"
				left:t = "(pw-w)/2"
				max-width:t = "pw"
				padding:t = "25@sf/@pf, 12@sf/@pf, 25@sf/@pf, 10@sf/@pf"
				hideEmptyText:t = "yes"
				text:t = "#items/recycledNewItems"
				text-align:t = "center"
			}
		}
		navBar {
			navLeft {

			}
			navRight {
				Button_text {
					id:t = "btn_ok"
					text:t = "#mainmenu/btnOk"
					btnName:t = "A"
					_on_click:t = "goBack"
					ButtonImg {

					}
				}
			}
		}
	}
}