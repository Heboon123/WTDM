tdiv {
	id:t = "bg_picture_container"
	size:t = "sw, sh"
	position:t = "root"
	background-color:t = "@black"
	tdiv {
		pos:t = "50%sw-50%w,50%sh-50%h"
		position:t = "absolute"
		size:t = "2.333sh, sh"
		overflow:t = "hidden"
		tdiv {
			id:t = "animated_bg_picture"
			pos:t = "pw/2-w/2, ph/2-h/2"
			position:t = "absolute"
			size:t = "2.5sh, sh"
			behaviour:t = "multiLayerImage"
			blend-time:t = "500"
		}
		tdiv {
			size:t = "0.333sh, sh"
			pos:t = "-w/2, 0"
			position:t = "absolute"
			background-image:t = "!ui/gameuiskin#debriefing_bg_grad@@ss"
			background-color:t = "@black"
		}
		tdiv {
			size:t = "0.333sh, sh"
			pos:t = "pw -w/2, 0"
			position:t = "absolute"
			background-image:t = "!ui/gameuiskin#debriefing_bg_grad@@ss"
			background-color:t = "@black"
		}
	}
}
div {
	width:t = "pw"
	position:t = "absolute"
	pos:t = "0, 50%ph-50%h"
	position:t = "relative"
	re-type:t = "9rect"
	background-color:t = "@white"
	background-position:t = "1,5"
	background-repeat:t = "expand"
	background-image:t = "#ui/gameuiskin#optionlist_background_middle"
	tdiv {
		flow:t = "vertical"
		width:t = "pw"
		height:t = "ph"
		position:t = "relative"
		padding:t = "15"
		textAreaCentered {
			id:t = "msgText"
			width:t = "1.1@sf"
			pos:t = "50%pw-50%w, 0"
			position:t = "relative"
			mediumFont:t = "yes"
			margin:t = "1.5*@scrn_tgt/100.0"
		}
		everyDayRewardProgress {
			id:t = "loading_progress_box"
			position:t = "relative"
			pos:t = "50%pw-50%w"
			width:t = "0.5@sf"
			height:t = "1@loopProgressHeight"
			foreground-color:t = "@red"
			pattern {
				type:t = "dark_diag_lines"
			}
		}
		textAreaCentered {
			id:t = "msgTextSub"
			width:t = "1.1@sf"
			pos:t = "50%pw-50%w, 0"
			position:t = "relative"
			smallFont:t = "yes"
			margin:t = "0.5*@scrn_tgt/100.0"
		}
		div {
			id:t = "buttons_holder"
			pos:t = "50%pw-50%w, 0"
			position:t = "relative"
			margin:t = "0.01@scrn_tgt"
			Button_text {
				id:t = "btn_ok"
				text:t = "#msgbox/btn_ok"
				on_click:t = "goBack"
				btnName:t = "A"
				ButtonImg {

				}
			}
			Button_text {
				id:t = "btn_cancel"
				text:t = "#msgbox/btn_cancel"
				on_click:t = "onCancel"
				btnName:t = "B"
				ButtonImg {

				}
			}
		}
	}
}
BottomMenuPanel {
	shadowSafeArea {

	}
	line {

	}
	Red_shadow_light {
		pos:t = "50%pw-50%w, 0"
		position:t = "absolute"
		size:t = "25%pw, ph"
	}
	GameLogoPlate {
		pos:t = "50%pw - 50%w, 50%ph-50%h"
		position:t = "absolute"
		Shadow {
			redPlate {
				pattern {
					type:t = "dark_diag_lines"
					tdiv {
						height:t = "1@titleLogoPlateHeight + 0.5@bh - 0.5@bhInVr"
						min-width:t = "1@titleLogoPlateWidth"
						tdiv {
							pos:t = "50%pw-50%w, 50%ph-50%h"
							position:t = "relative"
							padding:t = "0.01@scrn_tgt, 0"
							animated_wait_icon {
								id:t = "rot"
								pos:t = "0, 50%ph-50%h"
								position:t = "relative"
								background-rotation:t = "0"
							}
							textarea {
								id:t = "loading"
								pos:t = "0, 50%ph-50%h"
								position:t = "relative"
								text:t = "#loading"
								mediumFont:t = "yes"
								overlayTextColor:t = "active"
								textShade:t = "yes"
							}
						}
					}
				}
			}
		}
	}
	tdiv {
		behavior:t = "bhvUpdater"
		value:t = '{"viewId": "COPYRIGHT_TEXT"}'
		css-hier-invalidate:t = "yes"
		pos:t = "pw-w-1@bw, 50%ph - 50%h"
		position:t = "absolute"
		copyright {
			id:t = "copyright_text"
		}
	}
}
timer {
	id:t = "updater_timer"
	timer_handler_func:t = "onUpdate"
}