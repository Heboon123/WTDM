gamercard {
	css-hier-invalidate:t = "yes"
	flow:t = "horizontal"
	tdiv {
		id:t = "gamercard_drawer_container"
	}
	tdiv {
		size:t = "1@swOrRwInVr, 1@topBarHeight + 0.015@sf - 1@bhInVr"
		pos:t = "1@bwInVr, 1@bhInVr"
		position:t = "root"
		flow:t = "vertical"
		tdiv {
			size:t = "pw, ph - 0.02@sf"
			background-color:t = "#90000000"
		}
		tdiv {
			size:t = "pw, 0.02@sf"
			pos:t = "0, ph-h"
			position:t = "absolute"
			background-repeat:t = "expand"
			background-image:t = "#ui/gameuiskin#optionlist_background_top"
			background-color:t = "#90909090"
			background-position:t = "0, 0.25h, 0, 0"
		}
	}
	GamecardPlate {
		id:t = "gamercard_center"
		display:t = "hide"
		pos:t = "50%sw-50%w, 1@bh"
		position:t = "root"
		min-width:t = "1@gameModeSelectBlockWidth"
		padding-top:t = "1@gameModeSelectPaddingTop"
		background-color:t = "@gameModeColor"
		tdiv {
			id:t = "gamercard_panel_center"
			position:t = "relative"
			pos:t = "50%pw-50%w, 0"
			display:t = "hide"
			enable:t = "no"
			Button_text {
				id:t = "game_mode_change_button"
				_on_click:t = "::gcb.onOpenGameModeSelect"
				noMargin:t = "yes"
				btnName:t = "Y"
				visualStyle:t = "noFrame"
				buttonWink {

				}
				ButtonImg {

				}
				btnText {
					id:t = "game_mode_change_button_text"
					margin:t = "5@dp, 0"
				}
				additionalIconsDiv {
					input-transparent:t = "yes"
					css-hier-invalidate:t = "yes"
					newIconWidget {
						id:t = "new_game_modes_widget_place"
						pos:t = "0.5h -5@dp, 50%ph-50%h"
						position:t = "relative"
					}
				}
			}
		}
	}
	TopMenuPanel {
		shadowSafeArea {

		}
		TopMenuPanelBGPlace {
			id:t = "top_gamercard_bg"
			needRedShadow:t = "yes"
			TopMenuPanelBg {
				width:t = "0.5fw"
				overflow:t = "hidden"
				css-hier-invalidate:t = "yes"
				Red_shadow_light {
					pos:t = "pw-50%w, 0"
					position:t = "absolute"
					size:t = "1@titleLogoPlateWidth, 1@topBarHeight"
					class:t = "inverted"
				}
			}
			tdiv {
				id:t = "gamercard_logo_place"
				pos:t = "0, 1@titleLogoSafeareaOffsetTop"
				position:t = "relative"
				GameLogoPlate {
					id:t = "gamercard_logo"
					padding:t = "1@titleLogoPlatePadding, 1@topGamercardDarkLineHeight, 1@titleLogoPlatePadding, 0"
					top:t = "1@titleLogoPlatePaddingTop*@isInVr"
					position:t = "relative"
					Shadow {
						redPlate {
							pattern {
								type:t = "dark_diag_lines"
								padding-top:t = "1@titleLogoPlatePaddingTop-1@titleLogoPlatePaddingTop*@isInVr"
								img {
									id:t = "titleLogo"
									size:t = "1@titleLogoPlateWidth, 1@titleLogoPlateHeight"
									background-image:t = "ui/title128.ddsx"
								}
							}
						}
					}
				}
			}
			tdiv {
				id:t = "gamercard_tobattle_bg"
				height:t = "ph"
				background-color:t = "@gameModeColor"
				tdiv {
					id:t = "gamercard_tobattle"
					size:t = "1@gcToBattleButtonWidth, 0.06@sf"
					position:t = "relative"
					margin-left:t = "-1@titleLogoPlatePadding/2"
					allowDecreaseFont:t = "yes"
					display:t = "hide"
				}
			}
			TopMenuPanelBg {
				width:t = "0.5fw"
				overflow:t = "hidden"
				css-hier-invalidate:t = "yes"
				Red_shadow_light {
					pos:t = "-50%w, 0"
					position:t = "absolute"
					size:t = "1@titleLogoPlateWidth, 1@topBarHeight"
					class:t = "inverted"
				}
			}
		}
		tdiv {
			flow:t = "vertical"
			pos:t = "1@offsetBetweenScreenAndAirinfo, 1@topBarHeight + 2@dp"
			width:t = "50%pw - 1@offsetBetweenScreenAndAirinfo - 0.5@titleLogoPlateWidth"
			position:t = "absolute"
			total-input-transparent:t = "yes"
			textareaNoTab {
				id:t = "gc_title"
				overlayTextColor:t = "active"
				smallFont:t = "yes"
				text:t = ""
				max-width:t = "pw"
			}
			activeText {
				id:t = "online_info"
				smallFont:t = "yes"
				commonTextColor:t = "yes"
				text:t = ""
				max-width:t = "pw"
			}
			Button_text {
				id:t = "custom_lang_info"
				hasWarningText:t = "yes"
				position:t = "relative"
				visualStyle:t = "noBgr"
				isTextArea:t = "yes"
				display:t = "hide"
				on_click:t = "onCustomLangInfo"
				reduceHeight:t = "yes"
				smallFont:t = "yes"
				focus_border {

				}
			}
			Button_text {
				id:t = "custom_sound_mods"
				hasWarningText:t = "yes"
				position:t = "relative"
				visualStyle:t = "noBgr"
				isTextArea:t = "yes"
				text:t = "#mainmenu/customSoundMods"
				tooltip:t = "#mainmenu/customSoundMods/tooltip"
				display:t = "hide"
				on_click:t = "onCustomSoundMods"
				reduceHeight:t = "yes"
				smallFont:t = "yes"
				focus_border {

				}
			}
			tdiv {
				id:t = "mission_texts_block_left"
				flow:t = "vertical"
				smallFont:t = "yes"
			}
		}
		tdiv {
			id:t = "mission_texts_block_right"
			flow:t = "vertical"
			pos:t = "pw - w - 1@bw, 1@topBarHeight"
			position:t = "absolute"
			total-input-transparent:t = "yes"
		}
		tdiv {
			width:t = "1@rw"
			height:t = "1@gamercardHeight"
			pos:t = "1@bw,1@topGamercardButtonsPos"
			position:t = "absolute"
			tdiv {
				width:t = "@topGcPanelWidth"
				height:t = "ph"
				pos:t = "50%pw - w - 0.5@gcToBattleButtonPlaceWidth, 50%ph-50%h"
				position:t = "absolute"
				gcButtonsHolder {
					padding-right:t = "1@dp"
					class:t = "header"
					gcButtonsInterval:t = "yes"
					Button_text {
						id:t = "gc_nav_back"
						isColoredImg:t = "yes"
						display:t = "hide"
						_on_click:t = "goBack"
						img {
							background-image:t = "#ui/gameuiskin#back.svg"
						}
						btnText {
							pos:t = "@blockInterval, 50%ph-50%h"
							position:t = "relative"
							text-align:t = "left"
							text:t = "#mainmenu/btnBack"
						}
					}
				}
				tdiv {
					id:t = "topmenu_menu_panel"
					height:t = "ph"
					padding:t = "-1@gcButtonsInterval, 0"
				}
				tdiv {
					id:t = "left_gc_panel_free_width"
					width:t = "fw"
				}
				gcButtonsHolder {
					id:t = "gamercard_panel_left"
					padding-right:t = "1@dp"
					class:t = "header"
					gcButtonsInterval:t = "yes"
					Button_text {
						id:t = "gc_clanTag"
						tooltip:t = "#mainmenu/btnClanCard"
						display:t = "hide"
						_on_click:t = "onMyClanOpen"
						tdiv {
							id:t = "gc_clanAlert"
							position:t = "relative"
							pos:t = "0, 50%ph-50%h"
							size:t = "22/720@sf, 22/720@sf"
							background-color:t = "@white"
							background-image:t = "#ui/gameuiskin#new_icon.svg"
							background-svg-size:t = "22/720@sf, 22/720@sf"
						}
						btnText {
							id:t = "gc_clanTag_name"
							min-width:t = "ph"
							pos:t = "0, 50%ph-50%h"
							position:t = "relative"
							isDarkWhenDisabled:t = "no"
						}
						gcBtnLine {

						}
					}
					Button_text {
						id:t = "gc_profile"
						tooltip:t = "#mainmenu/btnProfile"
						_on_click:t = "onProfile"
						cardAvatar {
							id:t = "gc_icon"
						}
						tdiv {
							position:t = "relative"
							pos:t = "@blockInterval, 50%ph-50%h"
							flow:t = "vertical"
							tdiv {
								height:t = "0.025@sf"
								pos:t = "50%pw-50%w, 50%pw-50%w"
								position:t = "relative"
								activeText {
									id:t = "gc_rank"
									position:t = "relative"
									pos:t = "0, 50%ph-50%h"
									text:t = ""
								}
								cardImg {
									id:t = "gc_prestige"
									position:t = "relative"
									pos:t = "1@dp, 50%ph - 50%h"
								}
							}
							expProgress {
								id:t = "gc_rankProgress"
								width:t = "pw"
								pos:t = "0, ph"
								position:t = "absolute"
								small:t = "yes"
							}
						}
						btnText {
							id:t = "gc_name"
							pos:t = "@blockInterval, 50%ph-50%h"
							position:t = "relative"
							text-align:t = "left"
							isDarkWhenDisabled:t = "no"
						}
						div {
							id:t = "gc_manual_unlocks_unseen"
							position:t = "absolute"
							size:t = "pw,ph"
							popup_menu {
								behaviour:t = "bhvUnseen"
								pos:t = "(pw-w)/2, ph+2@dp"
								position:t = "absolute"
								menu_align:t = "bottom"
								noPadding:t = "yes"
								value:t = "manual_unlocks"
								display:t = "hide"
								tdiv {
									margin-right:t = "@dp"
									img {
										size:t = "@cIco, @cIco"
										pos:t = "0, ph/2-h/2"
										position:t = "relative"
										background-image:t = "#ui/gameuiskin#new_icon.svg"
										background-svg-size:t = "@cIco, @cIco"
									}
									textareaNoTab {
										text:t = "#mainmenu/rewardsNotCollected"
									}
								}
								popup_menu_arrow {

								}
							}
						}
					}
				}
			}
			tdiv {
				width:t = "@topGcPanelWidth"
				height:t = "ph"
				pos:t = "50%pw + 0.5@gcToBattleButtonPlaceWidth, 50%ph-50%h"
				position:t = "absolute"
				gcButtonsHolder {
					id:t = "gamercard_panel_right"
					width:t = "@topGcPanelWidth"
					class:t = "header"
					gcButtonsInterval:t = "yes"
					Button_text {
						id:t = "gc_warpoints"
						isColoredImg:t = "yes"
						tooltip:t = "#mainmenu/warpoints"
						showBonusPersonal:t = ""
						showBonusCommon:t = ""
						_on_click:t = "onOnlineShopLions"
						img {
							isFirstLeft:t = "yes"
							background-image:t = "#ui/gameuiskin#shop_warpoints.svg"
						}
						btnText {
							id:t = "gc_balance"
							min-width:t = "0.05@sf"
							pos:t = "@blockInterval, 50%ph-50%h"
							position:t = "relative"
							text-align:t = "left"
						}
						BonusCorner {
							bonusType:t = "personal"
						}
						BonusCorner {
							bonusType:t = "common"
						}
						gcBtnLine {

						}
					}
					Button_text {
						id:t = "gc_eagles"
						isColoredImg:t = "yes"
						tooltip:t = "#mainmenu/gold"
						_on_click:t = "onOnlineShopEagles"
						img {
							isFirstLeft:t = "yes"
							background-image:t = "#ui/gameuiskin#shop_warpoints_premium.svg"
						}
						btnText {
							id:t = "gc_gold"
							min-width:t = "0.05@sf"
							pos:t = "@blockInterval, 50%ph-50%h"
							position:t = "relative"
							text-align:t = "left"
						}
						gcBtnLine {

						}
					}
					Button_text {
						id:t = "gc_free_exp"
						class:t = "image"
						isColoredImg:t = "yes"
						showBonusPersonal:t = ""
						showBonusCommon:t = ""
						_on_click:t = "onConvertExp"
						img {
							background-image:t = "#ui/gameuiskin#convert_xp.svg"
						}
						BonusCorner {
							bonusType:t = "personal"
						}
						BonusCorner {
							bonusType:t = "common"
						}
						discount_notification {
							id:t = "changeExp_discount"
							type:t = "freeExp"
							display:t = "hide"
						}
						gcBtnLine {

						}
					}
					Button_text {
						id:t = "gc_PremiumAccount"
						class:t = "image"
						isColoredImg:t = "yes"
						_on_click:t = "onOnlineShopPremium"
						img {
							id:t = "gc_prempic"
							background-image:t = ""
						}
						gcBtnLine {

						}
					}
					Button_text {
						id:t = "gc_BattlePassProgress"
						class:t = "image"
						isColoredImg:t = "yes"
						tooltip:t = "#battlePass/btn_buy"
						display:t = "hide"
						enable:t = "no"
						on_click:t = "::gcb.openBattlePassShopWnd"
						img {
							id:t = "gc_BattlePassProgressImg"
							behaviour:t = "bhvUpdateByWatched"
							background-image:t = "#ui/gameuiskin#item_type_bp.svg"
							background-saturate:t = "1"
						}
					}
					tdiv {
						id:t = "right_gc_panel_free_width"
						width:t = "fw"
						inactive:t = "yes"
					}
					tdiv {
						id:t = "topmenu_menu_panel_right"
						height:t = "ph"
						padding:t = "-1@gcButtonsInterval, 0"
						inactive:t = "yes"
					}
				}
			}
		}
		infoBlock {
			css-hier-invalidate:t = "yes"
			total-input-transparent:t = "yes"
			title:t = "$tooltipObj"
			activeText {
				id:t = "gc_queue_wait_text"
				pos:t = "0, 50%ph-50%h"
				position:t = "relative"
				behaviour:t = "Timer"
				timer_interval_msec:t = "1000"
			}
			animated_wait_icon {
				id:t = "gc_queue_wait_icon"
				pos:t = "0,50%ph-50%h"
				position:t = "relaive"
				background-rotation:t = "0"
				display:t = "show"
			}
			tooltipObj {
				id:t = "queues_info_tooltip"
				on_tooltip_open:t = "onQueuesTooltipOpen"
				on_tooltip_close:t = "onTooltipObjClose"
				display:t = "hide"
			}
		}
		chatPopupNest {
			id:t = "chatPopupNest"
			position:t = "absolute"
			pos:t = "1@bw + 0.5@rw-w-0.55@titleLogoPlateWidth, @topBarHeight"
			skip-cursor:t = "skip"
		}
	}
}