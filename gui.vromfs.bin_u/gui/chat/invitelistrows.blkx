id:t = ""
inviteUid:t = ""
class:t = "simple"
highlightedRowLine {

}
selImg {
	style:t = "flow:horizontal;"
	tdiv {
		size:t = "fw, fh"
		tdiv {
			width:t = "fw"
			flow:t = "vertical"
			tdiv {
				id:t = "inviterBlock"
				enable:t = "no"
				display:t = "hide"
				ButtonImg {
					size:t = "@cIco, @cIco"
					pos:t = "0, 50%ph-50%h"
					position:t = "relative"
					margin-right:t = "0.01@scrn_tgt"
					showOnSelect:t = "yes"
					btnName:t = "Y"
				}
				textareaNoTab {
					id:t = "inviterName"
					inviteUid:t = ""
					overlayTextColor:t = "userlog"
					text:t = ""
					behaviour:t = "button"
					on_r_click:t = "onInviterInfo"
					on_click:t = "onInviterInfo"
				}
			}
			tdiv {
				width:t = "pw"
				cardImg {
					id:t = "inviteIcon"
					background-image:t = ""
				}
				textareaNoTab {
					id:t = "inviteText"
					width:t = "fw"
					pos:t = "0.01@scrn_tgt, 0"
					position:t = "relative"
					overlayTextColor:t = "active"
					text:t = ""
				}
			}
			textareaNoTab {
				id:t = "restrictions"
				width:t = "pw"
				padding-left:t = "1@cIco + 0.01@scrn_tgt"
				overlayTextColor:t = "warning"
				text:t = ""
				hideEmptyText:t = "yes"
			}
		}
		tdiv {
			id:t = "buttonsPlace"
			pos:t = "0, ph - h"
			position:t = "relative"
			padding:t = "-1@buttonMargin"
			Button_text {
				id:t = "acceptBtn"
				inviteUid:t = ""
				class:t = "double"
				tooltip:t = "#invite/accept"
				btnName:t = "X"
				showOnSelect:t = "yes"
				on_click:t = "onAccept"
				inactiveColor:t = "no"
				ButtonImg {

				}
				img {
					background-image:t = "#ui/gameuiskin#favorite"
				}
			}
			Button_text {
				id:t = "rejectBtn"
				inviteUid:t = ""
				class:t = "double"
				showOnSelect:t = "yes"
				tooltip:t = "#invite/reject"
				btnName:t = "LB"
				on_click:t = "onReject"
				ButtonImg {

				}
				img {
					background-image:t = "#ui/gameuiskin#icon_primary_fail.svg"
				}
			}
		}
	}
}