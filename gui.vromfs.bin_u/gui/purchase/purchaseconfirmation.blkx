position:t = "absolute"
size:t = "sw, sh"
blur {

}
blur_foreground {
	isRoot:t = "yes"
}
div {
	width:t = "pw"
	top:t = "0.5(ph-h)"
	position:t = "absolute"
	flow:t = "vertical"
	height:t = "0.5@scrn_tgt"
	max-height:t = "0.9@scrn_tgt"
	optionlist_container {
		width:t = "pw"
		type:t = "msg_box"
	}
	div {
		width:t = "pw"
		pos:t = "0, ph/2-h/2"
		position:t = "relative"
		flow:t = "vertical"
		overflow-y:t = "auto"
		textarea {
			id:t = "msgText"
			width:t = "1.1@sf"
			left:t = "0.5(pw-w)"
			position:t = "relative"
			text-align:t = "center"
			overflow-y:t = "auto"
			mediumFont:t = "yes"
			font-bold:t = "@fontSmall"
			padding-left:t = "0.5*@scrn_tgt/100.0"
			border-color:t = "@commonTextColor"
		}
		purchaseButtonsHolder {
			id:t = "buttons_holder"
			pos:t = "0.5(pw-w), 0.03@scrn_tgt"
			position:t = "relative"
			behavior:t = "PosNavigator"
			navigatorShortcuts:t = "noSelect"
			total-input-transparent:t = "yes"
			Button_text {
				selected:t = "yes"
				btnName:t = "X"
				on_click:t = "onButtonYes"
				ButtonImg {

				}
				btnText {
					text:t = "#msgbox/btn_yes"
					position:t = "absolute"
					pos:t = "0.5pw-0.5w, 0.5ph-0.5h"
				}
			}
			Button_text {
				btnName:t = "B"
				on_click:t = "onButtonNo"
				ButtonImg {

				}
				btnText {
					text:t = "#msgbox/btn_no"
					position:t = "absolute"
					pos:t = "0.5pw-0.5w, 0.5ph-0.5h"
				}
			}
		}
	}
}
tdiv {
	height:t = "1@gamercardHeight"
	pos:t = "50%pw + 0.5@gcToBattleButtonPlaceWidth, 1@topGamercardButtonsPos"
	position:t = "absolute"
	gcButtonsHolder {
		balanceText {
			id:t = "gc_warpoints"
			tooltip:t = "#mainmenu/warpoints"
			img {
				background-image:t = "#ui/gameuiskin#shop_warpoints.svg"
			}
			text {
				id:t = "gc_balance"
			}
			line {

			}
		}
		balanceText {
			id:t = "gc_eagles"
			tooltip:t = "#mainmenu/gold"
			img {
				background-image:t = "#ui/gameuiskin#shop_warpoints_premium.svg"
			}
			text {
				id:t = "gc_gold"
			}
		}
	}
}
dummy {
	behavior:t = "accesskey"
	btnName:t = "B"
	_on_click:t = "onButtonNo"
}
dummy {
	behavior:t = "accesskey"
	btnName:t = "A"
	_on_click:t = "onDummyA"
}