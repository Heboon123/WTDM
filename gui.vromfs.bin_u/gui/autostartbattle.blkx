hangarControlTracking {

}
frame {
	id:t = "autoStartQueueWnd"
	width:t = "((1@slotbarCountries $max 6) + 1) * 1@queueTableColumnWidth"
	pos:t = "0.5pw-0.5w, 0.3ph"
	position:t = "relative"
	flow:t = "vertical"
	text-halign:t = "center"
	display:t = "hide"
	textAreaCentered {
		id:t = "queue_for_battle"
		pos:t = "50%pw-50%w, 6@sf/@pf"
		position:t = "relative"
		text:t = "#mainmenu/queue_for_battle"
		smallFont:t = "no"
	}
	tdiv {
		id:t = "waiting_time"
		width:t = "pw"
		padding:t = "14*@sf/@pf"
		flow:t = "vertical"
		tdiv {
			pos:t = "pw/2-w/2, 0"
			position:t = "relative"
			size:t = "180@sf/@pf, 180@sf/@pf"
			tdiv {
				id:t = "queue_wait_icon_block"
				position:t = "absolute"
				size:t = "pw, ph"
			}
			centerBlock {
				position:t = "absolute"
				pos:t = "50%pw-50%w, 50%ph-50%h"
				flow:t = "vertical"
				activeText {
					text:t = "#multiplayer/currentWaitTime"
					smallFont:t = "yes"
					style:t = "color:@commonMenuButtonColor;"
				}
				textAreaCentered {
					id:t = "msgText"
					class:t = "active"
					text:t = "0:00"
					titleFont:t = "yes"
					pos:t = "50%pw-50%w,0"
					position:t = "relative"
				}
			}
		}
		tdiv {
			margin-top:t = "14@sf/@pf"
			flow:t = "horizontal"
			width:t = "pw"
			height:t = "0.02@sf"
			tdiv {
				size:t = "0.35pw, 3@sf/@pf"
				bgcolor:t = "#0E1114"
				position:t = "absolute"
				top:t = "0.5ph-0.5h"
			}
			activeText {
				text:t = "#multiplayer/hint"
				commonButtonColor:t = "yes"
				pos:t = "0.5pw-0.5w, 0.5ph-0.5h"
				position:t = "absolute"
				smallFont:t = "yes"
			}
			tdiv {
				size:t = "0.35pw, 3@sf/@pf"
				bgcolor:t = "#0E1114"
				position:t = "absolute"
				top:t = "0.5ph-0.5h"
				right:t = "0"
			}
		}
		loadingTip {
			id:t = "queue_tip"
			width:t = "pw"
			padding:t = "0, 0.5*@scrn_tgt/100.0"
			behaviour:t = "bhvLoadingTip"
			loadingHint {
				id:t = "tip_hint"
				width:t = "pw"
				commonTextColor:t = "yes"
				smallFont:t = "yes"
				behaviour:t = "bhvHint"
				isWrapInRowAllowed:t = "yes"
			}
		}
	}
}
timer {
	id:t = "queue_timeout_time"
	timer_handler_func:t = "onTimer"
	timer_interval_msec:t = "500"
}