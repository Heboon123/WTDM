root {
	tdiv {
		id:t = "promo_mainmenu_place"
		pos:t = "sw - w - 1@bw + 1@scrollBarSize, 1@titleLogoPlateHeight + 1@bh + 0.02@sf"
		position:t = "relative"
		height:t = "fh - 1@wndWithSlotbarBottomOffset"
		width:t = "1@promoButtonWidth + 1@scrollBarSize"
		flow:t = "vertical"
		mainMenuPromoScrollBar {
			id:t = "promo_mainmenu_place_top"
			position:t = "relative"
			pos:t = "pw-w, 0"
			css-hier-invalidate:t = "yes"
			height:t = "fh"
			width:t = "pw"
			flow:t = "vertical"
			overflow-y:t = "auto"
			behavior:t = "PosOptionsNavigator"
			navigatorShortcuts:t = "yes"
		}
		tdiv {
			id:t = "promo_mainmenu_place_bottom"
			position:t = "relative"
			pos:t = "0, 0.01@scrn_tgt"
			flow:t = "vertical"
		}
	}
	timer {
		id:t = "promo_blocks_timer_slow"
		timer_handler_func:t = "onPromoBlocksUpdate"
		timer_interval_msec:t = "5000"
	}
	timer {
		id:t = "promo_blocks_timer_fast"
		timer_handler_func:t = "onPromoBlocksTimer"
		timer_interval_msec:t = "1000"
	}
	tdiv {
		pos:t = "0.5sw-0.5w, 2@topBarHeight + 0.018@sf - @bh"
		position:t = "absolute"
		flow:t = "vertical"
		advertDiv {
			id:t = "game_modes_not_loaded_suppot"
			position:t = "relative"
			pos:t = "50%pw-50%w, 50%ph-50%h"
			width:t = "0.55sw"
			padding:t = "10@sf/@pf, 5@sf/@pf, 10@sf/@pf, 10@sf/@pf"
			display:t = "hide"
			css-hier-invalidate:t = "yes"
			behaviour:t = "bhvUpdateByWatched"
			Button_text {
				id:t = "game_modes_not_loaded_suppot_link"
				position:t = "relative"
				pos:t = "0.5pw-0.5w, 0.5ph-0.5h"
				visualStyle:t = "noFrame"
				externalLink:t = "yes"
				max-width:t = "0.5sw+0.5@slotbarWidthFull-0.01@sf"
				isLink:t = "yes"
				on_click:t = "onMsgLink"
				focus_border {

				}
				btnText {
					text:t = "#mainmenu/gamemodesNotLoaded/support_message"
					underline {

					}
				}
			}
		}
		advertDiv {
			id:t = "topmenu_advert"
			pos:t = "50%pw-50%w, 0"
			position:t = "relative"
			width:t = "0.55sw"
			padding:t = "10, 5, 10, 10"
			display:t = "hide"
			css-hier-invalidate:t = "yes"
			behavior:t = "Timer"
			textAreaCentered {
				id:t = "topmenu_advert_text"
				pos:t = "50%pw-50%w, 50%ph-50%h"
				position:t = "relative"
				width:t = "0.45sw"
				input-transparent:t = "yes"
				text:t = ""
				textHide:t = "yes"
				on_link_click:t = "onAdvertLinkClick"
				bqKey:t = "mainmenu_block_advert"
			}
		}
		advertDiv {
			id:t = "topmenu_psn_update"
			pos:t = "50%pw-50%w, 0"
			position:t = "relative"
			width:t = "0.55sw"
			padding:t = "10, 5, 10, 10"
			display:t = "hide"
			css-hier-invalidate:t = "yes"
			textAreaCentered {
				id:t = "topmenu_psn_update_text"
				pos:t = "50%pw-50%w, 50%ph-50%h"
				position:t = "relative"
				width:t = "0.45sw"
				input-transparent:t = "yes"
				text:t = "#mainmenu/ps5_update_request"
			}
		}
		textAreaCentered {
			id:t = "update_avail"
			max-width:t = "0.5sw+0.5@slotbarWidthFull-0.01@sf"
			pos:t = "0.5pw-0.5w, 0"
			position:t = "relative"
			text:t = "#mainmenu/update_avail_popup_title"
			tooltip:t = "#mainmenu/update_avail_popup_text"
			input-transparent:t = "yes"
			display:t = "hide"
		}
		Button_text {
			id:t = "btn_main_menu_showRequests"
			position:t = "relative"
			pos:t = "0.5pw-0.5w, 0"
			visualStyle:t = "noBgr"
			isTextArea:t = "yes"
			max-width:t = "0.5sw+0.5@slotbarWidthFull-0.01@sf"
			display:t = "hide"
			on_click:t = "on_show_clan_requests"
			focus_border {

			}
		}
		Button_text {
			id:t = "game_mode_notice"
			position:t = "relative"
			pos:t = "0.5pw-0.5w, 0"
			visualStyle:t = "noBgr"
			isTextArea:t = "yes"
			max-width:t = "0.5sw+0.5@slotbarWidthFull-0.01@sf"
			display:t = "hide"
			btnName:t = "R3"
			on_click:t = "onGMNoticeClick"
			focus_border {

			}
			ButtonImg {

			}
		}
	}
	airInfoBlock {
		id:t = "slot_info"
	}
	tdiv {
		pos:t = "50%pw-50%w, @battleBtnBottomOffset-1.5@battleButtonHeight-h"
		position:t = "absolute"
		flow:t = "vertical"
		animWaitMsg {
			behavior:t = "bhvUpdater"
			pos:t = "50%pw-50%w, 0"
			position:t = "relative"
			value:t = '{"viewId": "DL_DATA_WAIT_MSG"}'
			css-hier-invalidate:t = "yes"
			fade:t = "out"
			tdiv {
				flow:t = "vertical"
				pos:t = "0,50%ph-50%h"
				position:t = "relative"
				css-hier-invalidate:t = "yes"
				animText {
					id:t = "msgText"
					pos:t = "50%pw-50%w, 0"
					position:t = "relative"
					font:t = "@fontNormal"
					textShade:t = "yes"
					overlayTextColor:t = "active"
					text:t = "#loading/skin"
					color-factor:t = "0"
					fade:t = "out"
				}
				tdiv {
					pos:t = "50%pw-50%w, 0"
					position:t = "relative"
					dlDataStat {
						behavior:t = "bhvUpdater"
						pos:t = "50%pw-50%w,0"
						position:t = "relative"
						value:t = '{"viewId": "DL_DATA_STATE_TEXT"}'
						css-hier-invalidate:t = "yes"
						animText {
							text:t = ""
							font:t = "@fontTiny"
							color-factor:t = "0"
							fade:t = "out"
						}
					}
				}
			}
			animated_wait_icon {
				pos:t = "0,50%ph-50%h"
				position:t = "relative"
				background-rotation:t = "0"
				color-factor:t = "0"
				fade:t = "out"
				_transp-timer:t = "0"
			}
		}
		tdiv {
			id:t = "low-quality-model-warning"
			pos:t = "pw/2-w/2, -h"
			position:t = "relative"
			flow:t = "vertical"
			display:t = "hide"
			padding-bottom:t = "10*@sf/@pf_outdated"
			textAreaCentered {
				pos:t = "0.5pw-0.5w, 0"
				position:t = "relative"
				max-width:t = "0.33sw"
				style:t = "color:@warningTextColor"
				text:t = "#low_quality_model_in_hangar"
				input-transparent:t = "yes"
			}
			button {
				id:t = "low_quality_model_download_button"
				text:t = "#btn/load_high_quality_models"
				pos:t = "0.5pw-0.5w, 0.01@scrn_tgt"
				position:t = "relative"
				noMargin:t = "yes"
				tfont:t = "small"
				talign:t = "left"
				_on_click:t = "onLoadModels"
				underline {

				}
			}
		}
		tdiv {
			id:t = "unit_has_no_armoring"
			pos:t = "50%pw-50%w,0"
			position:t = "relative"
			display:t = "hide"
			activeText {
				titleFont:t = "yes"
				text:t = "#armor_class/no_armoring/common"
				position:t = "relative"
				pos:t = "50%pw-50%w,0"
				input-transparent:t = "yes"
			}
		}
		tdiv {
			id:t = "crew-notready-topmenu"
			pos:t = "50%pw-50%w,0"
			position:t = "relative"
			behavior:t = "Timer"
			flow:t = "vertical"
			tooltip:t = ""
			display:t = "hide"
			activeText {
				titleFont:t = "yes"
				overlayTextColor:t = "bad"
				text:t = "#mainmenu/crewLockLeft"
				pos:t = "50%pw-50%w,0"
				position:t = "relative"
				input-transparent:t = "yes"
			}
			activeText {
				id:t = "time"
				titleFont:t = "yes"
				text:t = "adfa sdf af"
				overlayTextColor:t = "bad"
				pos:t = "50%pw-50%w,0"
				position:t = "relative"
				input-transparent:t = "yes"
			}
			tdiv {
				id:t = "crew_unlock_buttons"
				position:t = "relative"
				pos:t = "50%pw-50%w, 0"
				Button_text {
					id:t = "btn_unlock_crew"
					on_click:t = "onUnlockCrew"
					hideText:t = "yes"
					css-hier-invalidate:t = "yes"
					visualStyle:t = "purchase"
					buttonWink {

					}
					buttonGlance {

					}
					textarea {
						id:t = "btn_unlock_crew_text"
						class:t = "buttonText"
					}
				}
				Button_text {
					id:t = "btn_unlock_crew_gold"
					on_click:t = "onUnlockCrew"
					hideText:t = "yes"
					css-hier-invalidate:t = "yes"
					visualStyle:t = "purchase"
					buttonWink {

					}
					buttonGlance {

					}
					textarea {
						id:t = "btn_unlock_crew_gold_text"
						class:t = "buttonText"
					}
				}
			}
		}
		tdiv {
			id:t = "rented_unit_info"
			activeText {
				id:t = "rented_unit_info_text"
				display:t = "hide"
				pos:t = "50%pw-50%w,0"
				position:t = "relative"
				behavior:t = "Timer"
				titleFont:t = "yes"
				overlayTextColor:t = "good"
				text:t = ""
				input-transparent:t = "yes"
			}
		}
		tdiv {
			id:t = "suggested_skin"
			flow:t = "vertical"
			pos:t = "50%(pw-w), 0"
			position:t = "relative"
			display:t = "hide"
			textAreaCentered {
				id:t = "info_text"
				pos:t = "50%pw-50%w,0"
				position:t = "relative"
				max-width:t = "@rw - 2@maxPromoButtonWidth"
				text:t = "#suggested_skin/available"
				input-transparent:t = "yes"
				overlayTextColor:t = "active"
				textShade:t = "yes"
			}
			Button_text {
				pos:t = "50%(pw-w), 0"
				position:t = "relative"
				text:t = "#mainmenu/btnPreview"
				class:t = "image"
				showConsoleImage:t = "no"
				_on_click:t = "onSkinPreview"
				img {
					background-image:t = "#ui/gameuiskin#btn_preview.svg"
				}
			}
		}
	}
}
tdiv {
	id:t = "hangar_hint"
	behavior:t = "Timer"
	timer_handler_func:t = "onBackgroundModelHintTimer"
	position:t = "root"
	input-transparent:t = "yes"
	order-popup:t = "yes"
	smallHint {
		id:t = "click_to_view_hint"
		display:t = "hide"
		text {
			pos:t = "0, 50%ph-50%h"
			position:t = "relative"
			margin:t = "@blockInterval, 0"
			text:t = "#click_to_view"
			tinyFont:t = "yes"
		}
		ButtonImg {
			size:t = "1@infoBtnIconSize, 1@infoBtnIconSize"
			pos:t = "0, 50%ph-50%h"
			position:t = "relative"
			btnName:t = "A"
		}
	}
	tooltipObj {
		id:t = "custom_hint"
	}
}