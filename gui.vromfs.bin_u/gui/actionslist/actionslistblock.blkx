id:t = "actions_list"
menu_align:t = ""
not-input-transparent:t = "yes"
iconed:t = "no"
tooltip:t = ""
on_end_edit:t = "onActionsListDeactivate"
flow:t = "vertical"
flow-align:t = "center"
listStyle:t = "center"
min-width:t = "0"
background-color:t = "@actionsListBgColor"
css-hier-invalidate:t = "yes"
hasActions:t = "yes"
move_out:t = "no"
action_list_info {
	id:t = "info_block"
	inactive:t = "yes"
	interactive:t = "no"
	position:t = "relative"
	left:t = "(pw-w)/2"
	width:t = "pw"
	css-hier-invalidate:t = "yes"
}
tdiv {
	id:t = "info_block_separator"
	position:t = "relative"
	pos:t = "(pw-w)/2, 1@sf/@pf"
	display:t = "hide"
	background-color:t = "@menuSeparatorColor"
	inactive:t = "yes"
}
itemsList {
	id:t = "list_nest"
	flow:t = "vertical"
	min-width:t = "pw"
	behavior:t = "posNavigator"
	navigatorShortcuts:t = "cancel"
	on_set_focus:t = "onFocus"
	on_cancel_edit:t = "onBtnClose"
}
timer {
	id:t = "update_timer"
	timer_handler_func:t = "update"
	timer_interval_msec:t = "100"
}