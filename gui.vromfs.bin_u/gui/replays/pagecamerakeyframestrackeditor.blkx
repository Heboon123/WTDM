replaySystemPage {
	replaySystemTab {
		replaySystemHeader {
			arrow {

			}
			activeText {
				text:t = "Camera keyframes track editor"
			}
		}
		content {
			row {
				Button_text {
					id:t = "add_key_button"
					text:t = "Add key"
					noMargin:t = "yes"
				}
				Button_text {
					id:t = "remove_last_key_button"
					text:t = "Remove last Key"
					noMargin:t = "yes"
				}
				Button_text {
					id:t = "clear_all_key_button"
					text:t = "Clear all Keys"
					noMargin:t = "yes"
				}
			}
			row {
				Button_text {
					id:t = "jump_prev_key"
					text:t = "jumpPrevKey"
					noMargin:t = "yes"
				}
				textValue {
					id:t = "track_time"
					width:t = "150@sf/@pf"
					text:t = "0"
				}
				Button_text {
					id:t = "jump_next_key"
					text:t = "jumpNextKey"
					noMargin:t = "yes"
				}
				activeText {
					text:t = "keyNo:"
				}
				activeText {
					id:t = "current_key_index"
					text:t = "0/0"
					margin-left:t = "1@blockInterval"
				}
			}
			row {
				textValue {
					id:t = "current_key_time"
					width:t = "650@sf/@pf"
					text:t = "0"
				}
				activeText {
					text:t = "time"
				}
			}
			row {
				textValue {
					id:t = "current_key_fov"
					width:t = "650@sf/@pf"
					text:t = "0"
				}
				activeText {
					text:t = "fov"
				}
			}
			row {
				textValue {
					id:t = "current_key_roll"
					width:t = "650@sf/@pf"
					text:t = "0"
				}
				activeText {
					text:t = "roll"
				}
			}
			replaySystemTab {
				replaySystemHeader {
					arrow {

					}
					activeText {
						text:t = "Apply keys filter parameters"
					}
				}
				content {
					row {
						CheckBox {
							id:t = "play_keys_position"
							text:t = "Play key`s position"
							CheckBoxImg {

							}
						}
					}
					row {
						CheckBox {
							id:t = "play_keys_rotation"
							text:t = "Play key`s rotation"
							CheckBoxImg {

							}
						}
					}
					row {
						CheckBox {
							id:t = "play_keys_roll"
							text:t = "Play key`s roll"
							CheckBoxImg {

							}
						}
					}
					row {
						CheckBox {
							id:t = "play_keys_fov"
							text:t = "Play key`s fov"
							CheckBoxImg {

							}
						}
					}
				}
			}
		}
	}
}