tdiv {
	width:t = "0.6sw"
	max-height:t = "0.6sh"
	position:t = "absolute"
	pos:t = "pw/2-w/2, 0.2sh"
	overflow-y:t = "auto"
	not-input-transparent:t = "yes"
	css-hier-invalidate:t = "yes"
	tdiv {
		margin:t = "1@blockInterval"
		width:t = "pw"
		min-height:t = "0.4sh"
		flow:t = "vertical"
		blur_foreground {

		}
		replaySystemPage {
			replaySystemTab {
				replaySystemHeader {
					arrow {

					}
					activeText {
						text:t = "Replay"
					}
				}
				content {
					row {
						Button_text {
							id:t = "rewind_button"
							text:t = "Rewind"
							noMargin:t = "yes"
						}
						slider {
							id:t = "rewind_slider"
							textValue {
								id:t = "rewind_slider_value"
							}
						}
						activeText {
							text:t = "time to rewind"
						}
					}
				}
			}
