replaySystemPage {
	replaySystemTab {
		replaySystemHeader {
			arrow {

			}
			activeText {
				text:t = "Replay"
			}
		}
		content {
			row {
				Button_text {
					id:t = "rewind_button"
					text:t = "Rewind"
					noMargin:t = "yes"
				}
				slider {
					id:t = "rewind_slider"
					textValue {
						id:t = "rewind_slider_value"
					}
				}
				activeText {
					text:t = "time to rewind"
				}
			}
		}
	}
	replaySystemTab {
		replaySystemHeader {
			arrow {

			}
			activeText {
				text:t = "Options"
			}
		}
		content {
			row {
				slider {
					id:t = "free_camera_inertia_slider"
					textValue {
						id:t = "free_camera_inertia_slider_value"
					}
				}
				activeText {
					text:t = "Free Camera Inertia"
				}
			}
			row {
				slider {
					id:t = "camera_max_speed_slider"
					textValue {
						id:t = "camera_max_speed_slider_value"
					}
				}
				activeText {
					text:t = "Camera Max speed"
				}
			}
			row {
				slider {
					id:t = "camera_max_orient_speed_slider"
					textValue {
						id:t = "camera_max_orient_speed_slider_value"
					}
				}
				activeText {
					text:t = "Camera Max Orient speed"
				}
			}
			row {
				slider {
					id:t = "camera_fov_slider"
					textValue {
						id:t = "camera_fov_slider_value"
					}
				}
				activeText {
					text:t = "Camera Fov"
				}
			}
			row {
				slider {
					id:t = "free_camera_zoom_speed_slider"
					textValue {
						id:t = "free_camera_zoom_speed_slider_value"
					}
				}
				activeText {
					text:t = "Free Camera Zoom Speed"
				}
			}
			row {
				Button_text {
					id:t = "reset_camera_roll_button"
					text:t = "Reset Camera Roll"
					noMargin:t = "yes"
				}
			}
		}
	}
}