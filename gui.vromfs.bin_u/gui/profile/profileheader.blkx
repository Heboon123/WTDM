tdiv {
	size:t = "pw, ph"
	pos:t = "50%pw-50%w, 0"
	position:t = "relative"
	css-hier-invalidate:t = "yes"
	Button_text {
		id:t = "btn_profile_icon"
		class:t = "profileImg"
		_on_click:t = "onChangePilotIcon"
		avatar {
			id:t = "profile-icon"
			position:t = "absolute"
			size:t = "pw,ph"
			behavior:t = "bhvAvatar"
			isFull:t = "yes"
			css-hier-invalidate:t = "yes"
			avatarFrame {
				id:t = "avatarFrame"
				display:t = "hide"
			}
			profileEditBtnShadow {
				type:t = "big"
				pos:t = "pw - w - 4@sf/@pf, 4@sf/@pf"
			}
			profileEditBtn {
				type:t = "big"
				pos:t = "pw - w - 5@sf/@pf, 5@sf/@pf"
				btnName:t = ""
				_on_click:t = "onChangePilotIcon"
			}
		}
		ButtonImg {

		}
		unseenIcon {
			id:t = "unseen_avatar"
			pos:t = "4@dp, 4@dp"
			position:t = "absolute"
			unseenText {

			}
		}
	}
	tdiv {
		position:t = "relative"
		height:t = "1@avatarButtonSize"
		left:t = "23@sf/@pf"
		css-hier-invalidate:t = "yes"
		tdiv {
			width:t = "pw"
			position:t = "absolute"
			flow:t = "vertical"
			css-hier-invalidate:t = "yes"
			tdiv {
				tdiv {
					re-type:t = "textarea"
					behaviour:t = "textArea"
					id:t = "clanTag"
					max-width:t = "pw"
					font:t = "@fontMedium"
					font-pixht:t = "38@sf/@pf"
					top:t = "ph-h"
					tooltip:t = ""
					padding-right:t = "1@blockInterval"
					hideEmptyText:t = "yes"
					text:t = ""
					color:t = "#FFFFFFFF"
				}
				tdiv {
					position:t = "relative"
					re-type:t = "textarea"
					behaviour:t = "textArea"
					id:t = "profile-name"
					top:t = "ph-h"
					talign:t = "left"
					font:t = "@fontBigBold"
					color:t = "#FFFFFFFF"
				}
			}
			tdiv {
				position:t = "relative"
				css-hier-invalidate:t = "yes"
				padding-top:t = "4@sf/@pf"
				Button_text {
					id:t = "profile_currentUser_btn_title"
					position:t = "relative"
					left:t = "-1@buttonTextPadding"
					hideEmptyText:t = "yes"
					isProfileTitleBtn:t = "yes"
					css-hier-invalidate:t = "yes"
					noMargin:t = "yes"
					visualStyle:t = "noBgr"
					on_click:t = "openChooseTitleWnd"
					textareaNoTab {
						id:t = "profile-currentUser-title"
						pos:t = "0, 0.5ph-0.5h"
						position:t = "relative"
					}
					focus_border {

					}
					ButtonImg {

					}
				}
				tdiv {
					position:t = "relative"
					flow:t = "horizontal"
					top:t = "(ph - h)/2 - 2@sf/@pf"
					css-hier-invalidate:t = "yes"
					profileEditBtn {
						pos:t = "pw, (ph - h)/2"
						_on_click:t = "openChooseTitleWnd"
					}
					unseenIcon {
						id:t = "unseen_titles"
						position:t = "relative"
						unseenText {

						}
					}
				}
			}
			textareaNoTab {
				id:t = "profile-currentUser-psnName"
				width:t = "pw"
				smallFont:t = "yes"
			}
			textareaNoTab {
				id:t = "profile-currentUser-steamName"
				width:t = "pw"
				text:t = ""
				smallFont:t = "yes"
			}
		}
		tdiv {
			id:t = "level_progress_container"
			position:t = "relative"
			flow:t = "horizontal"
			width:t = "pw"
			top:t = "ph-h-11@sf/@pf"
			tdiv {
				position:t = "relative"
				size:t = "42@sf/@pf, 42@sf/@pf"
				pos:t = "0, ph/2-h/2"
				cardImg {
					id:t = "profile-prestige"
					pos:t = "(pw-w)/2, (ph-h)/2"
					type:t = "profile"
					position:t = "absolute"
				}
			}
			tdiv {
				position:t = "relative"
				size:t = "2@sf/@pf, 42@sf/@pf"
				top:t = "(ph-h)/2"
				background-color:t = "#6f767d"
				margin-right:t = "14@sf/@pf"
				margin-left:t = "14@sf/@pf"
			}
			tdiv {
				position:t = "relative"
				flow:t = "vertical"
				width:t = "308@sf/@pf"
				pos:t = "0, (ph - h)/2 - @sf/@pf"
				tdiv {
					position:t = "relative"
					pos:t = "0, 0"
					padding-bottom:t = "8@sf/@pf"
					width:t = "pw"
					tdiv {
						position:t = "relative"
						flow:t = "horizontal"
						pos:t = "pw-w-2@sf/@pf, 0"
						activeText {
							smallFont:t = "yes"
							text:t = "#mainmenu/rank"
							position:t = "relative"
						}
						activeText {
							id:t = "profile-rank"
							smallFont:t = "yes"
							position:t = "relative"
							padding:t = "2@sf/@pf, 0"
						}
					}
					activeText {
						id:t = "profile-prestige_title"
						smallFont:t = "yes"
						position:t = "absolute"
						left:t = "2@sf/@pf"
					}
				}
				expProgress {
					padding-top:t = ""
					id:t = "profile-rankProgress"
					width:t = "pw"
					position:t = "relative"
					value:t = "0"
					isForProfile:t = "yes"
				}
			}
		}
	}
}