root {
	type:t = "big"
	blur {

	}
	blur_foreground {

	}
	tdiv {
		pos:t = "0, 0.5ph-0.5h"
		position:t = "absolute"
		size:t = "1@swOrRwInVr, 1@shOrRhInVr"
		background-color:t = "#CC111821"
		flow:t = "vertical"
		img {
			id:t = "offer_image"
			width:t = "pw"
			height:t = "0.211w"
			max-height:t = "0.45ph"
			background-image:t = "!ui/images/offer/personal_offer_image"
			background-repeat:t = "aspect-ratio"
			tdiv {
				pos:t = "0.5pw - 0.5w, 0.6ph - 0.5h"
				position:t = "absolute"
				flow:t = "horizontal"
				img {
					id:t = "header_image_left"
					width:t = "365@sf/@pf"
					height:t = "242@sf/@pf"
					background-image:t = "ui/images/offer/personal_offer_header"
					background-position:t = "0, 0, 500, 0"
					background-repeat:t = "part"
				}
				img {
					id:t = "header_image_center"
					height:t = "242@sf/@pf"
					background-image:t = "ui/images/offer/personal_offer_header"
					background-position:t = "450, 0, 450, 0"
					background-repeat:t = "part"
				}
				img {
					id:t = "header_image_right"
					width:t = "365@sf/@pf"
					height:t = "242@sf/@pf"
					background-image:t = "ui/images/offer/personal_offer_header"
					background-position:t = "500, 0, 0, 0"
					background-repeat:t = "part"
				}
				textareaNoTab {
					id:t = "limited_text"
					margin-left:t = "160@sf/@pf"
					margin-top:t = "64@sf/@pf"
					position:t = "absolute"
					text:t = "#specialOffer/limited"
					mediumFont:t = "yes"
					overlayTextColor:t = "active"
				}
				textareaNoTab {
					id:t = "personal_text"
					margin-left:t = "160@sf/@pf"
					margin-top:t = "100@sf/@pf"
					position:t = "absolute"
					text:t = "#specialOffer/personal"
					bigBoldFont:t = "yes"
					overlayTextColor:t = "active"
				}
				textareaNoTab {
					id:t = "time_expired_text"
					margin-left:t = "pw - w - 120@sf/@pf"
					margin-top:t = "46@sf/@pf - h"
					position:t = "absolute"
					text:t = "#specialOffer/valid"
					smallFont:t = "yes"
					overlayTextColor:t = "active"
				}
				textareaNoTab {
					id:t = "time_expired_value"
					margin-left:t = "pw - 152@sf/@pf - w/2"
					margin-top:t = "0.44ph - h/2"
					position:t = "absolute"
					mediumFont:t = "yes"
					overlayTextColor:t = "gray"
					css-hier-invalidate:t = "yes"
				}
			}
		}
		tdiv {
			id:t = "offer_markup"
			pos:t = "pw/2 - w/2, 0"
			position:t = "relative"
			flow:t = "horizontal"
		}
		Button_text {
			id:t = "btn_buy"
			size:t = "389@sf/@pf, 54@sf/@pf"
			pos:t = "pw/2 - w/2, 0"
			position:t = "relative"
			margin-top:t = "60@sf/@pf"
			hideText:t = "yes"
			on_click:t = "onBuy"
			btnName:t = "X"
			visualStyle:t = "purchase"
			buttonWink {

			}
			buttonGlance {

			}
			tdiv {
				id:t = "btn_buy_text_block"
				pos:t = "0.5pw-0.5w, 0.5ph-0.5h"
				position:t = "relative"
			}
			ButtonImg {

			}
		}
		tdiv {
			width:t = "1@rw"
			pos:t = "0.5pw-0.5w, 1@bh"
			position:t = "absolute"
			Breadcrumb {
				normalFont:t = "yes"
				Button_text {
					on_click:t = "goBack"
					visualStyle:t = "noBgr"
					img {

					}
					btnText {
						text:t = "#mainmenu/hangar"
					}
				}
			}
			tdiv {
				position:t = "absolute"
				pos:t = "pw-w-1@buttonCloseHeight, 0"
				Button_text {
					id:t = "gc_warpoints"
					visualStyle:t = "noFrame"
					tooltip:t = "#mainmenu/warpoints"
					showBonusPersonal:t = ""
					showBonusCommon:t = ""
					_on_click:t = "onOnlineShopLions"
					img {
						isFirstLeft:t = "yes"
						size:t = "@cIco, @cIco"
						background-image:t = "#ui/gameuiskin#shop_warpoints.svg"
						background-svg-size:t = "@cIco, @cIco"
					}
					btnText {
						id:t = "gc_balance"
						min-width:t = "0.05@sf"
						pos:t = "1@blockInterval, 50%ph-50%h"
						position:t = "relative"
						text-align:t = "left"
					}
					BonusCorner {
						bonusType:t = "personal"
					}
					BonusCorner {
						bonusType:t = "common"
					}
					chapterSeparator {
						position:t = "absolute"
						pos:t = "pw, 0"
					}
				}
				Button_text {
					id:t = "gc_eagles"
					visualStyle:t = "noFrame"
					tooltip:t = "#mainmenu/gold"
					_on_click:t = "onOnlineShopEagles"
					img {
						isFirstLeft:t = "yes"
						size:t = "@cIco, @cIco"
						background-image:t = "#ui/gameuiskin#shop_warpoints_premium.svg"
						background-svg-size:t = "@cIco, @cIco"
					}
					btnText {
						id:t = "gc_gold"
						min-width:t = "0.05@sf"
						pos:t = "1@blockInterval, 50%ph-50%h"
						position:t = "relative"
						text-align:t = "left"
					}
					chapterSeparator {
						position:t = "absolute"
						pos:t = "pw, 0"
					}
				}
			}
			Button_close {
				id:t = "btn_close"
				on_click:t = "goBack"
			}
		}
	}
	gamercard_div {

	}
}
timer {
	id:t = "update_timer"
	timer_handler_func:t = "onTimer"
	timer_interval_msec:t = "1000"
}