tdiv {
	id:t = "page_content"
	size:t = "pw, ph"
	flow:t = "vertical"
	margin-top:t = "2@blockInterval"
	tdiv {
		flow:t = "horizontal"
		size:t = "pw, 8@blockInterval"
		position:t = "relative"
		bgcolor:t = "@evenTrColor"
		margin-bottom:t = "2@blockInterval"
		textareaNoTab {
			id:t = "total_received"
			margin-left:t = "1@blockInterval"
			valign:t = "center"
			overlayTextColor:t = "active"
		}
		tdiv {
			size:t = "450@sf/@pf, ph"
			position:t = "absolute"
			left:t = "pw-w-1@blockInterval"
			popupFilter {
				margin-right:t = "1@blockInterval"
				valign:t = "center"
			}
			EditBox {
				id:t = "filter_edit_box"
				width:t = "fw"
				on_change_value:t = "applySkinFilter"
				on_cancel_edit:t = "onFilterCancel"
				text:t = ""
				edit-hint:t = "#contacts/search_placeholder"
				valign:t = "center"
			}
		}
	}
	textareaNoTab {
		id:t = "empty_text"
		overlayTextColor:t = "active"
		normalFont:t = "yes"
		position:t = "absolute"
		pos:t = "0.5pw-0.5w, 0.5ph-0.5h"
		text:t = "#profile/skins/filterStrong"
		display:t = "hide"
	}
	tdiv {
		id:t = "content"
		position:t = "relative"
		size:t = "pw, fh"
		flow:t = "horizontal"
		profileContentLeft {
			id:t = "treeSkinsNest"
		}
		profileContentSeparator {

		}
		profileContentRight {
			position:t = "relative"
			size:t = "fw, ph"
			flow:t = "vertical"
			tdiv {
				id:t = "skin_desc"
				flow:t = "vertical"
				size:t = "pw, 0.33ph"
				overflow:t = "auto"
			}
			tdiv {
				position:t = "relative"
				size:t = "pw, 2@sf/@pf"
				background-color:t = "@separatorColor"
				margin:t = "0, 6@sf/@pf, 0, 19@sf/@pf"
			}
			listbox {
				id:t = "skins_list"
				size:t = "pw, fh"
				margin-top:t = "1@blockInterval"
				flow:t = "h-flow"
				moveX:t = "linear"
				moveY:t = "closest"
				navigatorShortcuts:t = "yes"
				move-only-hover:t = "yes"
				on_select:t = "onSkinSelect"
			}
		}
	}
}