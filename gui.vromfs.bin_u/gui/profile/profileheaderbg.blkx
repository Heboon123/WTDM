tdiv {
	position:t = "root"
	width:t = "1@swOrRwInVr"
	pos:t = "(sw-w)/2, 1@bhInVr"
	height:t = "1@maxAccountHeaderHeight"
	id:t = "profile_header_picture"
	overflow-y:t = "auto"
	profileHeaderBackground {
		id:t = "profileHeaderBackground"
		position:t = "absolute"
		width:t = "sw*2"
		height:t = "@maxAccountHeaderHeight"
		pos:t = "(pw-w)/2, 0"
		background-color:t = "#FFFFFFFF"
		background-image:t = ""
		background-repeat:t = "aspect-ratio"
	}
}
tdiv {
	position:t = "absolute"
	pos:t = "(sw-w)/2, 1@bhInVr"
	size:t = "1@swOrRwInVr, 200@sf/@pf"
	rotation:t = "180"
	background-image:t = "!ui/images/profile/wnd_gradient.svg"
	background-color:t = "#402F4453"
}