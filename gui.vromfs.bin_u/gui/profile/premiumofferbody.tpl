tdiv {
  height:t='357@sf/@pf'
  tdiv {
    position:t='relative'
    pos:t='0, 0.5ph-0.5h + 20@sf/@pf'
    flow:t='vertical'
    tdiv {
      position:t='relative'
      pos:t='0.5pw-0.5w, 0'
      text {
        mediumFont:t='yes'
        overlayTextColor:t='userlog'
        text:t='<<title>>'
      }
      activeText {
        mediumFont:t='yes'
        text:t=' | '
      }
      activeText {
        mediumFont:t='yes'
        text:t='<<premiumTime>>'
      }
    }
    textareaNoTab {
      position:t='relative'
      pos:t='0.5pw-0.5w, 20@sf/@pf'
      flow:t='vertical'
      text:t='<<premiumDescription>>'
    }
  }
}
