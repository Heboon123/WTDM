root {
	blur {

	}
	blur_foreground {

	}
	frame {
		pos:t = "0.5pw-0.5w, 0.5ph-0.5h"
		position:t = "absolute"
		width:t = "80%sh"
		max-width:t = "1080@sf/@pf+2@framePadding"
		max-height:t = "@rh"
		class:t = "wndNav"
		frame_header {
			activeText {
				id:t = "promo_name"
				width:t = "p.p.w-2@buttonHeight"
				caption:t = "yes"
				pare-text:t = "yes"
			}
			Button_close {
				id:t = "btn_close"
			}
		}
		img {
			id:t = "promo_image"
			width:t = "pw"
			height:t = "0.75w"
			max-width:t = "1080@sf/@pf"
			max-height:t = "810@sf/@pf"
			position:t = "relative"
			pos:t = "50%pw-50%w, 0"
			background-image:t = "#ui/images/login_reward?P1"
		}
		tdiv {
			width:t = "pw"
			max-height:t = "fh"
			position:t = "relative"
			pos:t = "0, 1@framePadding"
			overflow-y:t = "auto"
			textarea {
				id:t = "promo_desc"
				position:t = "relative"
				pos:t = "50%pw-50%w, 0"
				max-width:t = "pw"
				removeParagraphIndent:t = "yes"
				text:t = ""
				font-bold:t = "@fontMedium"
				on_link_click:t = "proccessLinkFromText"
			}
		}
		navBar {
			navMiddle {
				paginatorPlace {
					id:t = "paginator_place"
					smallPaginatorWidth:t = "yes"
				}
			}
			navRight {
				Button_text {
					id:t = "btn_ok"
					text:t = "#mainmenu/acceptObjective"
					btnName:t = "X"
					on_click:t = "onAcceptClick"
					visualStyle:t = "secondary"
					buttonWink {

					}
					ButtonImg {

					}
				}
			}
		}
	}
}