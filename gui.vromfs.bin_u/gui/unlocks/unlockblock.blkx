padding:t = "1@tooltipPadding"
class:t = ""
tdiv {
	width:t = "pw"
	flow:t = "vertical"
	allowActionTooltipBlock {
		id:t = "allow_action_text_block"
		width:t = "pw"
		display:t = "hide"
		textareaNoTab {
			id:t = "allow_action_text"
			width:t = "pw"
			smallFont:t = "yes"
			textHide:t = "yes"
		}
	}
	contentBlock {
		id:t = "contentBlock"
		css-hier-invalidate:t = "yes"
		width:t = "pw"
		tdiv {
			width:t = "pw"
			div {
				id:t = "award_image_sizer"
				size:t = "@profileUnlockIconSize, @profileUnlockIconSize"
				layeredIconContainer {
					id:t = "award_image"
					size:t = "ph, ph"
					pos:t = "50%pw-50%w, 50%ph-50%h"
					position:t = "absolute"
				}
				img {
					id:t = "award_multiplier"
					background-image:t = "#ui/gameuiskin#window_body_shadow"
					pos:t = "ph-w, ph-h + 0.05ph"
					position:t = "absolute"
					display:t = "hide"
					textarea {
						id:t = "amount_text"
						caption:t = "yes"
						overlayTextColor:t = "userlog"
						text:t = ""
						removeParagraphIndent:t = "yes"
						padding:t = "0.05ph"
					}
				}
			}
			div {
				pos:t = "1@tooltipPadding, 0"
				position:t = "relative"
				flow:t = "vertical"
				width:t = "fw - 1@tooltipPadding"
				textareaNoTab {
					id:t = "award_title_text"
					width:t = "pw"
				}
				textareaNoTab {
					id:t = "unlock_name"
					width:t = "pw"
					pare-text:t = "yes"
					overlayTextColor:t = "unlockHeader"
					padding-top:t = "6@sf/@pf"
				}
				textareaNoTab {
					id:t = "desc_text"
					smallFont:t = "yes"
					width:t = "pw"
					padding-top:t = "2@dp"
				}
				challengeDescriptionProgress {
					id:t = "progress"
					max-width:t = "pw"
					value:t = ""
					display:t = "hide"
				}
				textareaNoTab {
					id:t = "mainCond"
					width:t = "pw"
					overflow:t = "hidden"
					smallFont:t = "yes"
					text:t = ""
					margin-top:t = "@blockInterval"
				}
				textareaNoTab {
					id:t = "multDesc"
					width:t = "pw"
					overflow:t = "hidden"
					smallFont:t = "yes"
					text:t = ""
				}
				textareaNoTab {
					id:t = "conds"
					width:t = "pw"
					overflow:t = "hidden"
					smallFont:t = "yes"
					text:t = ""
					margin-top:t = "@blockInterval"
				}
				textareaNoTab {
					id:t = "obtain_info"
					width:t = "pw"
					overflow:t = "hidden"
					smallFont:t = "yes"
					text:t = ""
				}
				textareaNoTab {
					id:t = "mult_awards_text"
					smallFont:t = "yes"
					width:t = "pw"
				}
				textareaNoTab {
					id:t = "award_text"
					smallFont:t = "yes"
					width:t = "pw"
				}
				div {
					id:t = "state"
					flow:t = "horizontal"
					width:t = "pw"
					padding-top:t = "6@sf/@pf"
					display:t = "hide"
					cardImg {
						id:t = "state_icon"
						pos:t = "-20%w, 0"
						size:t = "@unlockIconSize, @unlockIconSize"
						re-type:t = "9rect"
						background-repeat:t = "expand"
						background-image:t = "#ui/gameuiskin#favorite"
						margin:t = "0, 0, 8@sf/@pf, 0"
					}
					tdiv {
						width:t = "fw"
						pos:t = "0, ph/2-h/2"
						position:t = "relative"
						flow:t = "h-flow"
						textareaNoTab {
							id:t = "state_text"
							width:t = "pw"
							smallFont:t = "yes"
							hideEmptyText:t = "yes"
						}
					}
				}
			}
		}
		tdiv {
			id:t = "subunlocks"
			flow:t = "h-flow"
			width:t = "0.5@sf"
			position:t = "relative"
			pos:t = "50%(pw-w), 0"
			margin-top:t = "@blockInterval"
			display:t = "hide"
		}
	}
}