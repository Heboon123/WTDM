EditBox {
	id:t = "filter_edit_box"
	pos:t = "0, ph/2-h/2"
	position:t = "relative"
	width:t = "fw"
	on_activate:t = "onFilterEditBoxActivate"
	on_change_value:t = "onFilterEditBoxChangeValue"
	on_cancel_edit:t = "onFilterEditBoxCancel"
	text:t = ""
	edit-hint:t = "#contacts/search_placeholder"
}
Button_text {
	id:t = "filter_edit_cancel_btn"
	pos:t = "0, ph/2-h/2"
	position:t = "relative"
	class:t = "image"
	showConsoleImage:t = "no"
	noMargin:t = "yes"
	tooltip:t = "#mainmenu/btnReset"
	on_click:t = "onFilterEditBoxCancel"
	display:t = "hide"
	img {
		background-image:t = "#ui/gameuiskin#btn_close.svg"
	}
}