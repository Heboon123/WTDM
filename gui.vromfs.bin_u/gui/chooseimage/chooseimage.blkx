frame {
	id:t = "wnd_frame"
	class:t = "wndNav"
	size:t = "pw, ph"
	css-hier-invalidate:t = "yes"
	opaque:t = "yes"
	frame_header {
		css-hier-invalidate:t = "yes"
		HorizontalListBox {
			id:t = "image_types"
			height:t = "ph"
			class:t = "header"
			normalFont:t = "yes"
			activeAccesskeys:t = "RS"
			on_select:t = "onImageTypeSelect"
		}
		Button_close {
			id:t = "btn_back"
		}
	}
	tdiv {
		id:t = "listsNest"
		size:t = "pw, ph"
		tdiv {
			id:t = "avatars_list"
			position:t = "absolute"
			flow:t = "h-flow"
			size:t = "pw, ph"
			behavior:t = "posNavigator"
			canSelectNone:t = "yes"
			navigatorShortcuts:t = "yes"
			showSelect:t = "always"
			on_click:t = "onImageChoose"
			on_dbl_click:t = "onDblClick"
			on_select:t = "onImageSelect"
		}
		tdiv {
			id:t = "frames_list"
			position:t = "absolute"
			flow:t = "h-flow"
			size:t = "pw, ph"
			behavior:t = "posNavigator"
			canSelectNone:t = "yes"
			navigatorShortcuts:t = "yes"
			showSelect:t = "always"
			on_click:t = "onImageChoose"
			on_dbl_click:t = "onDblClick"
			on_select:t = "onImageSelect"
		}
	}
	navBar {
		navMiddle {
			id:t = "paginator_place"
		}
		navRight {
			Button_text {
				id:t = "btn_select"
				btnName:t = "A"
				_on_click:t = "onAction"
				visualStyle:t = "secondary"
				buttonWink {

				}
				ButtonImg {

				}
			}
			Button_text {
				id:t = "btn_fav"
				text:t = "#preloaderSettings/trackProgress"
				btnName:t = "A"
				on_click:t = "onToggleFav"
				visualStyle:t = "secondary"
				unlockId:t = ""
				isChecked:t = "no"
				buttonWink {

				}
				ButtonImg {

				}
			}
			Button_text {
				id:t = "btn_buy"
				text:t = "#mainmenu/btnOrder"
				btnName:t = "A"
				on_click:t = "onBuy"
				visualStyle:t = "purchase"
				buttonWink {

				}
				buttonGlance {

				}
				ButtonImg {

				}
			}
		}
	}
}