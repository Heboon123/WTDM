{
  "frame": {
    "id": "wnd_frame",
    "class": "wndNav",
    "size": "pw, ph",
    "css-hier-invalidate": "yes",
    "opaque": "yes",
    "frame_header": {
      "css-hier-invalidate": "yes",
      "HorizontalListBox": {
        "id": "image_types",
        "height": "ph",
        "class": "header",
        "normalFont": "yes",
        "activeAccesskeys": "RS",
        "on_select": "onImageTypeSelect"
      },
      "Button_close": {
        "id": "btn_back"
      }
    },
    "tdiv": {
      "id": "listsNest",
      "size": "pw, ph",
      "tdiv": [
        {
          "id": "avatars_list",
          "position": "absolute",
          "flow": "h-flow",
          "size": "pw, ph",
          "behavior": "posNavigator",
          "canSelectNone": "yes",
          "navigatorShortcuts": "yes",
          "showSelect": "always",
          "on_click": "onImageChoose",
          "on_dbl_click": "onDblClick",
          "on_select": "onImageSelect"
        },
        {
          "id": "frames_list",
          "position": "absolute",
          "flow": "h-flow",
          "size": "pw, ph",
          "behavior": "posNavigator",
          "canSelectNone": "yes",
          "navigatorShortcuts": "yes",
          "showSelect": "always",
          "on_click": "onImageChoose",
          "on_dbl_click": "onDblClick",
          "on_select": "onImageSelect"
        }
      ]
    },
    "navBar": {
      "navMiddle": {
        "id": "paginator_place"
      },
      "navRight": {
        "Button_text": [
          {
            "id": "btn_select",
            "btnName": "A",
            "_on_click": "onAction",
            "visualStyle": "secondary",
            "buttonWink": {},
            "ButtonImg": {}
          },
          {
            "id": "btn_fav",
            "text": "#preloaderSettings/trackProgress",
            "btnName": "A",
            "on_click": "onToggleFav",
            "visualStyle": "secondary",
            "buttonWink": {},
            "ButtonImg": {}
          },
          {
            "id": "btn_buy",
            "text": "#mainmenu/btnOrder",
            "btnName": "A",
            "on_click": "onBuy",
            "visualStyle": "purchase",
            "buttonWink": {},
            "buttonGlance": {},
            "ButtonImg": {}
          }
        ]
      }
    }
  }
}