root {
	type:t = "big"
	blur {

	}
	blur_foreground {

	}
	frame {
		pos:t = "0.5pw-0.5w, 0.5ph-0.5h"
		position:t = "absolute"
		width:t = "80%sh"
		max-width:t = "800*@sf/@pf_outdated + 2@framePadding"
		max-height:t = "@rh"
		class:t = "wndNav"
		frame_header {
			textareaNoTab {
				id:t = "award_name"
				width:t = "p.p.w-2@buttonHeight"
				caption:t = "yes"
				pare-text:t = "yes"
				overlayTextColor:t = "active"
			}
			Button_close {
				id:t = "btn_close"
			}
		}
		img {
			id:t = "award_image"
			width:t = "pw"
			height:t = "0.75w"
			max-width:t = "800*@sf/@pf_outdated"
			max-height:t = "600*@sf/@pf_outdated"
			pos:t = "50%pw-50%w, 0"
			position:t = "relative"
			background-image:t = "#ui/images/login_reward?P1"
			dummy {
				id:t = "award_image_button"
				link:t = ""
				on_click:t = "onMsgLink"
				bqKey:t = "show_unlock_window"
				size:t = "pw, ph"
			}
		}
		tdiv {
			width:t = "pw"
			max-height:t = "fh"
			pos:t = "0, 1@framePadding"
			position:t = "relative"
			flow:t = "vertical"
			overflow-y:t = "auto"
			textarea {
				id:t = "award_desc"
				pos:t = "50%pw-50%w, 0"
				position:t = "relative"
				max-width:t = "pw"
				removeParagraphIndent:t = "yes"
				text:t = ""
				font-bold:t = "@fontMedium"
				on_link_click:t = "proccessLinkFromText"
			}
			textarea {
				id:t = "award_reward"
				pos:t = "50%pw-50%w, 0"
				position:t = "relative"
				max-width:t = "pw"
				overlayTextColor:t = "userlog"
				removeParagraphIndent:t = "yes"
				text:t = ""
				on_link_click:t = "proccessLinkFromText"
			}
			tdiv {
				id:t = "reward_markup"
				pos:t = "0.5pw-0.5w, 0"
				position:t = "relative"
				flow:t = "vertical"
				display:t = "hide"
			}
			rankUpList {
				id:t = "reward_aircrafts"
				max-width:t = "pw"
				pos:t = "0.5pw-0.5w,0"
				position:t = "relative"
				flow:t = "h-flow"
				holdTooltipChildren:t = "yes"
				on_activate:t = "onUnitActivate"
				on_click:t = "onUnitActivate"
				on_r_click:t = "onUnitActivate"
			}
			textAreaCentered {
				id:t = "next_award"
				max-width:t = "pw"
				pos:t = "50%pw-50%w, 0.03@sf"
				position:t = "relative"
				textHide:t = "yes"
				text:t = ""
			}
		}
		navBar {
			navLeft {
				Button_text {
					id:t = "btn_sendEmail"
					display:t = "hide"
					text:t = "#mainmenu/btnSendEmail"
					on_click:t = "sendInvitation"
				}
			}
			navRight {
				Button_text {
					id:t = "btn_use_decorator"
					text:t = "#decorator/use/decal"
					btnName:t = "Y"
					_on_click:t = "onUseDecorator"
					enable:t = "no"
					display:t = "hide"
					ButtonImg {

					}
				}
				Button_text {
					id:t = "btn_ok"
					text:t = "#mainmenu/btnOk"
					btnName:t = "X"
					_on_click:t = "onOk"
					ButtonImg {

					}
				}
			}
		}
	}
}