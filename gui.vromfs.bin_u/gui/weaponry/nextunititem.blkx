position:t = "relative"
pos:t = ""
total-input-transparent:t = "yes"
css-hier-invalidate:t = "yes"
inactive:t = "yes"
tdiv {
	flow:t = "vertical"
	width:t = "@slot_width"
	textarea {
		id:t = "nextResearch_title"
		removeParagraphIndent:t = "yes"
		overlayTextColor:t = "active"
		padding-left:t = "1@slot_interval"
	}
	rankUpList {
		id:t = "next_unit"
		size:t = "@slot_width, @slot_height + 2@slot_vert_pad"
		holdTooltipChildren:t = "yes"
	}
	textarea {
		id:t = "available_free_exp_text"
		position:t = "relative"
		pos:t = "0, -1@shopItemPad"
		removeParagraphIndent:t = "yes"
		padding-left:t = "1@slot_interval"
	}
}