root {
	blur {

	}
	blur_foreground {

	}
	frame {
		width:t = "0.75@scrn_tgt+10"
		pos:t = "50%pw-50%w, 45%ph-40%h"
		position:t = "absolute"
		class:t = "wnd"
		frame_header {
			Button_close {

			}
			top_right_holder {
				hasRightIndent:t = "yes"
				CheckBox {
					id:t = "skip_this"
					pos:t = "0, 50%ph-50%h"
					position:t = "relative"
					text:t = "#options/doNotAskMeAgain"
					on_change_value:t = "onSkipMessageBox"
					btnName:t = "Y"
					CheckBoxImg {

					}
					ButtonImg {

					}
				}
			}
		}
		textAreaCentered {
			id:t = "msgText"
			width:t = "pw"
			text:t = ""
		}
		textAreaCentered {
			id:t = "listText"
			width:t = "pw"
			text:t = ""
			margin-top:t = "2*@sf/100.0"
		}
		tdiv {
			id:t = "buttons"
			pos:t = "50%pw-50%w, 0.03@sf"
			position:t = "relative"
			behavior:t = "posNavigator"
			navigatorShortcuts:t = "noSelect"
			total-input-transparent:t = "yes"
			Button_text {
				id:t = "btn_cancel"
				text:t = ""
				btnName:t = "B"
				on_click:t = "goBack"
				showConsoleImage:t = "no"
			}
			Button_text {
				id:t = "btn_select"
				text:t = "#mainmenu/toBattle"
				on_click:t = "onStart"
				showConsoleImage:t = "no"
			}
		}
		CheckBox {
			id:t = "slots-autoweapon"
			pos:t = "50%pw-50%w, 0.01@sf"
			position:t = "relative"
			class:t = "with_image"
			text:t = "#shop/auto_buy_weapons"
			smallFont:t = "yes"
			display:t = "hide"
			on_change_value:t = "onSlotsChangeAutoRefill"
			infoImg {
				size:t = "@cIco, @cIco"
				background-svg-size:t = "@cIco, @cIco"
				background-image:t = "#ui/gameuiskin#btn_weapons.svg"
			}
			CheckBoxImg {

			}
		}
	}
}
dummy {
	behavior:t = "button"
	on_click:t = "onAcceptSelection"
	btnName:t = "A"
}