tdiv {
	width:t = "pw"
	padding-bottom:t = "0.01@scrn_tgt"
	Button_text {
		id:t = "btn_url_mission_refresh"
		pos:t = "0,50%ph-50%h"
		position:t = "relative"
		class:t = "image"
		showConsoleImage:t = "no"
		tooltip:t = "#mainmenu/btnRefreshByUrl"
		_on_click:t = "onUrlMissionRefresh"
		display:t = "hide"
		enable:t = "no"
		img {
			background-image:t = "#ui/gameuiskin#refresh.svg"
		}
	}
	img {
		id:t = "descr-flag"
		top:t = "50%ph-50%h"
		position:t = "relative"
		iconType:t = "country"
		background-image:t = ""
		background-repeat:t = "aspect-ratio"
	}
	tdiv {
		pos:t = "1*@scrn_tgt/100.0, 0"
		position:t = "relative"
		width:t = "fw"
		flow:t = "vertical"
		titleTextArea {
			id:t = "descr-name"
			position:t = "relative"
			hideEmptyText:t = "yes"
			text:t = ""
		}
		textareaNoTab {
			id:t = "descr-date"
			hideEmptyText:t = "yes"
			text:t = ""
		}
		textareaNoTab {
			id:t = "descr-coop"
			overlayTextColor:t = "userlog"
			hideEmptyText:t = "yes"
			text:t = ""
		}
	}
}
tdiv {
	size:t = "pw,fh"
	pos:t = "0,0"
	position:t = "relative"
	overflow-y:t = "auto"
	scrollbarShortcuts:t = "yes"
	flow:t = "vertical"
	tdiv {
		id:t = "descr_scroll_top_point"
		pos:t = "0,0"
		position:t = "absolute"
	}
	img {
		id:t = "descr-chapterImg"
		width:t = "p.p.w"
		height:t = "w*128/512"
		background-repeat:t = "expand"
		background-image:t = ""
	}
	tdiv {
		id:t = "descr-countries"
		iconType:t = "country"
		display:t = "hide"
		height:t = "7.5*@scrn_tgt/100.0"
	}
	textareaNoTab {
		id:t = "descr-wpAward"
		width:t = "pw-10"
		pos:t = "0, 0"
		position:t = "relative"
		hideEmptyText:t = "yes"
		text:t = ""
	}
