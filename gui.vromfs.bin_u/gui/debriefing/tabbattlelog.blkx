debriefingTab {
	id:t = "battle_log_tab"
	tdiv {
		size:t = "pw, ph"
		flow:t = "vertical"
		debrBlock {
			width:t = "pw"
			padding:t = "1@debrPad"
			RadioButtonList {
				id:t = "battle_log_filter"
				navigatorShortcuts:t = "yes"
				on_select:t = "onChangeBattlelogFilter"
			}
			debrSeparator {
				class:t = "bottom"
			}
		}
		debrBlock {
			id:t = "battle_log_div"
			size:t = "pw, fh"
			overflow-y:t = "auto"
			scrollbarShortcuts:t = "yes"
			tdiv {
				width:t = "pw"
				min-height:t = "ph"
				margin:t = "1@debrPad"
				chatLog {
					width:t = "pw"
					id:t = "battle_log"
					style:t = "color:@white"
					text:t = ""
					on_link_click:t = "onChatLinkClick"
					on_link_rclick:t = "onChatLinkRClick"
				}
			}
		}
	}
}