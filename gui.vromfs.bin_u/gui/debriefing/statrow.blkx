title:t = "$tooltipObj"
td {
	width:t = "0.5pw"
	min-width:t = "0.4@sf"
	img {
		id:t = "name_icon"
		size:t = "1@sIco, 1@sIco"
		top:t = "0.5ph-0.5h"
		position:t = "relative"
		background-image:t = ""
		background-svg-size:t = "1@sIco, 1@sIco"
	}
	activeText {
		id:t = "name"
	}
}
td {
	id:t = "td_value"
	min-width:t = "0.1@sf"
	cellType:t = "tdRight"
	activeText {
		id:t = "value"
		parseTags:t = "yes"
	}
	img {
		id:t = "value_image"
		size:t = "1@sIco, 1@sIco"
		top:t = "0.5ph-0.5h"
		position:t = "relative"
		background-image:t = ""
		background-svg-size:t = "1@sIco, 1@sIco"
	}
}
td {
	id:t = "td_reward"
	min-width:t = "0.1@sf"
	cellType:t = "tdRight"
	activeText {
		id:t = "reward"
		parseTags:t = "yes"
	}
	img {
		id:t = "reward_image"
		size:t = "1@sIco, 1@sIco"
		top:t = "0.5ph-0.5h"
		position:t = "relative"
		background-image:t = ""
		background-svg-size:t = "1@sIco, 1@sIco"
	}
}
tooltipObj {
	id:t = "tooltip_"
	display:t = "hide"
	on_tooltip_open:t = "onTrTooltipOpen"
	on_tooltip_close:t = "onTooltipObjClose"
}