tdiv {
	id:t = "gc_navbar_sizer"
	size:t = "pw, ph"
	navLeft {
		Button_text {
			id:t = "btn_view_replay"
			text:t = "#mainmenu/btnViewReplay"
			btnName:t = "start"
			_on_click:t = "onViewReplay"
			display:t = "hide"
			ButtonImg {

			}
		}
		Button_text {
			id:t = "btn_save_replay"
			text:t = "#mainmenu/btnSaveReplay"
			_on_click:t = "onSaveReplay"
			display:t = "hide"
			ButtonImg {

			}
		}
		Button_text {
			id:t = "btn_parse_replay"
			text:t = "#hitsAnalisys"
			on_click:t = "onParseReplay"
			display:t = "hide"
			ButtonImg {

			}
		}
		Button_text {
			id:t = "btn_requirements_list"
			text:t = "#unlocks/requirements"
			_on_click:t = "onViewBattleTaskRequirements"
			display:t = "hide"
			enable:t = "no"
			ButtonImg {

			}
		}
		Button_text {
			id:t = "btn_user_options"
			text:t = "#mainmenu/btnUserAction"
			btnName:t = "RB"
			on_click:t = "onUserOptions"
			ButtonImg {

			}
		}
	}
	navMiddle {
		id:t = "nav_middle"
		Button_text {
			id:t = "btn_next"
			class:t = "battle"
			navButtonFont:t = "yes"
			text:t = "#mainmenu/btnOk"
			css-hier-invalidate:t = "yes"
			_on_click:t = "onNext"
			btnName:t = "X"
			display:t = "hide"
			pattern {
				type:t = "bright_texture"
			}
			buttonWink {
				_transp-timer:t = "0"
			}
			buttonGlance {

			}
			ButtonImg {

			}
			btnText {
				id:t = "btn_next_text"
				text:t = "#mainmenu/btnOk"
			}
		}
		Button_text {
			id:t = "btn_skip"
			text:t = "#msgbox/btn_skip"
			_on_click:t = "goBack"
			btnName:t = "B"
			ButtonImg {

			}
		}
	}
	navRight {
		Button_text {
			id:t = "btn_view_highlights"
			class:t = "image"
			text:t = "#mainmenu/btnViewHighlights"
			_on_click:t = "onViewHighlights"
			display:t = "hide"
			enable:t = "no"
			isColoredImg:t = "yes"
			img {
				background-image:t = "#ui/gameuiskin#nv_icon"
			}
			ButtonImg {

			}
		}
		Button_text {
			id:t = "btn_back"
			text:t = "#mainmenu/btnQuit"
			_on_click:t = "goBack"
			btnName:t = "B"
			display:t = "hide"
			visualStyle:t = "secondary"
			buttonWink {

			}
			ButtonImg {

			}
		}
	}
}
dummy {
	_on_click:t = "onNext"
	behavior:t = "accesskey"
	accessKey:t = "Enter"
}
dummy {
	_on_click:t = "goBack"
	btnName:t = "B"
	behavior:t = "accesskey"
}