debriefingTab {
	id:t = "chat_history_tab"
	flow:t = "vertical"
	debrBlock {
		id:t = "chat_history_scroll_block"
		size:t = "pw, ph"
		overflow-y:t = "auto"
		scrollbarShortcuts:t = "yes"
		tdiv {
			id:t = "chat_history_div"
			width:t = "pw"
			min-height:t = "ph"
			margin:t = "1@debrPad"
			chatLog {
				width:t = "pw"
				id:t = "chat_log"
				style:t = "color:@white"
				text:t = ""
				on_link_click:t = "onChatLinkClick"
				on_link_rclick:t = "onChatLinkRClick"
			}
		}
	}
}