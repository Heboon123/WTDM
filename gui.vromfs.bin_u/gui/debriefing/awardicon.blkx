_size-timer:t = "1"
title:t = "$tooltipObj"
awardDiv {
	id:t = "move_part"
	size:t = "ph, ph"
	pos:t = "(pw-w)/2, (ph-h)/2"
	position:t = "absolute"
	noMargin:t = "yes"
	css-hier-invalidate:t = "yes"
	input-transparent:t = "yes"
	total-input-transparent:t = "yes"
	layeredIconContainer {
		id:t = "award_img"
	}
	img {
		id:t = "award_multiplier"
		background-image:t = "#ui/gameuiskin#window_body_shadow"
		pos:t = "pw-w, ph-h + 0.2ph"
		position:t = "relative"
		display:t = "hide"
		textarea {
			id:t = "amount_text"
			overlayTextColor:t = "userlog"
			text:t = ""
			removeParagraphIndent:t = "yes"
			padding:t = "0.05ph"
		}
	}
}
tooltipObj {
	id:t = "tooltip_"
	display:t = "hide"
	on_tooltip_open:t = "onAwardTooltipOpen"
	on_tooltip_close:t = "onTooltipObjClose"
}