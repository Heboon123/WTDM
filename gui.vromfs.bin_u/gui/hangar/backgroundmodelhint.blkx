tdiv {
	id:t = "hangar_hint"
	behavior:t = "Timer"
	timer_handler_func:t = "onBackgroundModelHintTimer"
	position:t = "root"
	input-transparent:t = "yes"
	order-popup:t = "yes"
	smallHint {
		id:t = "click_to_view_hint"
		display:t = "hide"
		text {
			pos:t = "0, 50%ph-50%h"
			position:t = "relative"
			margin:t = "@blockInterval, 0"
			text:t = "#click_to_view"
			tinyFont:t = "yes"
		}
		ButtonImg {
			size:t = "1@infoBtnIconSize, 1@infoBtnIconSize"
			pos:t = "0, 50%ph-50%h"
			position:t = "relative"
			btnName:t = "A"
		}
	}
	tooltipObj {
		id:t = "custom_hint"
	}
}