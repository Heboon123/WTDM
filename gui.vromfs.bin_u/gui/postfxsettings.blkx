hangarControlTracking {

}
tdiv {
	id:t = "postfx_settings"
	pos:t = "30,50"
	size:t = "sw,sh"
	frame {
		pos:t = "30,50"
		behaviour:t = "moveObj"
		class:t = "wndNav"
		flow:t = "vertical"
		width:t = "0.7@sf"
		min-width:t = "2@sliderWidth + 7@optPad"
		total-input-transparent:t = "yes"
		frame_header {
			activeText {
				text:t = "#mainmenu/btnPostFxSettings"
				caption:t = "yes"
			}
			Button_close {
				id:t = "button_close"
			}
		}
		table {
			id:t = "postfx_table"
			behavior:t = "PosOptionsNavigator"
			class:t = "optionsTable"
			width:t = "pw"
			margin:t = "5"
			baseRow:t = "yes"
			selfFocusBorder:t = "yes"
			smallFont:t = "yes"
		}
		navBar {
			navLeft {
				Button_text {
					id:t = "button_reset"
					text:t = "#options/resetToDefaults"
					_on_click:t = "onResetToDefaults"
					btnName:t = "X"
					ButtonImg {

					}
				}
			}
		}
	}
}