position:t = "absolute"
optionlist_container {
	style:t = "size:1@swOrRwInVr,1@shOrRhInVr;background-color:#C0000000;overflow:hidden;"
	div {
		id:t = "btn-credits-close"
		tabStop:t = "yes"
		size:t = "0.9@rw, 0.9@rh"
		pos:t = "50%pw-50%w,50%ph-50%h"
		position:t = "absolute"
		input-transparent:t = "yes"
		tdiv {
			id:t = "credits-text"
			flow:t = "vertical"
			behavior:t = "CreditsScroll"
			position:t = "relative"
			cur_slide_offs:t = "1000000"
			top:t = "0.9sh+h"
			inited:t = "no"
			width:t = "pw"
			textarea {
				id:t = "textarea"
				font-bold:t = "hud"
				input-transparent:t = "yes"
				halign:t = "center"
				text-align:t = "left"
				width:t = "@rw"
				style:t = "font-bold:@fontBigBold;font:@fontMedium;color:@creditsText;"
			}
		}
	}
}
