root {
	type:t = "big"
	blur {

	}
	blur_foreground {

	}
	frame {
		width:t = "0.7*@scrn_tgt"
		flow:t = "vertical"
		class:t = "wndNav"
		isCenteredUnderLogo:t = "yes"
		frame_header {
			activeText {
				id:t = "comment_wnd_title"
				caption:t = "yes"
				text:t = "#clan/writeCommentary"
			}
			Button_close {
				id:t = "btn_back"
			}
		}
		EditBox {
			id:t = "comment_editbox"
			size:t = "pw, 0.2@scrn_tgt"
			margin-top:t = "0.005@scrn_tgt"
			multiline:t = "yes"
			max-len:t = "250"
			font:t = "@fontNormal"
			text:t = ""
			on_change_value:t = "onChangeComment"
			on_cancel_edit:t = "goBack"
		}
		navBar {
			navRight {
				Button_text {
					id:t = "ok_btn"
					pos:t = "pw-w, 0"
					position:t = "relative"
					text:t = "#msgbox/btn_ok"
					_on_click:t = "onOk"
					btnName:t = "A"
					ButtonImg {

					}
				}
			}
		}
	}
}