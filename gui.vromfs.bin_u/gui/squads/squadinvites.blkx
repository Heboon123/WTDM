rootUnderPopupMenu {
	on_click:t = "goBack"
	on_r_click:t = "goBack"
}
popup_menu {
	id:t = "squad_invites"
	position:t = "root"
	not-input-transparent:t = "yes"
	css-hier-invalidate:t = "yes"
	flow:t = "vertical"
	menu_align:t = "top"
	div {
		id:t = "options_block"
		width:t = "0.35@sf"
		padding-bottom:t = "1@blockInterval"
		flow:t = "vertical"
		activeText {
			pos:t = "50%pw - 50%w, 0"
			position:t = "relative"
			text:t = "#options/squadSize"
			text-align:t = "center"
		}
		ComboBox {
			id:t = "squad_size_option"
			size:t = "pw, @buttonHeight"
			pos:t = "50%pw - 50%w, 1@blockInterval"
			position:t = "relative"
			btnName:t = "X"
			on_select:t = "onSquadSizeChange"
		}
		textAreaCentered {
			width:t = "pw"
			pos:t = "50%pw - 50%w, 1@blockInterval"
			position:t = "relative"
			smallFont:t = "yes"
			text:t = "#guiHints/squadSize"
		}
	}
	CheckBox {
		id:t = "receive_applications"
		width:t = "fw"
		text:t = "#squad/receiveApplications"
		value:t = "yes"
		btnName:t = "Y"
		on_change_value:t = "onReceiveApplications"
		CheckBoxImg {

		}
		ButtonImg {

		}
	}
	activeText {
		id:t = "invited_players_header"
		pos:t = "50%pw - 50%w, 1@blockInterval"
		position:t = "relative"
		text:t = "#squad/invitedPlayers"
		text-align:t = "center"
		margin:t = "0.01@sf, 0"
	}
	div {
		id:t = "invites_list"
		pos:t = "50%pw-50%w, 0"
		position:t = "relative"
		padding-bottom:t = "1@blockInterval"
		flow:t = "h-flow"
		flow-align:t = "center"
	}
	activeText {
		id:t = "applications_list_header"
		pos:t = "50%pw - 50%w, 1@blockInterval"
		position:t = "relative"
		text:t = "#squad/applications"
		text-align:t = "center"
		margin:t = "0.01@sf, 0"
	}
	div {
		id:t = "applications_list"
		pos:t = "50%pw-50%w, 0"
		position:t = "relative"
		padding-bottom:t = "1@blockInterval"
		flow:t = "h-flow"
		flow-align:t = "center"
	}
	popup_menu_arrow {

	}
}
gamercard_div {

}
chatPopupNest {
	id:t = "chatPopupNest"
	position:t = "absolute"
	pos:t = "1@bw + 0.5@rw-w-0.55@titleLogoPlateWidth, @topBarHeight"
}