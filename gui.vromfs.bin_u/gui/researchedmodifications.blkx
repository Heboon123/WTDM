root {
	type:t = "big"
	blur {

	}
	blur_foreground {

	}
	frame {
		width:t = "1@researchNotificationWidth"
		max-height:t = "1@maxWindowHeight"
		class:t = "wndNav"
		isCenteredUnderLogo:t = "yes"
		frame_header {
			activeText {
				text:t = "#mainmenu/newResearch/title"
				caption:t = "yes"
			}
			Button_close {

			}
		}
		img {
			width:t = "pw"
			height:t = "200.0/600pw"
			background-image:t = "#ui/images/engineer"
			pos:t = "50%pw-50%w,0"
			position:t = "relative"
			tdiv {
				size:t = "pw,ph"
				position:t = "absolute"
				background-color:t = "@shadeBackgroundColor4"
			}
			rankUpList {
				id:t = "rankup_aircraft_table"
				overflow-y:t = "auto"
				scrollbarShortcuts:t = "yes"
				position:t = "absolute"
				pos:t = "50%pw-50%w, 50%ph-50%h"
				holdTooltipChildren:t = "yes"
			}
		}
		navBar {
			navLeft {

			}
			navRight {
				Button_text {
					id:t = "btn_buy"
					text:t = "#mainmenu/btnOrder"
					btnName:t = "A"
					_on_click:t = "purchaseUnit"
					visualStyle:t = "purchase"
					buttonWink {

					}
					buttonGlance {

					}
					ButtonImg {

					}
				}
				Button_text {
					id:t = "btn_trainCrew"
					text:t = "#mainmenu/btnTrainCrew"
					btnName:t = "A"
					_on_click:t = "trainCrew"
					visualStyle:t = "secondary"
					buttonWink {

					}
					ButtonImg {

					}
				}
				Button_text {
					id:t = "btn_exit"
					text:t = "#mainmenu/finish"
					btnName:t = "B"
					_on_click:t = "goBack"
					ButtonImg {

					}
				}
			}
		}
	}
}