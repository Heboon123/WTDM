tdiv {
	id:t = "target_cursor"
	behavior:t = "Timer"
	timer_handler_func:t = "onTargetingCursorTimer"
	size:t = "48@sf/@pf, 48@sf/@pf"
	left:t = "sw/2-w/2"
	top:t = "sh/2-h/2"
	position:t = "root"
	background-image:t = "#ui/gameuiskin#protection_analysis_target.svg"
	background-svg-size:t = "48@sf/@pf, 48@sf/@pf"
	background-color:t = "@minorTextColor"
}
tooltipObj {
	id:t = "dmviewer_hint"
	behavior:t = "Timer"
	timer_handler_func:t = "onDMViewerHintTimer"
	position:t = "root"
	pos:t = "sw/2-w/2, 70%sh-h/2"
	style:t = "padding:16@sf/@pf; color-factor:192;"
	display:t = "hide"
	flow:t = "vertical"
	input-transparent:t = "yes"
	order-popup:t = "yes"
	skip-cursor:t = "skip"
	textareaNoTab {
		id:t = "dmviewer_title"
		overlayTextColor:t = "active"
		text:t = ""
		input-transparent:t = "yes"
	}
	tdiv {
		id:t = "dmviewer_desc"
		position:t = "relative"
		margin-top:t = "8@sf/@pf"
		input-transparent:t = "yes"
		flow:t = "vertical"
	}
	movie {
		id:t = "dmviewer_anim"
		display:t = "hide"
		size:t = "@tooltipAnimationWidth,@tooltipAnimationHeight"
		movie-autoStart:t = "yes"
		movie-loop:t = "yes"
	}
	tdiv {
		id:t = "dmviewer_ext_hint"
		flow:t = "vertical"
		position:t = "relative"
		width:t = "0.5@sf"
		total-input-transparent:t = "yes"
		display:t = "hide"
		rowSeparator {
			margin:t = "0, @blockInterval"
		}
		textareaNoTab {
			position:t = "relative"
			margin-top:t = "@blockInterval"
			overlayTextColor:t = "active"
			text:t = "#xray_ext_hint/title"
		}
		textareaNoTab {
			id:t = "dmviewer_ext_hint_desc"
			position:t = "relative"
			margin-top:t = "@blockInterval"
			hideEmptyText:t = "yes"
			text:t = ""
			smallFont:t = "yes"
			width:t = "pw"
		}
		img {
			id:t = "dmviewer_ext_hint_icon"
			position:t = "relative"
			margin-top:t = "2@blockInterval"
			size:t = "@dIco, @dIco"
			background-image:t = ""
		}
		loadingHint {
			id:t = "dmviewer_ext_hint_shortcut"
			behaviour:t = "bhvHint"
			position:t = "relative"
			width:t = "pw"
			isXrayHint:t = "yes"
			smallFont:t = "yes"
			isWrapInRowAllowed:t = "yes"
			flow-align:t = "left"
		}
	}
	tdiv {
		id:t = "topValueHint"
		display:t = "hide"
		img {
			background-image:t = "#ui/gameuiskin#spec_icon2.svg"
			background-svg-size:t = "@sIco,@sIco"
			size:t = "@sIco,@sIco"
		}
		textareaNoTab {
			text:t = "#crewSkillParameter/topSkillValue"
			overlayTextColor:t = "faded"
			smallFont:t = "yes"
			valign:t = "center"
		}
	}
	textareaNoTab {
		id:t = "dmviewer_debug"
		text:t = ""
		input-transparent:t = "yes"
	}
}