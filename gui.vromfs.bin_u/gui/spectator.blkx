tdiv {
	id:t = "spectator_root"
	size:t = "sw - 2@bw, sh - 2@bh"
	pos:t = "pw/2-w/2, ph/2-h/2"
	position:t = "absolute"
	multiplayerScore {
		pos:t = "50%pw-50%w, 2@spectatorToolbarPadding + 1@spectatorToolbarBtnSize + 0.02@scrn_tgt"
		position:t = "absolute"
	}
	tdiv {
		id:t = "stats_left"
		css-hier-invalidate:t = "yes"
		re-type:t = "9rect"
		width:t = "1@spectatorTableWidth + 1@spectatorBw"
		position:t = "absolute"
		pos:t = "-1@spectatorBw -4@dp, 1@spectatorTopOffset"
		min-height:t = "2@sIco"
		background-image:t = "#ui/gameuiskin#expandable_item_ww_transp_reflect"
		background-position:t = "2, 2, 0, 2"
		background-repeat:t = "expand"
		background-color:t = "#99999999"
		padding:t = "1@spectatorBw, 1@dp, 1@framePadding, 1@dp"
		flow:t = "vertical"
		tdiv {
			id:t = "content"
			overflow-y:t = "auto"
			scroll-align:t = "left"
			width:t = "1@spectatorRowWidth"
			pos:t = "0, 0"
			position:t = "relative"
			min-height:t = "1@spectatorRowHeight"
			max-height:t = "p.p.h -1@spectatorTopOffset -1@mIco -1@spectatorTableWidth -24@sf/@pf"
			invisibleSelection:t = "yes"
			table {
				id:t = "table_team1"
				class:t = "spectatorTeamTable"
				side:t = "left"
				team:t = ""
				behavior:t = "posNavigator"
				clearOnFocusLost:t = "yes"
				navigatorShortcuts:t = "yes"
				selfFocusBorder:t = "yes"
				on_click:t = "onPlayerClick"
				on_r_click:t = "onPlayerRClick"
				on_activate:t = "onSelectPlayer"
				on_set_focus:t = "onChangeFocusTable"
			}
		}
		dyncampresult {
			id:t = "header"
			pos:t = "1.5@mIco, -0.5@mIco -h/2"
			position:t = "absolute"
			textShade:t = "yes"
		}
	}
	tdiv {
		id:t = "stats_right"
		css-hier-invalidate:t = "yes"
		re-type:t = "9rect"
		width:t = "1@spectatorTableWidth + 1@spectatorBw"
		position:t = "absolute"
		pos:t = "pw - w + 4@dp +1@spectatorBw, 1@spectatorTopOffset"
