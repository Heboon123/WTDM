					id:t = "history_container"
					position:t = "absolute"
					size:t = "pw, ph"
					display:t = "show"
					overflow-y:t = "auto"
					scroll-align:t = "left"
					auto-scroll:t = "yes"
					css-hier-invalidate:t = "yes"
					historyLog {
						id:t = "history_log"
						width:t = "pw"
						punctuation-exception:t = '-.,'"():/\@[]=^+'
						text:t = ""
					}
				}
				chatPlaceSpectator {
					id:t = "chat_container"
					position:t = "absolute"
					size:t = "pw, ph"
					display:t = "hide"
				}
				tdiv {
					id:t = "order_status"
					position:t = "absolute"
					size:t = "pw, ph"
				}
			}
		}
	}
