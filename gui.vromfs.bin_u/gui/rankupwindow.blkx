root {
	blur {

	}
	blur_foreground {

	}
	frame {
		width:t = "4@slot_width+2@slot_interval + 20"
		max-height:t = "ph"
		pos:t = "0.5sw-0.5w,0.5sh-0.5h"
		position:t = "absolute"
		flow:t = "vertical"
		class:t = "wndNav"
		frame_header {
			textarea {
				id:t = "rankup_country_title"
				text:t = "#new_rank_country"
			}
			Button_close {
				id:t = "btn_back"
			}
		}
		img {
			id:t = "background_country"
			width:t = "pw"
			height:t = "0.5w"
			max-width:t = "800"
			max-height:t = "400"
			pos:t = "0.5pw-0.5w, 0"
			position:t = "relative"
			background-color:t = "@white"
			background-image:t = ""
			overflow:t = "hidden"
		}
		tdiv {
			width:t = "pw"
			flow:t = "vertical"
			tdiv {
				pos:t = "50%pw-50%w, 0.015@scrn_tgt"
				position:t = "relative"
				cardImg {
					id:t = "country_icon"
					background-image:t = "#ui/gameuiskin#country_0.svg"
				}
				textareaNoTab {
					id:t = "player_rank"
					pos:t = "0.005@sf, 0"
					position:t = "relative"
					text-align:t = "left"
					max-width:t = "p.p.w-0.05@scrn_tgt"
					padding-top:t = "-0.01@sf"
					caption:t = "yes"
				}
			}
			textarea {
				id:t = "availableNewAirText"
				max-width:t = "pw"
				pos:t = "0.5pw-0.5w"
				position:t = "relative"
				padding:t = "0, 0.005@sf"
				text-align:t = "center"
				text:t = ""
			}
			tdiv {
				id:t = "rankup_aircraft_holder"
				width:t = "pw"
				max-height:t = "4(1@slot_height+2@slot_vert_pad)"
				decreased-height:t = "3(1@slot_height+2@slot_vert_pad)"
				overflow-y:t = "auto"
				scrollbarShortcuts:t = "yes"
				rankUpList {
					id:t = "rankup_aircraft_table"
					max-width:t = "pw"
					pos:t = "0.5pw-0.5w"
					position:t = "relative"
					flow:t = "h-flow"
					holdTooltipChildren:t = "yes"
				}
			}
			textAreaCentered {
				id:t = "next_award"
				width:t = "pw"
				pos:t = "50%pw-50%w, 0.01@sf"
				position:t = "relative"
				text:t = ""
			}
		}
		navBar {
			navMiddle {
				Button_text {
					id:t = "btn_close"
					text:t = "#mainmenu/btnOk"
					btnName:t = "A"
					_on_click:t = "goBack"
					ButtonImg {

					}
				}
			}
		}
	}
}