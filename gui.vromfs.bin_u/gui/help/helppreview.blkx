tdiv {
	size:t = "pw, ph"
	flow:t = "vertical"
	HorizontalListBox {
		id:t = "tabs_list"
		height:t = "1@frameHeaderHeight"
		class:t = "header"
		normalFont:t = "yes"
		withImages:t = "no"
		activeAccesskeys:t = "RS"
		on_select:t = "onHelpSheetChange"
	}
	HorizontalListBox {
		id:t = "sub_tabs_list"
		normalFont:t = "yes"
		withImages:t = "no"
		navigatorShortcuts:t = "yes"
		on_select:t = "onHelpSubSheetChange"
	}
	tdiv {
		id:t = "help_sheet"
		size:t = "pw, fh"
	}
	tdiv {
		id:t = "link_lines_block"
		size:t = "pw, ph"
		position:t = "root"
		total-input-transparent:t = "yes"
		display:t = "hide"
	}
}