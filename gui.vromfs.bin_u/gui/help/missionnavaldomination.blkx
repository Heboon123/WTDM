			pos:t = "60/1760pw, 115/900ph"
			position:t = "absolute"
			width:t = "395/1760pw"
			flow:t = "vertical"
			controlsHelpTextArea {
				width:t = "pw"
				text:t = "#hints/tutorial_newbie/naval/number_of_capture_zones"
			}
			controlsHelpTextArea {
				width:t = "pw"
				text:t = "#hints/tutorial_newbie/naval/captured_by_enemy"
				helpLine {
					size:t = "383/1760p.p.p.w, 1*@sf/@pf"
					pos:t = "pw, ph/2"
					position:t = "absolute"
				}
				helpLine {
					size:t = "1*@sf/@pf, ph"
					pos:t = "pw + 382/1760p.p.p.w, ph/2-h"
