tdiv {
	id:t = "help_background_image"
	size:t = "pw, ph"
	max-height:t = "pw/1760*900"
	background-repeat:t = "expand"
	background-color:t = "@white"
	background-image:t = "@!ui/images/missions_tutorial/tutorial_slide_naval_capture?P1"
	tdiv {
		size:t = "pw, ph"
		pos:t = "0.5pw-0.5w, 0.5ph-0.5h"
		position:t = "relative"
