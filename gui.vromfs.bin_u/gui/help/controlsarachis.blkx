tdiv {
	id:t = "help_background_image"
	size:t = "pw, ph"
	max-height:t = "pw/1760*900"
	background-repeat:t = "expand"
	background-color:t = "@white"
	tdiv {
		size:t = "pw, ph"
		pos:t = "0.5pw-0.5w, 0.5ph-0.5h"
		position:t = "relative"
		controlsHelpFrame {
			id:t = "worm_label"
			pos:t = "255/1760pw, 50/900ph"
			position:t = "absolute"
			width:t = "550/1760pw"
			padding-top:t = "0.007@sf"
			padding-bottom:t = "0.012@sf"
			flow:t = "vertical"
			titleTextArea {
				text:t = "#mainmenu/important_notice"
				padding-bottom:t = "0.005@sf"
			}
			textareaNoTab {
				text:t = "#controls/help/arachis/worm"
			}
		}
		tdiv {
			id:t = "worm_point"
			pos:t = "183/1760pw-w/2, 132/900ph-h/2"
			position:t = "absolute"
			size:t = "1@dp, 1@dp"
		}
		tdiv {
			id:t = "sonicwave_point"
			pos:t = "200/1760pw, 810/900ph-h/2"
			position:t = "absolute"
			size:t = "80@dp, 80@dp"
			background-color:t = "@black"
			border:t = "yes"
			border-color:t = "@commonTextColor"
			padding:t = "3@dp"
			img {
				size:t = "pw, ph"
				position:t = "absolute"
				background-image:t = "#ui/gameuiskin#damage_small"
			}
			img {
				size:t = "pw, ph"
				position:t = "absolute"
				background-image:t = "#ui/gameuiskin#armor_big"
			}
			img {
				size:t = "pw, ph"
