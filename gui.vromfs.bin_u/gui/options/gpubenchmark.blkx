root {
	blur {

	}
	blur_foreground {

	}
	frame {
		pos:t = "50%(pw-w), 50%(ph-h)"
		position:t = "absolute"
		class:t = "wndNav"
		size:t = "0.8@sf, 0.35@sf"
		flow:t = "vertical"
		frame_header {
			activeText {
				text:t = "#gpuBenchmark/title"
				caption:t = "yes"
			}
			Button_close {

			}
		}
		tdiv {
			size:t = "pw, ph"
			tdiv {
				id:t = "benchmarkStart"
				position:t = "absolute"
				size:t = "pw, ph"
				textareaNoTab {
					position:t = "relative"
					pos:t = "50%(pw-w), 50%(ph-h)"
					text:t = "#gpuBenchmark/desc"
				}
			}
			tdiv {
				id:t = "waitAnimation"
				position:t = "absolute"
				pos:t = "50%(pw-w), 50%(ph-h)"
				display:t = "hide"
				animated_wait_icon {
					background-rotation:t = "0"
					margin-right:t = "1@blockInterval"
				}
				textareaNoTab {
					id:t = "progressText"
					position:t = "relative"
					pos:t = "0, 50%(ph-h)"
				}
				timer {
					id:t = "progress_timer"
					timer_handler_func:t = "onUpdate"
					timer_interval_msec:t = "1000"
				}
			}
			tdiv {
				id:t = "presetSelection"
				display:t = "hide"
				flow:t = "vertical"
				width:t = "pw"
				pos:t = "50%(pw-w), 0"
				position:t = "relative"
				margin-top:t = "0.01@scrn_tgt"
				margin-bottom:t = "0.005@scrn_tgt"
				tdiv {
					width:t = "pw"
					margin-bottom:t = "0.005@scrn_tgt"
					margin-left:t = "0.02@scrn_tgt"
					textareaNoTab {
						width:t = "pw"
						text:t = "#gpuBenchmark/resultsTitle"
					}
				}
				tdiv {
					width:t = "pw"
					margin-top:t = "0.02@scrn_tgt"
					textarea {
						text:t = "#gpuBenchmark/resultsEffect"
					}
					textarea {
						position:t = "absolute"
						left:t = "50%pw+3@blockInterval"
						text:t = "#gpuBenchmark/resultsPresetName"
					}
				}
				rowSeparator {

				}
				tdiv {
					id:t = "resultsList"
					width:t = "pw"
					padding:t = "0.01@scrn_tgt, 0.01@scrn_tgt"
					flow:t = "vertical"
				}
			}
		}
		navBar {
			navLeft {
				Button_text {
					text:t = "#msgbox/btn_cancel"
					on_click:t = "goBack"
					btnName:t = "B"
					ButtonImg {

					}
				}
			}
			navRight {
				Button_text {
					id:t = "btnStart"
					text:t = "#gpuBenchmark/btnStart"
					btnName:t = "EnterX"
					on_click:t = "onBenchmarkStart"
					ButtonImg {

					}
				}
				Button_text {
					id:t = "btnApply"
					text:t = "#mainmenu/btnApply"
					btnName:t = "EnterX"
					on_click:t = "onPresetApply"
					ButtonImg {

					}
				}
			}
		}
	}
}