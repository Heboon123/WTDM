root {
	type:t = "big"
	blur {

	}
	blur_foreground {

	}
	frame {
		size:t = "0.8*@scrn_tgt, 0.5@scrn_tgt"
		flow:t = "vertical"
		class:t = "wndNav"
		isCenteredUnderLogo:t = "yes"
		frame_header {
			activeText {
				id:t = "clan_title_table"
				caption:t = "yes"
				text:t = "#clan/candidatesList"
			}
			Button_close {
				id:t = "btn_back"
			}
		}
		table {
			id:t = "candidatesList"
			width:t = "pw"
			height:t = "fh"
			padding-bottom:t = "0.02*@scrn_tgt"
			class:t = "lbTable"
			selfFocusBorder:t = "yes"
			behavior:t = "PosNavigator"
			text-valign:t = "center"
			text-halign:t = "center"
			navigatorShortcuts:t = "yes"
			on_click:t = "onSelect"
			_on_r_click:t = "onUserRClick"
			on_dbl_click:t = "onUserCard"
		}
		navBar {
			navLeft {
				Button_text {
					id:t = "btn_reject"
					text:t = "#clan/requestReject"
					display:t = "hide"
					_on_click:t = "onRequestReject"
					ButtonImg {

					}
				}
				Button_text {
					id:t = "btn_user_options"
					text:t = "#mainmenu/btnUserAction"
					btnName:t = "X"
					display:t = "hide"
					_on_click:t = "onUserAction"
					ButtonImg {

					}
				}
			}
			navMiddle {
				id:t = "paginator_place"
			}
			navRight {
				Button_text {
					id:t = "btn_removeBlacklist"
					text:t = "#msgbox/btn_delete"
					_on_click:t = "onDeleteFromBlacklist"
					display:t = "hide"
					btnName:t = "A"
					ButtonImg {

					}
				}
				Button_text {
					id:t = "btn_approve"
					visualStyle:t = "secondary"
					text:t = "#clan/requestApprove"
					display:t = "hide"
					_on_click:t = "onRequestApprove"
					ButtonImg {

					}
					buttonWink {

					}
				}
			}
		}
	}
}