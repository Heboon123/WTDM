id:t = "clan_container"
flow:t = "h-flow"
tdiv {
	width:t = "pw"
	flow:t = "h-flow"
	tdiv {
		width:t = "pw"
		tdiv {
			width:t = "0.55pw"
			padding:t = "1@blockInterval"
			margin-right:t = "1@blockInterval"
			background-color:t = "@separatorBlockColor"
			img {
				id:t = "clan-icon"
				width:t = "0.26pw"
				height:t = "w"
				max-height:t = "0.14@sf"
				max-width:t = "0.14@sf"
				background-image:t = "#ui/images/avatars/cardicon_default"
				display:t = "hide"
			}
			tdiv {
				width:t = "fw"
				flow:t = "h-flow"
				tdiv {
					padding:t = "1@blockInterval, 0"
					width:t = "pw"
					flow:t = "vertical"
					tdiv {
						width:t = "pw"
						tdiv {
							id:t = "nest_lock_clan_req"
							pos:t = "0, ph/2-h/2"
							position:t = "relative"
							clan_locked:t = "no"
							Button_text {
								id:t = "btn_lock_clan_req"
								class:t = "image"
								tooltip:t = "#clan/open_close_new_req"
								display:t = "hide"
								on_click:t = "onLockNewReqests"
								btnName:t = "L3"
								img {

								}
								ButtonImg {

								}
							}
							img {
								id:t = "img_lock_clan_req"
								size:t = "@unlockStageIconSize, @unlockStageIconSize"
								background-svg-size:t = "@unlockStageIconSize, @unlockStageIconSize"
								background-color:t = "@white"
								display:t = "hide"
								tooltip:t = "#clan/closed_for_new_req"
							}
						}
						textareaNoTab {
							id:t = "clan-title"
							pos:t = "0, ph/2-h/2"
							position:t = "relative"
							overlayTextColor:t = "active"
							mediumFont:t = "yes"
							text:t = ""
							hideEmptyText:t = "yes"
							overflow-y:t = "none"
							pare-text:t = "yes"
							max-width:t = "pw"
							max-height:t = "0.2@sf"
						}
					}
					textareaNoTab {
						id:t = "clan-memberCount-date"
						text:t = ""
					}
					textareaNoTab {
						id:t = "clan-prevChanges"
						smallFont:t = "yes"
						hideEmptyText:t = "yes"
						text:t = ""
					}
					textareaNoTab {
						id:t = "clan-region"
						smallFont:t = "yes"
						width:t = "pw"
						max-height:t = "0.05@sf"
						overflow-y:t = "auto"
						hideEmptyText:t = "yes"
						text:t = ""
					}
					textareaNoTab {
						id:t = "clan-membershipReq"
						smallFont:t = "yes"
						hideEmptyText:t = "yes"
						text:t = ""
					}
				}
			}
		}
		tdiv {
			id:t = "clan_main_stats"
			width:t = "fw"
			flow:t = "h-flow"
			display:t = "hide"
			textareaNoTab {
				id:t = "update_stats_info_text"
				max-width:t = "pw"
				smallFont:t = "yes"
				font-bold:t = "@fontNormal"
				overlayTextColor:t = "faded"
				text:t = ""
			}
			table {
				id:t = "clan_stats_table"
				width:t = "pw"
				class:t = "schemeTable"
			}
		}
	}
	textareaNoTab {
		id:t = "clan-motto"
		width:t = "pw"
		position:t = "relative"
		pos:t = "0, 1@buttonMargin"
		hideEmptyText:t = "yes"
		text-align:t = "center"
		overlayTextColor:t = "userlog"
		text:t = ""
		bgcolor:t = "@evenTrColor"
	}
	textarea {
		id:t = "clan-about"
		width:t = "pw"
		padding:t = "0, 0.01@sf"
		max-height:t = "0.1@sf"
		text-align:t = "left"
		hideEmptyText:t = "yes"
		smallFont:t = "yes"
		overflow-y:t = "auto"
		text:t = ""
	}
}
tdiv {
	size:t = "pw, @clanMedalSizeMin + 2@buttonMargin"
	tdiv {
		position:t = "absolute"
		div {
			total-input-transparent:t = "yes"
			tooltip:t = "#clan/squadron_rating"
			cardImg {
				id:t = "clan_elo_icon"
				margin:t = "0.25@sIco"
				background-image:t = "#ui/gameuiskin#lb_elo_rating.svg"
				type:t = "medium"
			}
			textarea {
				id:t = "clan_elo_value"
				margin:t = "0.25@sIco"
				text:t = ""
				caption:t = "yes"
				overlayTextColor:t = "userlog"
				removeParagraphIndent:t = "yes"
				input-transparent:t = "yes"
			}
		}
		div {
			total-input-transparent:t = "yes"
			tooltip:t = "#clan/squadron_activity"
			cardImg {
				id:t = "clan_activity_icon"
				margin:t = "0.25@sIco"
				background-image:t = "#ui/gameuiskin#lb_activity.svg"
				type:t = "medium"
				display:t = "hide"
			}
			textarea {
				id:t = "clan_activity_value"
				margin:t = "0.25@sIco"
				text:t = ""
				caption:t = "yes"
				overlayTextColor:t = "userlog"
				removeParagraphIndent:t = "yes"
				display:t = "hide"
			}
		}
	}
	HorizontalListBox {
		id:t = "modes_list"
		type:t = "transparent"
		position:t = "absolute"
		pos:t = "0, ph - h"
		on_select:t = "cp_onStatsModeChange"
		navigatorShortcuts:t = "yes"
		move-only-hover:t = "yes"
	}
	div {
		id:t = "clan_awards_container"
		position:t = "relative"
		pos:t = "pw-w+1@blockInterval, 0"
		padding:t = "0, 1@buttonMargin"
		max-width:t = "6@clanMedalSizeMin"
		overflow:t = "hidden"
		total-input-transparent:t = "yes"
		behaviour:t = "button"
		on_click:t = "onClanSeasonRewardLog"
		skip-navigation:t = "yes"
		display:t = "hide"
	}
}
tdiv {
	id:t = "clan_members_list_nest"
	size:t = "pw, fh"
	overflow-y:t = "auto"
	table {
		id:t = "clan_members_list"
		width:t = "pw"
		padding-top:t = "1@blockInterval"
		class:t = "lbTable"
		useNavigatorOrInteractiveCells:t = "yes"
		text-valign:t = "center"
		text-halign:t = "center"
		navigatorShortcuts:t = "yes"
		selfFocusBorder:t = "yes"
		on_select:t = "onSelectUser"
		on_r_click:t = "onUserRClick"
		on_dbl_click:t = "onUserCard"
		on_set_focus:t = "onMembersListFocus"
	}
}
tdiv {
	id:t = "lb_table_nest"
	size:t = "pw, fh"
	padding-top:t = "1@blockInterval"
}
animated_wait_icon {
	id:t = "clan_loading"
	pos:t = "0.5pw-0.5w, 0.5ph-0.5h"
	position:t = "absolute"
	background-rotation:t = "0"
}
textareaNoTab {
	id:t = "season_over_notice"
	position:t = "relative"
	padding:t = "0, 1@blockInterval"
	pos:t = "50%pw-50%w, 0"
	smallFont:t = "yes"
	text:t = "#worldwar/seasonIsOverNotice"
	display:t = "hide"
}
inlineHolder {
	id:t = "clan_actions"
	position:t = "relative"
	width:t = "pw"
	flow:t = "h-flow"
	flow-align:t = "center"
	css-hier-invalidate:t = "yes"
	display:t = "hide"
	enable:t = "no"
	bgcolor:t = "@evenTrColor"
	re-type:t = "9rect"
	foreground-color:t = "@frameBorderColor"
	foreground-position:t = "0, 0, 0, h-1@dp"
	Button_text {
		id:t = "btn_showRequests"
		text:t = "#clan/btnShowRequests"
		wink:t = "no"
		display:t = "hide"
		on_click:t = "onShowRequests"
		visualStyle:t = "secondary"
		buttonWink {

		}
	}
	Button_text {
		id:t = "btn_clanActivity"
		text:t = "#clan/activity"
		display:t = "hide"
		on_click:t = "onClanAverageActivity"
		visualStyle:t = "noFrame"
		showConsoleImage:t = "no"
	}
	Button_text {
		id:t = "btn_clanVehicles"
		text:t = "#clan/vehicles"
		display:t = "hide"
		on_click:t = "onClanVehicles"
		visualStyle:t = "noFrame"
		showConsoleImage:t = "no"
	}
	Button_text {
		id:t = "btn_clanSquads"
		text:t = "#clan/clan_squads"
		display:t = "hide"
		on_click:t = "onClanSquads"
		visualStyle:t = "noFrame"
		showConsoleImage:t = "no"
	}
	Button_text {
		id:t = "btn_log"
		text:t = "#clan/clan_log"
		display:t = "hide"
		on_click:t = "onClanLog"
		visualStyle:t = "noFrame"
		showConsoleImage:t = "no"
	}
}
