tdiv {
	pos:t = "35%@rw, 15%@rh"
	position:t = "relative"
	flow:t = "vertical"
	controlsHelpTextArea {
		id:t = "hint_clan_elo"
		width:t = "@shopWidthMax"
		margin-left:t = "0.5*@shopWidthMax"
		text:t = "#clan/squadron_rating"
	}
	controlsHelpTextArea {
		id:t = "hint_clan_activity"
		width:t = "@shopWidthMax"
		margin-top:t = "@helpInterval"
		text:t = "#clan/squadron_activity"
	}
}
tdiv {
	pos:t = "50%@rw, 30%@rh"
	position:t = "absolute"
	flow:t = "vertical"
	controlsHelpTextArea {
		id:t = "hint_air_kills"
		width:t = "@shopWidthMax"
		text:t = "#multiplayer/air_kills"
	}
	controlsHelpTextArea {
		id:t = "hint_ground_kills"
		width:t = "@shopWidthMax"
		margin-left:t = "0.5*0.22@scrn_tgt"
		margin-top:t = "@helpInterval"
		text:t = "#multiplayer/ground_kills"
	}
	controlsHelpTextArea {
		id:t = "hint_death"
		width:t = "@shopWidthMax"
		margin-left:t = "1*0.22@scrn_tgt"
		margin-top:t = "@helpInterval"
		text:t = "#multiplayer/deaths"
	}
	controlsHelpTextArea {
		id:t = "hint_time_pvp_played"
		width:t = "@shopWidthMax"
		margin-left:t = "1.5*0.22@scrn_tgt"
		margin-top:t = "@helpInterval"
		text:t = "#multiplayer/time_pvp_played"
	}
}
tdiv {
	pos:t = "50%@rw, 70%@rh"
	position:t = "absolute"
	controlsHelpTextArea {
		id:t = "hint_membership_activity"
		width:t = "@shopWidthMax"
		text:t = "#clan/membership_activity"
	}
}