frame {
	id:t = "queue_table"
	position:t = "relative"
	text-halign:t = "center"
	tdiv {
		id:t = "ia_tooltip"
		width:t = "((1@slotbarCountries $max 6)+1) * 1@queueTableColumnWidth"
		pos:t = "0.5pw-0.5w, 0"
		position:t = "relative"
		tdiv {
			width:t = "pw"
			pos:t = "pw-w, 0"
			position:t = "relative"
			padding:t = "2"
			tdiv {
				width:t = "pw"
				flow:t = "vertical"
				HorizontalListBox {
					id:t = "ia_table_clusters_list"
					width:t = "pw"
					min-height:t = "1@buttonHeight"
					smallFont:t = "yes"
					class:t = "tabs"
					flow:t = "h-flow"
					input-transparent:t = "yes"
					navigatorShortcuts:t = "yes"
					on_select:t = "onClustersTabChange"
				}
				table {
					id:t = "ia_tooltip_table"
					width:t = "pw"
					pos:t = "50%pw-50%w"
					position:t = "relative"
					display:t = "hide"
				}
				div {
					id:t = "queue_box_container"
					width:t = "pw"
				}
				textAreaCentered {
					id:t = "unstable_cluster_warning"
					pos:t = "50%pw-50%w, 6@sf/@pf"
					position:t = "relative"
					max-width:t = "pw"
					text:t = "#multiplayer/cluster_connection_unstable"
					text-halign:t = "center"
					overlayTextColor:t = "bad"
					smallFont:t = "yes"
					display:t = "hide"
					wink:t = "no"
				}
				tdiv {
					id:t = "waiting_time"
					width:t = "pw"
					padding:t = "10*@sf/@pf_outdated"
					flow:t = "vertical"
					tdiv {
						id:t = "available_countries"
						pos:t = "50%pw-50%w, 0"
						position:t = "relative"
						padding-bottom:t = "0.01@scrn_tgt"
						flow:t = "vertical"
						activeText {
							id:t = "countries_header"
							pos:t = "50%pw-50%w, 0"
							position:t = "relative"
							text-align:t = "center"
							smallFont:t = "yes"
							text:t = ""
						}
						tdiv {
							id:t = "countries_icons"
							pos:t = "50%pw-50%w, 0"
							position:t = "relative"
						}
					}
					tdiv {
						pos:t = "pw/2-w/2, 0"
						position:t = "relative"
						size:t = "180@sf/@pf, 180@sf/@pf"
						tdiv {
							id:t = "queue_wait_icon_block"
							position:t = "absolute"
							size:t = "pw, ph"
						}
						centerBlock {
							position:t = "absolute"
							pos:t = "50%pw-50%w, 50%ph-50%h"
							flow:t = "vertical"
							activeText {
								text:t = "#multiplayer/currentWaitTime"
								smallFont:t = "yes"
								style:t = "color:@commonMenuButtonColor;"
							}
							textAreaCentered {
								id:t = "msgText"
								text-halign:t = "center"
								class:t = "active"
								text:t = "00:00:00"
								titleFont:t = "yes"
								pos:t = "50%pw-50%w,0"
								position:t = "relative"
							}
						}
					}
					activeText {
						id:t = "inQueue-topmenu-text"
						display:t = "hide"
						wink:t = "no"
						text:t = "#yn1/waiting_for_game_query"
						style:t = "color:@userlogColoredText;"
						pos:t = "50%pw-50%w, 10*@sf/@pf_outdated"
						position:t = "relative"
						hideEmptyText:t = "yes"
						text:t = ""
					}
					activeText {
						id:t = "queue_players_total"
						text:t = ""
						style:t = "color:@commonMenuButtonColor;"
						pos:t = "50%pw-50%w, 10*@sf/@pf_outdated"
						position:t = "relative"
						smallFont:t = "yes"
						hideEmptyText:t = "yes"
						text:t = ""
					}
				}
				timer {
					id:t = "queue_update_timer"
					timer_handler_func:t = "onUpdate"
					timer_interval_msec:t = "1000"
				}
				tdiv {
					margin-bottom:t = "14@sf/@pf"
					flow:t = "horizontal"
					pos:t = "50%pw-50%w, 0"
					position:t = "relative"
					activeText {
						id:t = "battle_rating_label"
						position:t = "relative"
						pos:t = "0, 50%ph-50%h"
						smallFont:t = "yes"
						commonButtonColor:t = "yes"
						hideEmptyText:t = "yes"
					}
					activeText {
						id:t = "battle_rating"
						position:t = "relative"
						pos:t = "1@blockInterval, 50%ph-50%h-2@dp"
						normalBoldFont:t = "yes"
						overlayTextColor:t = "userlog"
						hideEmptyText:t = "yes"
					}
				}
				tdiv {
					flow:t = "horizontal"
					width:t = "pw"
					height:t = "0.02@sf"
					tdiv {
						size:t = "0.35pw, 3@sf/@pf"
						bgcolor:t = "#0E1114"
						position:t = "absolute"
						top:t = "0.5ph-0.5h"
					}
					activeText {
						text:t = "#multiplayer/hint"
						commonButtonColor:t = "yes"
						pos:t = "0.5pw-0.5w, 0.5ph-0.5h"
						position:t = "absolute"
						smallFont:t = "yes"
					}
					tdiv {
						size:t = "0.35pw, 3@sf/@pf"
						bgcolor:t = "#0E1114"
						position:t = "absolute"
						top:t = "0.5ph-0.5h"
						right:t = "0"
					}
				}
				loadingTip {
					id:t = "queue_tip"
					width:t = "pw"
					padding:t = "0, 0.5*@scrn_tgt/100.0"
					behaviour:t = "bhvLoadingTip"
					loadingHint {
						id:t = "tip_hint"
						width:t = "pw"
						commonTextColor:t = "yes"
						smallFont:t = "yes"
						behaviour:t = "bhvHint"
						isWrapInRowAllowed:t = "yes"
					}
				}
				tdiv {
					id:t = "crossplay_tip"
					display:t = "hide"
					flow:t = "vertical"
					width:t = "pw"
					tdiv {
						width:t = "pw"
						height:t = "0.02@sf"
						flow:t = "horizontal"
						tdiv {
							size:t = "0.2pw, 3@sf/@pf"
							bgcolor:t = "#0E1114"
							position:t = "absolute"
							top:t = "0.5ph-0.5h"
						}
						activeText {
							text:t = "#options/ps4_crossplay"
							pos:t = "0.5pw-0.5w, 0.5ph-0.5h"
							position:t = "absolute"
							smallFont:t = "yes"
							overlayTextColor:t = "warning"
						}
						tdiv {
							size:t = "0.2pw, 3@sf/@pf"
							bgcolor:t = "#0E1114"
							position:t = "absolute"
							top:t = "0.5ph-0.5h"
							right:t = "0"
						}
					}
					textareaNoTab {
						width:t = "pw"
						smallFont:t = "yes"
						overflow-y:t = "auto"
						padding:t = "0.01@scrn_tgt, 0.005@scrn_tgt"
						text:t = "#queue/crossPlay"
						overlayTextColor:t = "warning"
					}
				}
			}
		}
	}
	div {
		id:t = "queue_table_timer"
		behavior:t = "Timer"
		timer_handler_func:t = "onUpdate"
		timer_interval_msec:t = "1000"
	}
}