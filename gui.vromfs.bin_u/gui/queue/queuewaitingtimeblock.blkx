tdiv {
	id:t = "waiting_time"
	width:t = "pw"
	padding:t = "10*@sf/@pf_outdated"
	flow:t = "vertical"
	tdiv {
		id:t = "available_countries"
		pos:t = "50%pw-50%w, 0"
		position:t = "relative"
		padding-bottom:t = "0.01@scrn_tgt"
		flow:t = "vertical"
		activeText {
			id:t = "countries_header"
			pos:t = "50%pw-50%w, 0"
			position:t = "relative"
			text-align:t = "center"
			smallFont:t = "yes"
			text:t = ""
		}
		tdiv {
			id:t = "countries_icons"
			pos:t = "50%pw-50%w, 0"
			position:t = "relative"
		}
	}
	tdiv {
		pos:t = "pw/2-w/2, 0"
		position:t = "relative"
		size:t = "180@sf/@pf, 180@sf/@pf"
		tdiv {
			id:t = "queue_wait_icon_block"
			position:t = "absolute"
			size:t = "pw, ph"
		}
		centerBlock {
			position:t = "absolute"
			pos:t = "50%pw-50%w, 50%ph-50%h"
			flow:t = "vertical"
			activeText {
				text:t = "#multiplayer/currentWaitTime"
				smallFont:t = "yes"
				style:t = "color:@commonMenuButtonColor;"
			}
			textAreaCentered {
				id:t = "msgText"
				text-halign:t = "center"
				class:t = "active"
				text:t = "00:00:00"
				titleFont:t = "yes"
				pos:t = "50%pw-50%w,0"
				position:t = "relative"
			}
		}
	}
	activeText {
		id:t = "inQueue-topmenu-text"
		display:t = "hide"
		wink:t = "no"
		text:t = "#yn1/waiting_for_game_query"
		style:t = "color:@userlogColoredText;"
		pos:t = "50%pw-50%w, 10*@sf/@pf_outdated"
		position:t = "relative"
		hideEmptyText:t = "yes"
		text:t = ""
	}
	activeText {
		id:t = "queue_players_total"
		text:t = ""
		style:t = "color:@commonMenuButtonColor;"
		pos:t = "50%pw-50%w, 10*@sf/@pf_outdated"
		position:t = "relative"
		smallFont:t = "yes"
		hideEmptyText:t = "yes"
		text:t = ""
	}
}
timer {
	id:t = "queue_update_timer"
	timer_handler_func:t = "onUpdate"
	timer_interval_msec:t = "1000"
}