div {
	id:t = "queue_box"
	size:t = "pw, ph"
	pos:t = "0,0"
	position:t = "relative"
	padding:t = "4*@sf/@pf_outdated"
	flow:t = "vertical"
	overflow-y:t = "auto"
	behavior:t = "Timer"
	timer_interval_msec:t = "1000"
	tdiv {
		size:t = "pw, fh"
		flow:t = "vertical"
		overflow-y:t = "auto"
		frameBlock_dark {
			id:t = "1_block"
			pos:t = "0,0"
			position:t = "relative"
			width:t = "pw"
			padding:t = "3*@sf/@pf_outdated"
			flow:t = "vertical"
			display:t = "hide"
			css-hier-invalidate:t = "yes"
			bgTeamColor:t = "blue"
			activeText {
				id:t = "team_name"
				text:t = "#events/teamA"
				position:t = "relative"
				pos:t = "0, 0"
			}
			tdiv {
				id:t = "countries"
				width:t = "pw"
				position:t = "relative"
				pos:t = "0, 0"
			}
			tdiv {
				margin-left:t = "0.01*@sf"
				activeText {
					id:t = "players_count"
					text:t = ""
				}
			}
			table {
				id:t = "table_queue_stat"
				width:t = "pw"
				smallFont:t = "yes"
			}
		}
		frameBlock_dark {
			id:t = "2_block"
			pos:t = "0, @blockInterval"
			position:t = "relative"
			width:t = "pw"
			padding:t = "3*@sf/@pf_outdated"
			flow:t = "vertical"
			display:t = "hide"
			css-hier-invalidate:t = "yes"
			bgTeamColor:t = "red"
			activeText {
				id:t = "team_name"
				text:t = "#events/teamB"
				position:t = "relative"
				pos:t = "0, 0"
			}
			tdiv {
				id:t = "countries"
				width:t = "pw"
				position:t = "relative"
				pos:t = "0, 0"
			}
			tdiv {
				margin-left:t = "0.01*@sf"
				activeText {
					id:t = "players_count"
					text:t = ""
				}
			}
			table {
				id:t = "table_queue_stat"
				width:t = "pw"
				smallFont:t = "yes"
			}
		}
	}
	tdiv {
		id:t = "wait_time_block"
		padding:t = "10*@sf/@pf_outdated, 5*@sf/@pf_outdated"
		size:t = "pw, 80*@sf/@pf_outdated"
		tdiv {
			pos:t = "50%pw-50%w, 50%ph-50%h"
			position:t = "absolute"
			textAreaCentered {
				id:t = "waitText"
				pos:t = "0, 50%ph-50%h"
				position:t = "relative"
				max-width:t = "180*@sf/@pf_outdated"
				text-align:t = "center"
				class:t = "active"
				text:t = ""
			}
			animated_wait_icon {
				pos:t = "0, 50%ph-50%h"
				position:t = "relative"
				background-rotation:t = "0"
			}
		}
	}
}