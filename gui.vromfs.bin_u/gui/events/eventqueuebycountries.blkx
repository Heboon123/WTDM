div {
	id:t = "queue_box"
	size:t = "pw, ph"
	position:t = "relative"
	padding:t = "4*@sf/@pf_outdated"
	flow:t = "vertical"
	overflow-y:t = "auto"
	behavior:t = "Timer"
	timer_interval_msec:t = "1000"
	HorizontalListBox {
		id:t = "clusters_list"
		width:t = "pw"
		min-height:t = "1@buttonHeight"
		smallFont:t = "yes"
		class:t = "tabs"
		flow:t = "h-flow"
		on_select:t = "onClusterChange"
	}
	table {
		id:t = "stats_table"
		width:t = "pw"
		position:t = "relative"
		text-halign:t = "center"
	}
	tdiv {
		id:t = "wait_time_block"
		padding:t = "10*@sf/@pf_outdated, 5*@sf/@pf_outdated"
		size:t = "pw, 80*@sf/@pf_outdated"
		tdiv {
			pos:t = "50%pw-50%w, 50%ph-50%h"
			position:t = "absolute"
			textAreaCentered {
				id:t = "waitText"
				pos:t = "0, 50%ph-50%h"
				position:t = "relative"
				max-width:t = "180*@sf/@pf_outdated"
				text-align:t = "center"
				class:t = "active"
				text:t = ""
			}
			animated_wait_icon {
				id:t = "queue_wait_icon"
				pos:t = "0, 50%ph-50%h"
				position:t = "relative"
				background-rotation:t = "0"
			}
		}
	}
	text {
		id:t = "countries_sets_header"
		pos:t = "50%pw - 50%w, 1@blockInterval"
		position:t = "relative"
		text:t = "#multiplayer/countriesSets"
		text-align:t = "center"
		display:t = "hide"
	}
	tdiv {
		id:t = "countries_sets"
		width:t = "pw"
		flow:t = "vertical"
	}
	text {
		id:t = "custom_mode_header"
		pos:t = "50%pw - 50%w, 1@blockInterval"
		position:t = "relative"
		text:t = "#events/playersRooms"
		tooltip:t = "#events/playersRooms/tooltip"
		text-align:t = "center"
		display:t = "hide"
	}
	tdiv {
		halign:t = "center"
		dummy {
			_on_click:t = "leaveQueue"
			btnName:t = "B"
		}
		SwitchBox {
			id:t = "custom_mode_checkbox"
			textChecked:t = "#options/enabled"
			textUnchecked:t = "#options/disabled"
			tooltip:t = "#events/playersRooms/tooltip"
			on_change_value:t = "onCustomModeCheckbox"
			selectable:t = "yes"
			display:t = "hide"
			enable:t = "no"
			SwitchSliderBg {
				SwitchSliderBgOn {

				}
				SwitchSlider {

				}
			}
		}
	}
}