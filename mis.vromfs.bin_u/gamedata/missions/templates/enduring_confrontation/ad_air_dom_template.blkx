			varCompareBool {
				var_value:t = "ad_search_in_progress"
				value:b = false
				comparasion_func:t = "equal"
			}
		}
		actions {
			comment {
				value:t = "no active searching procedure found and idx is valid"
			}
			varSetBool {
				value:b = true
				var:t = "ad_search_in_progress"
			}
			varModify {
				operator:t = "add"
				value:r = 1
				var:t = "ad_search_cur_idx"
			}
			comment {
				value:t = "creating zone name"
			}
			varSetString {
				value:t = "flt_area_"
