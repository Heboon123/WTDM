			}
			varAddString {
				value:t = "@ad_search_cur_idx"
				digits:i = 2
				var:t = "ad_search_zone_name"
			}
			comment {
				value:t = "creating var name, which contains army"
			}
			varSetString {
				value:t = "flt_area_"
				var:t = "ad_search_army_cur_var_name"
