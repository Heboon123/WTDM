	ad_search_zone_start {
		is_enabled:b = false
		comments:t = ""
		props {
			actionsType:t = "PERFORM_ONE_BY_ONE"
			conditionsType:t = "ALL"
			enableAfterComplete:b = false
		}
		events {
			periodicEvent {
				time:r = 1
			}
		}
		conditions {

		}
		actions {
			comment {
				value:t = "selecting random row for searching"
			}
			comment {
				value:t = "need to ignore border values (1 i 7) "
			}
			comment {
				value:t = "counting starting index for search"
			}
			varSetRandomInt {
				var:t = "ad_search_col_idx"
				from:i = 2
				to_not_including:i = 7
				var_to_not_including:t = "flt_zones_in_col"
			}
			comment {
				value:t = "counting maximum allowing idx for current col"
			}
			varSetInt {
				value:i = 0
				var:t = "ad_search_max_idx"
				input_var:t = "ad_search_col_idx"
			}
			varModify {
				operator:t = "mul"
				value:r = 7
				var:t = "ad_search_max_idx"
				use_variable:b = true
				var_value:t = "flt_zones_in_col"
			}
			varModify {
				operator:t = "add"
				value:r = -1
				var:t = "ad_search_max_idx"
			}
			comment {
				value:t = "finding minimal value"
			}
			varSetInt {
				value:i = 0
				var:t = "ad_search_min_idx"
				input_var:t = "ad_search_max_idx"
			}
			varModify {
				operator:t = "subAB"
				value:r = 7
				var:t = "ad_search_min_idx"
				use_variable:b = true
				var_value:t = "flt_zones_in_col"
			}
			varModify {
				operator:t = "add"
				value:r = 2
				var:t = "ad_search_min_idx"
			}
			comment {
				value:t = "choosing current idx"
			}
			varSetInt {
				value:i = 0
				var:t = "ad_search_cur_idx"
				input_var:t = "ad_search_min_idx"
			}
			comment {
				value:t = "starting rotation"
			}
			triggerEnable {
				target:t = "ad_search_zone_rotator"
			}
		}
		else_actions {

		}
	}
