clear {
	scatteringEffect:p2 = 0, 0
	moonSize:r = 0.27
	moonTex:t = "pluto"
	mudMul:r = 0.001
	clouds_weather_gen {
		layers[0].coverage:r = 0
		layers[1].coverage:r = 0
	}
	sky {
		mie_height:p2 = 1, 1
		mieG:p2 = 0.25, 0.4
		betaMScale:p2 = 4, 4
		sun_brightness:r = 1.971
		moon_brightness:r = 15
		planet_scale:r = 1
		mie_scale:r = 22
		mie2_thickness:r = 200
		mie2_scale:r = 2.7
		rayleigh_alt_scale:r = 1.75
		mie_absorption_scale:r = 12
		moon_color:p3 = 1, 0.6, 0.2
	}
	celestialBody {
		zenith:r = 65
		azimuth:r = 70
		tex:t = "moon"
		size:r = 0.1
		intensity:r = 1.45
		phase:r = 0.25
	}
	clouds_gen {
		humidity:p2 = 0, 0
		persistence:p2 = 0, 0
		asymmetry:p2 = 0, 0
	}
	strata_clouds {
		amount:p2 = 0, 0.2
		altitude:p2 = 10000, 12000
	}
}
