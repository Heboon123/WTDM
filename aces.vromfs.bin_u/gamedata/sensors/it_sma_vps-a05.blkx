type:t = "radar"
name:t = "SMA VPS-A05"
transivers {
	search {
		power:r = 100
		band:i = 4
		rcs:r = 1
		range:r = 16000
		rangeMax:r = 20000
		antenna {
			azimuth {
				angleHalfSens:r = 3.3
				sideLobesSensitivity:r = -30
			}
			elevation {
				angleHalfSens:r = 27
				sideLobesSensitivity:r = -35
			}
		}
	}
}
scanPatterns {
	searchNarrow {
		type:t = "cylinder"
		azimuthLimits:p2 = -180, 180
		elevationLimits:p2 = 0, 27
		yawStabLimit:r = 180
		pitchStabLimit:r = 10
		rollStabLimit:r = 10
		period:r = 1
		barHeight:r = 27
		barsCount:i = 1
		rowMajor:b = true
		centerElevation:r = 13.5
		preciseMinor:b = true
	}
	searchMedium {
		type:t = "cylinder"
		azimuthLimits:p2 = -180, 180
		elevationLimits:p2 = 0, 35
		yawStabLimit:r = 180
		pitchStabLimit:r = 10
		rollStabLimit:r = 10
		period:r = 2
		barHeight:r = 17.5
		barsCount:i = 2
		rowMajor:b = true
		centerElevation:r = 17.5
		preciseMinor:b = true
	}
}
scanPatternSets {
	search {
		scanPattern1:t = "searchNarrow"
		scanPattern2:t = "searchMedium"
	}
}
signals {
	search {
		groundClutter:b = false
		friendFoeId:b = true
		absDopplerSpeed:b = true
		distance {
			presents:b = true
			minValue:r = 500
			maxValue:r = 20000
			maxUnambiguousValue:r = 80000
		}
		dopplerSpeed {
			presents:b = true
			minValue:r = 15
			maxValue:r = 1000
			signalWidthMin:r = 5
			width:r = 0
		}
	}
}
