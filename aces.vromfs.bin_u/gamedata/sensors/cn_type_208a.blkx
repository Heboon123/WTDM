			angleHalfSens:r = 4
			sideLobesSensitivity:r = -27
		}
	}
}
illuminationTransmitter {
	power:r = 200
	antenna {
		angleHalfSens:r = 12
		sideLobesSensitivity:r = -27
	}
}
scanPatterns {
	searchWide {
		type:t = "pyramide"
