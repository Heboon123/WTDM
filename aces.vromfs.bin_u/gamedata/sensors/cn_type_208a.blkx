		barHeight:r = 3
		barsCount:i = 3
		rowMajor:b = true
	}
	searchNormal {
		type:t = "pyramide"
		azimuthLimits:p2 = -60, 60
		elevationLimits:p2 = -14, 26
		pitchStabLimit:r = 25
		rollStabLimit:r = 70
		period:r = 1.5
		width:r = 30
		barHeight:r = 3
		barsCount:i = 3
		rowMajor:b = true
	}
	searchNarrow {
		type:t = "pyramide"
		azimuthLimits:p2 = -60, 60
		elevationLimits:p2 = -14, 26
		pitchStabLimit:r = 25
		rollStabLimit:r = 70
		period:r = 0.1
		width:r = 5
		barHeight:r = 3
		barsCount:i = 1
		rowMajor:b = true
	}
	bvrLockSearch {
		type:t = "pyramide"
		azimuthLimits:p2 = -60, 60
		elevationLimits:p2 = -14, 26
		pitchStabLimit:r = 25
		rollStabLimit:r = 70
		period:r = 0.3
		width:r = 6
		barHeight:r = 3
		barsCount:i = 3
		rowMajor:b = true
		indicate:b = true
	}
	track {
		type:t = "no"
		azimuthLimits:p2 = -60, 60
		elevationLimits:p2 = -14, 26
	}
}
signals {
	search {
		dynamicRange:p2 = 40, 10
		groundClutter:b = true
		aircraftAsTarget:b = true
		friendFoeId:b = true
		distance {
			presents:b = true
			minValue:r = 300
			maxValue:r = 60000
			width:r = 300
		}
	}
	track {
		dynamicRange:p2 = 40, 12
		groundClutter:b = true
		aircraftAsTarget:b = true
		angularAccuracy:r = 0.06
		distanceAccuracy:r = 16
		track:b = true
		distance {
			presents:b = true
			minValue:r = 300
			maxValue:r = 60000
			width:r = 300
		}
	}
}
scanPatternSets {
	search {
		scanPattern1:t = "searchNormal"
		scanPattern2:t = "searchWide"
		scanPattern3:t = "searchNarrow"
	}
}
scopeRangeSets {
	search {
		range1:r = 30000
		range2:r = 60000
	}
}
