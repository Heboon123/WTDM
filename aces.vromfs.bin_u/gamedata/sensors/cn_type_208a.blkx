type:t = "radar"
name:t = "Type 208A"
showMissileLaunchZone:b = true
transivers {
	common {
		sideLobesAttenuation:r = -20
		power:r = 300
		band:i = 8
		rcs:r = 5
		range:r = 30000
		rangeMax:r = 60000
		timeGainControl:b = true
		multipathEffect:p4 = 0, 1, 60, 0
