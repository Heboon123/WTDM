					}
					setCenterElevation {
						source:t = "activeTargetOfInterest"
					}
					setDistGatePos {
						source:t = "activeTargetOfInterest"
						width:r = 0
					}
					setRelSpeedGatePos {
						source:t = "activeTargetOfInterest"
						width:r = 0
					}
				}
			}
