type:t = "radar"
canMoveCamera:b = true
name:t = "Auto tracker"
showOnHud:b = false
designationSourceType:i = 0
transivers {
	track {
		power:r = 60
		band:i = -1
		rcs:r = 3000
		range:r = 10000
		rangeMax:r = 15000
		visibilityType:t = "optic"
		illuminationMultMin:r = 0.01
		antenna {
			angleHalfSens:r = 0.05
			sideLobesSensitivity:r = -120
		}
	}
}
scanPatterns {
	lock {
		type:t = "cone"
		period:r = 0.2
		width:r = 0.01
		azimuthLimits:p2 = -180, 180
		elevationLimits:p2 = -180, 180
		yawStabLimit:r = 180
		pitchStabLimit:r = 180
		rollStabLimit:r = 180
		rollStabLimit:r = 180
	}
	losLock {
		type:t = "cone"
		period:r = 0.2
		width:r = 0.01
		azimuthLimits:p2 = -180, 180
		elevationLimits:p2 = -180, 180
		yawStabLimit:r = 180
		pitchStabLimit:r = 180
		rollStabLimit:r = 180
		rollStabLimit:r = 180
	}
	track {
		type:t = "no"
		azimuthLimits:p2 = -180, 180
		elevationLimits:p2 = -180, 180
		yawStabLimit:r = 180
		pitchStabLimit:r = 180
		rollStabLimit:r = 180
		pitchStabMandatory:b = true
	}
}
signals {
	track {
		anglesFinder:b = true
		rangeFinder:b = true
		groundClutter:b = false
		aircraftAsTarget:b = true
		groundVehiclesAsTarget:b = true
		angularAccuracy:r = 0
		distanceAccuracy:r = 0
		detectWeapon:b = false
		distance {
			presents:b = true
			minValue:r = 100
			width:r = 200
			maxValue:r = 15000
		}
	}
}
