			azimuth {
				angleHalfSens:r = 10
				sideLobesSensitivity:r = -35
			}
			elevation {
				angleHalfSens:r = 45
				sideLobesSensitivity:r = -35
			}
		}
	}
	track {
		power:r = 200
		band:i = 4
		rcs:r = 3
		range:r = 10000
		rangeMax:r = 10000
