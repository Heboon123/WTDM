		rowMajor:b = false
		indicate:b = true
	}
	losLock {
		type:t = "pyramide"
		azimuthLimits:p2 = -180, 180
		elevationLimits:p2 = -10, 90
		yawStabLimit:r = 180
		pitchStabLimit:r = 90
		rollStabLimit:r = 10
		period:r = 0.25
		width:r = 5
		barHeight:r = 1.5
		barsCount:i = 1
		rowMajor:b = false
		indicate:b = true
	}
	track {
		type:t = "no"
		azimuthLimits:p2 = -180, 180
		elevationLimits:p2 = -9, 90
		yawStabLimit:r = 180
		pitchStabLimit:r = 90
		rollStabLimit:r = 10
