		pitchStabLimit:r = 10
		rollStabLimit:r = 10
		period:r = 1.2
		barHeight:r = 15
		barsCount:i = 1
		rowMajor:b = true
	}
	lock {
		type:t = "pyramide"
		azimuthLimits:p2 = -180, 180
		elevationLimits:p2 = -10, 90
